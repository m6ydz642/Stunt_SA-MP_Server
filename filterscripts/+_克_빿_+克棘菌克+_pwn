forward Updating();
public Updating()
{
	update ++;
	if(update == 0)
	{
		ServerMapnameChange("¼·Æø¹ü °ÅÁö");
	}
	
 	else if(update == 1)
	{
	   	ServerMapnameChange("¼·Æø¹ü ¾ö¸¶ Ã¢³à ");
 	}
	else if(update == 2)
	{
	   	ServerMapnameChange("¼·Æø¹üÀº Å©·¡Ä¿µµ ¾Æ´Ô");
	    update = -1;
	}
	return 1;
}
