public OnPlayerCommandText(playerid, cmdtext[])
{
if (strcmp(cmdtext, "/열어", true) == 0) if(IsPlayerAdmin(playerid))
	{
        MoveObject(aaa,192.945297, -232.396484, 3.281359, 3);
        SendClientMessageToAll(COLOR_YELLOW, " + 어드민존 철장이 열렸습니다");
		return 1;
	}

if (strcmp(cmdtext, "/닫어", true) == 0) if(IsPlayerAdmin(playerid))
	{
        MoveObject(aaa,192.945297, -227.671417, 3.281359, 3);
        SendClientMessageToAll(COLOR_YELLOW, " + 어드민존 철장이 닫혔습니다");
		return 1;
	}
	
	if (strcmp(cmdtext, "/az", true) == 0) if(IsPlayerAdmin(playerid))
	{
	SendClientMessage(playerid, COLOR_YELLOW, " + 이런시팔 미안합니다 셋타이머 설정하는방법을 몰라서 ");
	SendClientMessage(playerid, COLOR_YELLOW, " + 어드민존 명령어는 /열어 /닫어 입니다 ㅋㅋㅋㅋㅋㅋ ");
		return 1;
	}
	
	if(!strcmp(cmdtext,"/azg",true) || !strcmp(cmdtext, "/어드민존",true)) // 명령어 적는곳
{
    if(IsPlayerInAnyVehicle(playerid) == 1)
    {
        SetVehiclePos(GetPlayerVehicleID(playerid), 189.079590, -228.462097, 0.637375); // 여기에다가 좌표적고
    }
    else
    {
        SetPlayerPos(playerid, 189.079590, -228.462097, 0.637375); // 여기도 좌표
    }

    return 1;
}
return 0;
}
