if(strcmp(cmd, "/musiccom", true) == 0) {
		SendClientMessage(playerid, COLOR_ORANGE," + _____________________성재 - Mode_____________________________");
	 	SendClientMessage(playerid, COLOR_YELLOW," + 비행학교: /flight 자동차학교: /driving 백그라운드: /back");
 		SendClientMessage(playerid, COLOR_YELLOW," + 비디오게임1: /Video1 비디오게임2: /Video2 비디오게임3: /Video3");
 		SendClientMessage(playerid, COLOR_YELLOW," + 자전거,보트학교 음악 : /bb");
