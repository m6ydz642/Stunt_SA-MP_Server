		{
		 SendClientMessage(playerid, COLOR_BLUE, " + 없다 ㅋㅋ ");
		 
			return 1;
			}
		if(strcmp(cmd, "/phone", true) == 0) {
			if(IsPlayerConnected(playerid)) {
				tmp = stringslice(cmdtext,1);
				if(!strlen(tmp)) {
					SendClientMessage(playerid, COLOR_GRAD2, "USAGE: /phone [유저번호]");
					return 1;
				}
				new icheck = strval(stringslice(cmdtext,2));
				giveplayerid = strval(tmp);
				GetPlayerName(playerid, sendername, sizeof(sendername));
				if(mobile_state[playerid] == 0) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 알수없는 오류입니다.");
					return 1;
				} else if(mobile_state[playerid] == 1) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 가까운 대리점에서 폰을 구입하십시오");
					return 1;
                } else if(mobile_state[playerid] == 2) {
                	SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 개통이 필요합니다.");
					return 1;
				} else if(mobile_state[playerid] == 4) {
                	SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 상대방이 통화중입니다.");
					return 1;
				} else if(mobile_state[playerid] == 5) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 전화를 걸려고 하는 고객님은 폰이 꺼져있습니다 [/폰전원] 을 해주세요");
					return 1;
				}
				if(mobile_rf[playerid] <= 1) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 현재 고객님은 통화권 이탈지역에 있습니다");
					return 1;
				}
				if(!IsPlayerConnected(giveplayerid)) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 지금 거신 번호는 없는 번호입니다. 다시 확인하고 걸어주십시오");
					return 1;
				}
				if(playerid == giveplayerid) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] LG TeleCom 음성 사서함 입니다 고객님의 음성메시지는 0건 입니다");
					return 1;
				}
				if(mobile_state[giveplayerid] == 0) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 알수없는 오류입니다.");
					return 1;
				} else if(mobile_state[giveplayerid] == 1) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 해당 사용자는 폰이 없습니다.");
					return 1;
				} else if(mobile_state[giveplayerid] == 2) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 고객님의 사정으로 착신이 불가능합니다.");
					return 1;
				} else if(mobile_state[giveplayerid] == 4) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 통화중입니다.");
					return 1;
				} else if(mobile_state[giveplayerid] == 5) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 전화기가 꺼져있어 소리셈으로 연결됩니다.");
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 연결된 후에는 통화료가 부과됩니다.");
					return 1;
				}
				if(mobile_rf[giveplayerid] <= 1) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 고객님이 서비스가 불가능한 지역에 있거나 발신할수 없는 번호입니다 ");
					return 1;
				}
				new call_id;
				call_id = create_call();
				if(call_id == -1) {
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 기지국 이랑 연결하지 못했 습니다");
					return 1;
				}
                 mobile_call[playerid] = call_id;
                mobile_call[giveplayerid] = call_id;
				mobile_state[playerid] = 4;
				mobile_state[giveplayerid] = 4;
				call_data[call_id][use] = true;
				call_data[call_id][cid] = playerid;
				call_data[call_id][cstate] = 1;
				if(icheck == 1) {
					call_data[call_id][check] = true;
					if(!ismadmin(giveplayerid)) {
						format(string, sizeof(string), " + 발신자표시제한님에게 연락이 왔습니다. 받으실려면 /pickup 을 입력하세요.");
					} else {
						format(string, sizeof(string), " + %s 발신자표시제한님에게 연락이 왔습니다. 받으실려면 /pickup 을 입력하세요.",sendername);
					}
				} else {
					call_data[call_id][check] = false;
					format(string, sizeof(string), " + %s님에게 연락이 왔습니다. 받으실려면 /pickup 을 입력하세요.", sendername);
				}
				SendClientMessage(giveplayerid, COLOR_YELLOW, string);
				GetPlayerName(giveplayerid, sendername, sizeof(sendername));
				SendClientMessage(playerid, COLOR_WHITE, " + 연락을 끊으시려면 /hangup 을 입력하세요.");
				SetPlayerSpecialAction(playerid,SPECIAL_ACTION_USECELLPHONE);
				return 1;
			}
			return 1;
		}
/*	if(strcmp(cmd, "/phone", true) == 0) {
