IsPlayerInArea(playerID, Float:x1, Float:y1, Float:x2, Float:y2)
{

	if(IsPlayerConnected(playerID))
	{
		new Float:X, Float:Y, Float:Z;

		GetPlayerPos(playerID, X, Y, Z);
		if(X >= x1 && X <= x2 && Y >= y1 && Y <= y2)
		{
			return 1;
		}
	}
	return 0;
}

public MoneyGrubScoreUpdate()
{
	new Float:rfcheck;
	new Float:rfcheck2;
	new name[MAX_PLAYER_NAME];
	//new string[256];
	new string[255];
	if(admin_countdown < 0) {
	    admin_checkcount = false;
	    admin_countdown = 0;
	}
	if(admin_checkcount == true) {
	    if(admin_countdown == 0) {
			GameTextForAll("~r~GO!!",1500,3);
		} else {
		    new gstr[256];
		    if(admin_countdown <= 10) {
				format(gstr,sizeof(gstr),"~r~%d",admin_countdown);
			} else if(admin_countdown <= 50) {
				format(gstr,sizeof(gstr),"~g~%d",admin_countdown);
			} else {
				format(gstr,sizeof(gstr),"~y~%d",admin_countdown);
			}
			GameTextForAll(gstr,1300,3);
		}
	}
	for(new i=0; i<MAX_PLAYERS; i++)
	{

	    if(IsPlayerConnected(i)) {
	        if(testhack[i] >= 8) Kick(i);
			if(IsPlayerInAnyVehicle(i)) {
	        SetPlayerVirtualWorld(i,GetVehicleVirtualWorld(GetPlayerVehicleID(i)));
	        }
		}
		if (IsPlayerConnected(i)) {
		    if(lock_bb[i] == 1) {
				SetPlayerInterior(i,0);
				ClearAnimations(i);
				TogglePlayerControllable(i,1);
				lock_bb[i] = 0;
			} 
			if(lock_bb[i] >= 2) {
			    lock_bb[i] --;
			}

			if (readypayandspray[i] == 1) {
				CarSprayCountdown[i] --;
				if (CarSprayCountdown[i] == 0) {
					SetVehiclePos(CarSprayVehicleid[i], CarSprayX[i], CarSprayY[i], CarSprayZ[i]);
					TogglePlayerControllable(i,1);
					askedpayandspray[i]=0;
					readypayandspray[i]=0;
					CarSprayVehicleid[i]=0;
					CarSprayX[i] = 0.0000;
					CarSprayY[i] = 0.0000;
					CarSprayZ[i] = 0.0000;
				}
			}
			if (askedpayandspray[i] == 1) {
				CarSprayCountdown[i] --;
				if (CarSprayCountdown[i] == 0) {
					if(!IsPlayerInAnyVehicle(i)){
						SendClientMessage(i, COLOR_SYSTEM_GM , " + 당신은 차에 타야합니다.");
						GiveMoney(i, 25000);
						askedpayandspray[i]=0;
						readypayandspray[i]=0;
						CarSprayVehicleid[i]=0;
						CarSprayX[i] = 0.0000;
						CarSprayY[i] = 0.0000;
						CarSprayZ[i] = 0.0000;
					} else {
						CarSprayX[i]=X250[i];
						CarSprayY[i]=Y250[i];
						CarSprayZ[i]=Z250[i];
						CarSprayVehicleid[i] = GetPlayerVehicleID(i);
						SetVehicleZAngle(CarSprayVehicleid[i], 180.0000);
						SetVehiclePos(CarSprayVehicleid[i], -1420.6775, 2585.8892, 55.6749);
						askedpayandspray[i]=0;
						readypayandspray[i]=1;
						CarSprayCountdown[i]=6;
						TogglePlayerControllable(i,0);
					}
				}
			}
			if(stunt1[i] > 0)stunt1[i]--;
			if(stunt2[i] > 0)stunt2[i]--;
			if(IsPlayerInVehicle(i, 277))
			{
				expup[i] ++;
		  		if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 439))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 440))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			
			if(IsPlayerInVehicle(i, 492))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 493))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 494))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 495))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			if(IsPlayerInVehicle(i, 496))
			{
				expup[i] ++;
				if(expup[i] == 15) {
					exp[i] ++;
					expup[i] = 0;
				}
				GiveMoney(i,15);
			}
			
			
			new Float:X, Float:Y, Float:Z;
            GetPlayerPos(i, X, Y, Z);
            if(admin_checkcount == true) {
                if(admin_countdown == 0) {
					PlayerPlaySound(i, 1057, X, Y, Z);
				} else {
					PlayerPlaySound(i, 1056, X, Y, Z);
				}
			}
            if(isPlayerInArea(i,skyinf) && Z <= 133 && Z >= 145 && god[i] == 0) {
                OnPlayerSpawn(i);
			}
			if(mobile_state[i] == 3 || mobile_state[i] == 4) {
			    if(!ishadmin(i)) {
			        new tmprfn;
			        tmprfn = mobile_rfn[i];
			        rfcheck = 9999;
					rfcheck2 = GetPlayerDistanceToPoint(i,-2502.7605,-698.3276,140.0);
					if(rfcheck > rfcheck2 && mobile_srf[1] == 1) {
						rfcheck = rfcheck2;
						mobile_rfn[i] = 1;
					}
					rfcheck2 = GetPlayerDistanceToPoint(i,-341.5190,1607.6796,75.5348);
					if(rfcheck > rfcheck2 && mobile_srf[2] == 1) {
						rfcheck = rfcheck2;
						mobile_rfn[i] = 2;
					}
					rfcheck2 = GetPlayerDistanceToPoint(i,1456.9651,-786.0055,92.8562);
					if(rfcheck > rfcheck2 && mobile_srf[3] == 1) {
						rfcheck = rfcheck2;
						mobile_rfn[i] = 3;
					}
					rfcheck2 = GetPlayerDistanceToPoint(i,2323.5735,1283.5907,97.1828);
					if(rfcheck > rfcheck2 && mobile_srf[4] == 1) {
						rfcheck = rfcheck2;
						mobile_rfn[i] = 4;
					}
					if(rfcheck < 600) {
						mobile_rf[i] = 5;
						mobile_battery[i] = mobile_battery[i] - 1;
					} else if(rfcheck < 900) {
						mobile_rf[i] = 4;
						mobile_battery[i] = mobile_battery[i] - 2;
					} else if(rfcheck < 1300) {
						mobile_rf[i] = 3;
						mobile_battery[i] = mobile_battery[i] - 3;
					} else if(rfcheck < 1600) {
						mobile_rf[i] = 2;
						mobile_battery[i] = mobile_battery[i] - 4;
					} else if(rfcheck < 2000) {
						mobile_rf[i] = 1;
						mobile_battery[i] = mobile_battery[i] - 5;
					} else {
					    rfcheck = -1;
						mobile_rf[i] = 0;
						mobile_rfn[i] = -1;
						mobile_battery[i] = mobile_battery[i] - 10;
					}
					if(mobile_state[i] == 3) {
						if( mobile_battery[i] > 0) {
							if(mobile_battery[i] <= 50 && random(4) == 2) {
								SendClientMessage(i, COLOR_GRAD2, " + [PHONE] 배터리가 부족합니다. [/폰충전] 입력 하세요");
							}
						} else {
                            mobile_state[i] = 5;
                            mobile_battery[i] = 0;
                            SendClientMessage(i, COLOR_GRAD2, " + [PHONE] 배터리가 부족해서 전원이 꺼집니다 [/폰충전] 하세요");
                            //SendClientMessage(i, COLOR_GRAD2, " + [PHONE] 배터리가 부족해서 전원이 꺼집니다 [/폰충전] 하세요");
                            SendClientMessage(i, COLOR_GRAD2, " + [PHONE] [/폰전원] 을 한번더하시면 다시 전원이 켜지고 또 하면 꺼집니다");
						}
					}
 					mobile_rfw[i] = rfcheck;
					if(mobile_rfn[i] != tmprfn)
					{
					    new pstr[256];
					    new vstr[256];
					    
   						if(mobile_rfn[i] == -1)
						   {
						    format(pstr,sizeof(pstr)," + [PHONE] 통화권 이탈로 기지국과 연결이 끊겼습니다 [전파수신상태:0] (신호세기:매우약함)");
         } else {
          format(pstr,sizeof(pstr)," + [PHONE] %d번 기지국과 연결되었습니다 [전파수신상태:5] (신호세기:강함) ",mobile_rfn[i]);
          format(vstr,sizeof(vstr)," + [PHONE] 전파수신상태[%d] (신호세기:강함)",mobile_rf);
    				 }
						SendClientMessage(i,COLOR_ECHO,pstr);
					}
				}
			} else {
				mobile_rf[i] = 0;
				mobile_rfn[i] = -1;
			}
/*
//전파수신상태 시작
new vstr[256];
if(mobile_rf == -1)
{
format(vstr,sizeof(vstr)," + [PHONE] 전파수신상태[%d] (신호세기:강함)",mobile_rf);
SendClientMessage(i, COLOR_GRAD2, vstr);
*/


	GetPlayerName(i, name, sizeof(name));
	new cash;
	cash = GetPlayerMoney(i);
	if(cash >= 900000000) {
   			    SetMoney(i,0);
				format(string,sizeof(string)," + %s(%d) 님은 돈핵을 쓰셧습니다.",name,i);
				printf(" + [긴급알림] %s(%d) 님은 돈핵을 쓰셧습니다.",name,i);
		        SendClientMessageToAll(COLOR_RED,string);
		        testhack[i]++;
		        testhack[i]++;
			}
			if(moneyhack[i] > cash) {
			    cashtDel[i] = 0;
			    cashDel[i]++;
			    if(cashDel[i] >= 20) {
			        cashDel[i] = 0;
			        SetPlayerMoney(i,moneyhack[i]);
				}
			} else if(moneyhack[i] == cash) {
			    cashDel[i] = 0;
			    cashtDel[i] = 0;
			} else if(moneyhack[i] < cash && !SKIP_WEAPONCHECK[i]) {
			    cashDel[i] = 0;
			    cashtDel[i]++;
			    if( (cash-moneyhack[i]) == 999999999 || (cash-moneyhack[i]) == 500000000) {
			        cashtDel[i] = 0;
				    format(string,sizeof(string)," + %s(%d) 라는 사람은 돈핵을 써서 $%d를 만들었어.",name,i,cash-moneyhack[i]);
                     printf(" + %s(%d) 라는 사람은 돈핵을 써서 $%d를 만들었어.",name,i);
					SendClientMessageToAll(COLOR_RED,string);
					Kick(i);
				} else {
					if(cashtDel[i] >= 10) {
						format(string,sizeof(string)," + %s(%d) 라는 사람은 돈핵을 써서 $%d를 만들었어.",name,i,cash-moneyhack[i]);
                        printf(" + %s(%d) 라는 사람은 돈핵을 써서 $%d를 만들었어.",name,i);
						SendClientMessageToAll(COLOR_RED,string);
						SetPlayerMoney(i,moneyhack[i]);
						testhack[i]++;
						testhack[i]++;
					}
				}

			}
			SetPlayerScore(i, moneyhack[i]+bank[i]);
/*
			if(CashScore - oldcash[i] >= 10000000 && [i] <= 0) {
				if(!ismadmin(i)) {
					new string[255];
			        new pname[255];
			        GetPlayerName(i,pname,sizeof(pname));
					format(string,sizeof(string)," + %s(%d) 님은 돈핵을 쓰셧습니다.",pname,i);
			        SendClientMessageToAll(COLOR_RED,string);
			    //    SetPlayerMoney(i, oldcash[i]);
			        CashScore = oldcash[i];
				 }
			}*/
/*			if(loginflag[i] == 0) {
			    if(loginse[i] == 180) {
					format(string,sizeof(string)," + %s(%d) 님은 3분동안 로그인 하지 않으셨으므로 강퇴됩니다. 획!",name,i);
			        SendClientMessageToAll(COLOR_RED,string);
			        Kick(i);
				}
				loginse[i] = loginse[i] + 1;
			    if(GetPlayerInterior(i) == 12) {

			    } else {
			        if(classing[i] == 0){
		    		SetPlayerPos(i,2324.4199,-1147.1016,1050.7101);
					SetPlayerInterior(i,12);
					}
			    }
			}*/
   			if(JailTime[i] < 0) {
				if(isJail[i] == 1) {
				    isJail[i]=0;
					SetPlayerInterior(i,0);
					SetPlayerPos(i, -1619.9363,691.8935,7.1875);
					SendClientMessage(i,COLOR_RED," + 석방 되었습니다.");
					loginsystem(i,0);
				}
			} else {
				JailTime[i] = JailTime[i] - 1;
				if(JailTime[i] == 50) {
					SendClientMessage(i,COLOR_RED," + 석방까지 50초 남았습니다.");
				} else if(JailTime[i] == 40) {
					SendClientMessage(i,COLOR_RED," + 석방까지 40초 남았습니다.");
				} else if(JailTime[i] == 30) {
					SendClientMessage(i,COLOR_RED," + 석방까지 30초 남았습니다.");
				} else if(JailTime[i] == 20) {
					SendClientMessage(i,COLOR_RED," + 석방까지 20초 남았습니다.");
				} else if(JailTime[i] == 10) {
					SendClientMessage(i,COLOR_RED," + 석방까지 10초 남았습니다.");
				} else if(JailTime[i] == 5) {
					SendClientMessage(i,COLOR_RED," + 석방까지 5초 남았습니다.");
				} else if(JailTime[i] == 3) {
					SendClientMessage(i,COLOR_RED," + 석방까지 3초 남았습니다.");
				}
			}
			if(isJail[i] == 1) {
			    if(GetPlayerInterior(i) == 6 && GetPlayerVirtualWorld(i) == 0) {
			    } else {
			        if(loginflag[i] == 1 && !SKIP_WEAPONCHECK[i]) {
					SetPlayerInterior(i,6);
					SetPlayerVirtualWorld(i,0);
					SetPlayerPos(i,263.51,77.76,1001.04);
			        testhack[i]++;
			        testhack[i]++;
					}
				}
			}
	        testhack[i]--;
			// 끝
		}
	}
	if(admin_countdown == 0) {
	    admin_countdown = 0;
	    admin_checkcount = false;
	} else {
	    admin_countdown--;
	}
}

