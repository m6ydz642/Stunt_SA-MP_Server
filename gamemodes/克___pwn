		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/countdown [sec]");
		    return 1;
	    }
	    adoors = strval(stringslice(cmdtext,1));
	    if(adoors <= 0) {
		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/countdown [sec]");
		    return 1;
		}
		if(strval(stringslice(cmdtext,2)) == 1) {
