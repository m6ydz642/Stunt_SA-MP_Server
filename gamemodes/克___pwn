/*
public OnGUIRCONUserConnect( userid )
{
	new szIP[128];
	GUIRCON_GetUserIP( userid, szIP, 128 );
	printf( "New GUIRCON user with ID %i connected (%s)", userid, szIP );
	GUIRCON_SendPlayerlist( userid );
}

public OnGUIRCONUserDisconnect( userid )
{
	printf( "GUIRCON user %i has disconnected", userid );
}

public OnGUIRCONCommand( userid, cmd[] )
{
	// userid is a unique ID number based on what socket the connection uses
	// This will remain the same for as long as that user is connected to
	// GUIRCON. Once they disconnect the ID will become free and another
	// connection use it.
	// ID's are assigned on a first available basis. The lowest number which
	// is free will be assigned to the next incoming connection

	// cmd[] is the data that is sent by the user
	// See OnRconCommand (standard function) for the basis of this

	// Some simple commands have been done already to give you an idea
	// of how to use this callback
	new idx;
	new sendstr[128];
	new tmp[256];
	new tmp2[256];
	tmp = strtok( cmd, idx );

	// General RCON
	if( strcmp( tmp, "KICK", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
			Kick( toplayer );
	}
	else if( strcmp( tmp, "BAN", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
			Ban( toplayer );
	}
	// Player Information
	else if( strcmp( tmp, "GETIP", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
		{
			new strip[64];
			GetPlayerIp( toplayer, strip, 64 );
			format( sendstr, 128, "PLAYERIP %i %s", toplayer, strip );
			GUIRCON_SendToUser( userid, strip );
		}
	}
	else if( strcmp( tmp, "GETHEALTH", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
		{
			new Float:fHealth;
			GetPlayerHealth( toplayer, fHealth );
			format( sendstr, 128, "PLAYERHEALTH %i %f", toplayer, fHealth );
			GUIRCON_SendToUser( userid, sendstr );
		}
	}
	else if( strcmp( tmp, "GETARMOUR", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
		{
			new Float:fArmour;
			GetPlayerArmour( toplayer, fArmour );
			format( sendstr, 128, "PLAYERARMOUR %i %f", toplayer, fArmour );
			GUIRCON_SendToUser( userid, sendstr );
		}
	}
	else if( strcmp( tmp, "GETPOS", true ) == 0 )
	{
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
		{
			new Float:fX; new Float:fY; new Float:fZ;
			GetPlayerPos( toplayer, fX, fY, fZ );
			format( sendstr, 128, "PLAYERPOS %i %f %f %f", toplayer, fX, fY, fZ );
			GUIRCON_SendToUser( userid, sendstr );
		}
	}
	// Messages
	else if( strcmp( tmp, "PRIVMSG", true ) == 0 )
	{
		// PM a player
		tmp2 = strtok( cmd, idx );
		if( !strlen(tmp2) ) return 0;
		new toplayer = strval( tmp );
		if( IsPlayerConnected( toplayer ) )
		{
			new text[512];
			new sendtext[512];
			strmid( sendtext, cmd, idx + 1, strlen(cmd) );
			format( sendtext, sizeof(sendtext), " + @* Admin: %s", text );
			SendClientMessage( toplayer, 0xFFFFFFAA, sendtext );
		}
		return 1;
	}
	else if( strcmp( tmp, "MSG", true ) == 0 )
	{
		new text[512];
		new sendtext[512];
		strmid( text, cmd, 4, strlen(cmd) );
		format( sendtext, sizeof(sendtext), " + @* Admin: %s", text );
		printf( "sending %s to everyone", sendtext );
		SendClientMessageToAll( 0xFFFFFFAA, sendtext );
		return 1;
	}

	return 0;
}





