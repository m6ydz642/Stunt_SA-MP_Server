	if(strcmp(cmd, "/bank", true) == 0) {

	    tmp = strtok(cmdtext, idx);
	    if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /bank [금액]");
			return 1;
	    }
	    moneys = strval(tmp);

	    if(moneys < 1) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 명령어를 똑바로 쳐주세요.");
			return 1;
		}

		if(GetMoney(playerid) < moneys) {
			moneys = GetMoney(playerid);
		}
		if(moneys + bank[playerid] >= 2000000000) {
		format(string, sizeof(string), " + 통장에 보유 가능한 돈은 $20억이 최대입니다.");
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
  		if(playerCheckpoint[playerid] == CP_BANK) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
  		if(getCheckpointType(playerid) == CP_BANK3) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(getCheckpointType(playerid) == CP_BANK4) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(getCheckpointType(playerid) == CP_BANK5) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(getCheckpointType(playerid) == CP_BANK6) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}

		if(IsPlayerInArea(playerid, -1615.9764,852.6021,-1569.6628,880.0115)) {
		GiveMoney(playerid, 0-moneys);
		bank[playerid]+=moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + %s (id: %d) 님은 $%d 를 입금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 입금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(bank[playerid] <= -5000000){
		bank[playerid] = -500000;
		}
	 	SendClientMessage(playerid, COLOR_YELLOW, " + 이 명령어는 은행에서만 가능합니다.");
		return 1;
	}
	if(strcmp(cmd, "/createcard", true) == 0) {
	    tmp = strtok(cmdtext, idx);
	    if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /createcard [비밀번호]");
			return 1;
	    }
	    if(BankPass[playerid] >= 1) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 이미 당신은 비밀번호가 있습니다.");
			return 1;
	    }
	    if(strval(tmp) == 0) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 비밀번호를 0 또는 문자로 지정할 수 없습니다.");
			return 1;
	    }
		BankPass[playerid] = strval(tmp);
		format(string, sizeof(string), " + 통장 비밀번호가 %d 로 지정되었습니다.",BankPass[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		udb_UserSetInt(loginid[playerid],"BankPass",BankPass[playerid]);
		return 1;
	}
	
	if(strcmp(cmd, "/changecard", true) == 0) {
		new oldpass;
		new newpass;
	    tmp = strtok(cmdtext, idx);
		oldpass = strval(tmp);
	    if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /changecard [현재 비밀번호] [바꿀 비밀번호]");
			return 1;
	    }
	    tmp = strtok(cmdtext, idx);
		newpass = strval(tmp);
		if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /changecard [현재 비밀번호] [바꿀 비밀번호]");
			return 1;
	    }
	    if(BankPass[playerid] == 0) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 비밀번호가 생성되어 있어야 합니다.");
			return 1;
	    }
		if(BankPass[playerid] != oldpass) {
			SendClientMessage(playerid, COLOR_WHITE, " + 비밀번호가 맞지 않습니다.");
			return 1;
	    }
	    if(newpass == 0) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 비밀번호를 0 으로 지정할 수 없습니다.");
			return 1;
	    }
		BankPass[playerid] = newpass;
		format(string, sizeof(string), " + 통장 비밀번호가 %d 로 지정되었습니다.",BankPass[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		udb_UserSetInt(loginid[playerid],"BankPass",BankPass[playerid]);
		return 1;
	}
	if(strcmp(cmd, "/ofcard", true) == 0) {
	    tmp = strtok(cmdtext, idx);
	    if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /ofcard [비밀번호]");
			return 1;
	    }
	    if(BankPass[playerid] == 0) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 당신은 비밀번호를 생성하지 않았습니다.");
			return 1;
	    }
	    if(strval(tmp) != BankPass[playerid]) {
	    	SendClientMessage(playerid, COLOR_WHITE, " + 비밀번호가 틀립니다.");
			return 1;
	    }
		if(BankOpen[playerid] == 0) {
  		SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_ORANGE," + 통장이 열렸습니다.");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        
		BankOpen[playerid] = 1;
		return 1;
		}
		if(BankOpen[playerid] == 1) {
		BankOpen[playerid] = 0;
  		SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_ORANGE," + 통장이 닫혔습니다.");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
        SendClientMessage(playerid,COLOR_GREY," ");
		return 1;
		}
		return 1;
	}
	if(strcmp(cmd, "/get", true) == 0) {


	    tmp = strtok(cmdtext, idx);
	    if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /get [amount]");
			return 1;
	    }
	    moneys = strval(tmp);

	    if(moneys < 1) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 명령어를 똑바로 쳐주세요.");
			return 1;
		}
		if(BankOpen[playerid] == 0 && BankPass[playerid] == 0) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 통장 비밀번호를 생성 해주세요.(/createcard [비밀번호])");
			return 1;
		}
		if(BankOpen[playerid] == 0) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 통장 비밀번호를 입력해 열어 주십시오. (여는 방법 : /ofcard [비밀번호]");
			return 1;
		}

	    if(moneys > bank[playerid])
	        moneys = bank[playerid];
     	if(playerCheckpoint[playerid] == CP_BANK) {
		GiveMoney(playerid, moneys);
		bank[playerid] -= moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), "--- %s (id: %d) 님은 $%d 를 출금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 출금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
 		if(playerCheckpoint[playerid] == CP_BANK3) {
		GiveMoney(playerid, moneys);
		bank[playerid] -= moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), "--- %s (id: %d) 님은 $%d 를 출금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 출금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(playerCheckpoint[playerid] == CP_BANK4) {
		GiveMoney(playerid, moneys);
		bank[playerid] -= moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), "--- %s (id: %d) 님은 $%d 를 출금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 출금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
		if(playerCheckpoint[playerid] == CP_BANK5) {
		GiveMoney(playerid, moneys);
		bank[playerid] -= moneys;
		GetPlayerName(playerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), "--- %s (id: %d) 님은 $%d 를 출금했습니다. 통장잔액은 $%d 입니다.", giveplayer,playerid,moneys,bank[playerid]);
		printf(string);
		format(string, sizeof(string), " + $%d 를 출금했습니다. 당신의 통장잔액은 $%d 입니다.", moneys, bank[playerid]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		return 1;
		}
