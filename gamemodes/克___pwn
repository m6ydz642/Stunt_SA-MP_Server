	TextDrawBoxColor(vehiclebar[5], 0x00aa00ff);
	TextDrawSetShadow(vehiclebar[5],0);
	TextDrawTextSize(vehiclebar[5], 571, 0);

	vehiclebar[6] = TextDrawCreate(551.0, 59.0, " ");
	TextDrawUseBox(vehiclebar[6], true);
