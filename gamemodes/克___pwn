	AddMenuItem(Exhausts,0,"X-Flow Exhaus  $690");
	AddMenuItem(Exhausts,0,"<back>");
	Frontbumper = CreateMenu("Upgrades",1,20,120,150,40);
	SetMenuColumnHeader(Frontbumper,0,"Upgrades     Cost");
	AddMenuItem(Frontbumper,0,"Alient Front Bumper  $990");
	AddMenuItem(Frontbumper,0,"X-Flow Front Bumper  $900");
