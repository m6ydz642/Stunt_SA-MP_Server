			GetPlayerName(playerid, sendername, sizeof(sendername));
			isJail[giveplayerid] = 1;
      		JailTime[giveplayerid] = strval(tmp);
			SetPlayerInterior(giveplayerid,6);
			SetPlayerPos(giveplayerid,263.51,77.76,1001.04);
			format(string, sizeof(string), "--- %s (id: %d) 감옥에 가둠 %s (id: %d).", sendername,playerid,giveplayer,giveplayerid);
			printf(string);
			format(string, sizeof(string), " + %s (id: %d)를 감옥에 가둠.", giveplayer,giveplayerid);
			SendClientMessage(playerid, COLOR_YELLOW, string);
			format(string, sizeof(string), " + 긴급뉴스를 알려드리겠습니다!");
			SendClientMessageToAll(COLOR_HEADLINE, string);
			format(string, sizeof(string), " + %d번 %s 님은 %d초 동안 감옥에 가두어졌답니다.", giveplayerid,giveplayer,JailTime[giveplayerid]);
			SendClientMessageToAll(COLOR_WHITE, string);
		}
		else {
			format(string, sizeof(string), " + %d 번의 사용자는 없습니다.", giveplayerid);
			SendClientMessage(playerid, COLOR_YELLOW, string);
		}
		}
		return 1;
	}
// START DANCING
 	if(strcmp(cmd, "/dance", true) == 0) {
		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) {

		    // Get the dance style param
      		tmp = strtok(cmdtext, idx);
			if(!strlen(tmp)) {
				SendClientMessage(playerid,0xFF0000FF," + Usage: /dance [style 1-3]");
				return 1;
			}

			dancestyle = strval(tmp);
			if(dancestyle < 1 || dancestyle > 3) {
			    SendClientMessage(playerid,0xFF0000FF," + Usage: /dance [style 1-3]");
			    return 1;
			}

			if(dancestyle == 1) {
			    SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE1);
			} else if(dancestyle == 2) {
			    SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE2);
			} else if(dancestyle == 3) {
			    SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE3);
			}
 	  		return 1;
	 }
	}
	// HANDSUP
 	if(strcmp(cmd, "/handup", true) == 0) {
		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) {
		  SetPlayerSpecialAction(playerid,SPECIAL_ACTION_HANDSUP);
          return 1;
     }
	}
    // Drunk
    if(strcmp(cmd, "/drunk", true) == 0) {
        if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
     {
