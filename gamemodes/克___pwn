 	if(strcmp(cmd,"/releasefish",true)==0)
	{
	    if(IsPlayerConnected(playerid))
	    {
			tmp = strtok(cmdtext, idx);
			if(!strlen(tmp))
			{
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /releasefish [fish]");
				return 1;
			}
			new fishid = strval(tmp);
			if(fishid < 1 || fishid > 5) { SendClientMessage(playerid, COLOR_GREY, " + 1~5까지의 바구니만 가능합니다."); return 1; }
			else if(fishid == 1 && Fishes[playerid][pWeight1] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 1번 바구니에 물고기가 없습니다."); return 1; }
			else if(fishid == 2 && Fishes[playerid][pWeight2] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 2번 바구니에 물고기가 없습니다."); return 1; }
			else if(fishid == 3 && Fishes[playerid][pWeight3] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 3번 바구니에 물고기가 없습니다."); return 1; }
			else if(fishid == 4 && Fishes[playerid][pWeight4] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 4번 바구니에 물고기가 없습니다."); return 1; }
			else if(fishid == 5 && Fishes[playerid][pWeight5] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 5번 바구니에 물고기가 없습니다."); return 1; }
			ClearFishID(playerid, fishid);
			Fishes[playerid][pLastFish] = 0;
   			Fishes[playerid][pFishID] = 0;
   				udb_UserSetInt(loginid[playerid],"fishWeight5",Fishes[playerid][pWeight5]);
    udb_UserSetInt(loginid[playerid],"fishWeight4",Fishes[playerid][pWeight4]);
    udb_UserSetInt(loginid[playerid],"fishWeight3",Fishes[playerid][pWeight3]);
