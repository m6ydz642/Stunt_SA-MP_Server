public OnPlayerText(playerid, text[])
{
	new headtext[255];
	new fortext[255];
	new forname[26];
	new foroz[200];

	GetPlayerName(playerid,forname,sizeof(forname));
	player_stop[playerid] = 0;
	if(loginflag[playerid] == 0){
 		SendClientMessage(playerid,GetPlayerColor(playerid)," + 로그인을 하셔야 말을 하실수있습니다.");
	  	SendClientMessage(playerid, COLOR_GREEN, " + 로그인을 하시려면 /logincom 을 참고해주세요");
	  	return 0;
  	}

   	FixChars(text);
//	format( szSend, 512, "MSG %i %s", playerid, text );
//	GUIRCON_SendToAll( szSend );
	if(mobile_state[playerid] == 4) {
		if( call_data[mobile_call[playerid]][cstate] == 2 ) {
		    if(call_data[mobile_call[playerid]][check] == true && playerid == call_data[mobile_call[playerid]][cid]) {
				format(fortext,sizeof(fortext)," +++ [통화]발신자표시제한 ▷ %s",text);
			} else {
				format(fortext,sizeof(fortext)," +++ [통화]%s(%d) ▷ %s",forname,playerid,text);
			}
			for(new i=0; i < MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i)) {
					if(mobile_call[playerid] == mobile_call[i]) {
						SendClientMessage(i, COLOR_YELLOW,fortext);
					}
				}
			}
			return 0;
		}
	}
	if(!ismadmin(playerid) || ishadmin(playerid))
	{
    	for(new s=0,total=sizeof(Yells);s<total;s++)
	    {
        	new pos;
			while((pos = strfind(text,Yells[s],true)) != -1) for(new i = pos, j = pos + strlen(Yells[s]); i < j; i++) text[i] = '*';
		}
	}
	if(isadmin(playerid) && !ishadmin(playerid))
	{
		if(text[0]=='*'){
			SendClientMessageToAll(GetPlayerColor(playerid), " ");
			format(fortext, sizeof(fortext), " + %s(운영자) ▷ %s",forname,text); //운영자로 말
			SendClientMessageToAll(GetPlayerColor(playerid), fortext);
			SendClientMessageToAll(GetPlayerColor(playerid), " ");
			return 0;
		}
	}
	if(MyJob[playerid] == JOB_LOST) format(jobname, sizeof(jobname), "백수");
	if(MyJob[playerid] == JOB_TAXI) format(jobname, sizeof(jobname), "택시기사");
    if(MyJob[playerid] == JOB_COOK) format(jobname, sizeof(jobname), "요리사");
    if(MyJob[playerid] == JOB_SPEEDCAR) format(jobname, sizeof(jobname), "단속반");
    if(MyJob[playerid] == JOB_BOXER) format(jobname, sizeof(jobname), "복서");
    if(isJail[playerid] == 1) format(jobname, sizeof(jobname), "죄수");
    
	if( isadmin(playerid) && !ishadmin(playerid)) {
		format(headtext,sizeof(headtext),"@ %s",jobname);
	} else if(udb_UserInt(loginid[playerid],"claner")) {
	    format(headtext,sizeof(headtext),"$ %s",jobname);
	} else {
	    format(headtext,sizeof(headtext),"+ %s",jobname);
	}
	if(playeroz[playerid] >= 1) {
	format(foroz,sizeof(fortext),"[%s 조직] ",ozNames[playeroz[playerid]]);
	} else
	{
	format(foroz,sizeof(fortext)," ");
	}
	if(nickchange[playerid] == 1)
	{
	
format(fortext,sizeof(fortext)," + [%d채널]%s%s %s(%d) ▷ %s",GetPlayerVirtualWorld(playerid),foroz,headtext,nnn[playerid],playerid,text);
printf("+ [%d채널]%s%s %s(%d) ▷ %s",GetPlayerVirtualWorld(playerid),foroz,headtext,nnn[playerid],playerid,text);
} else if(nickchange[playerid] == 0) //닉네임 변경하고 채팅
{
format(fortext,sizeof(fortext)," + [%d채널]%s%s %s(%d) ▷ %s",GetPlayerVirtualWorld(playerid),foroz,headtext,forname,playerid,text);
printf("+ [%d채널]%s%s %s(%d) ▷ %s",GetPlayerVirtualWorld(playerid),foroz,headtext,forname,playerid,text);
}
//일반채팅 말하는거
	chatkick[playerid] ++; //도배시 킥
	ProxDetector(500.0, playerid, fortext,GetPlayerColor(playerid),GetPlayerColor(playerid),GetPlayerColor(playerid),GetPlayerColor(playerid),GetPlayerColor(playerid));

//    SendClientMessageToAll(GetPlayerColor(playerid),fortext);
	return 0;
}

FixChars(string[]) for (new i=-1;i<strlen(string);i++) if (string[i] < 0) string[i] += 256;


//스페이스 공백 검사
stringslice(const string[],stringindex)
{
 new stringstart;
 new stringnum;
 new stringlength;
 new result[25];

 for(stringnum=0;stringnum<=stringindex && stringlength<=strlen(string);++stringnum)
 {
  if(stringnum>0)stringlength++;
  stringstart=stringlength;
  while(stringlength<=strlen(string) && string[stringlength]!=' ' && string[stringlength]!=EOS)
  {
   stringlength++;
  }
 }

 stringnum=0;
 stringlength=stringlength-stringstart;
 while(stringnum<=stringlength && stringnum<=sizeof(result))
 {
  result[stringnum]=string[stringnum+stringstart];
  stringnum++;
 }

 result[stringlength]=EOS;
 return result;
}
/*getnick(const string[])
{
	new stringnum;
//	new stringlength;
	new result[255];
	new adata;

 for(stringnum=0;stringnum<=strlen(string);++stringnum)
 {
	if(adata == 0 && string[stringnum]) {
	    adata = stringnum+1;
	}
 }

 for(stringnum=adata;stringnum<=strlen(string);++stringnum)
 {
	result[stringnum-adata] = string[stringnum];
 }
 result[strlen(string)-adata]=EOS;
 return result;
}
*/


