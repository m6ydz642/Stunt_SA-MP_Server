		strmid(Cooking[playerid][pCook1], string, 0, strlen(string), 255);
		strmid(Cooking[playerid][pCook2], string, 0, strlen(string), 255);
		strmid(Cooking[playerid][pCook3], string, 0, strlen(string), 255);
		strmid(Cooking[playerid][pCook4], string, 0, strlen(string), 255);
		strmid(Cooking[playerid][pCook5], string, 0, strlen(string), 255);
	}
	return 1;
}

public LoadBoxer()
{
	new arrCoords[3][64];
	new strFromFile2[256];
	new File: file = fopen("boxer.ini", io_read);
	if (file)
	{
		fread(file, strFromFile2);
		split(strFromFile2, arrCoords, ',');
		Titel[TitelWins] = strval(arrCoords[0]);
		strmid(Titel[TitelName], arrCoords[1], 0, strlen(arrCoords[1]), 255);
		Titel[TitelLoses] = strval(arrCoords[2]);
		fclose(file);
	}
	return 1;
}

public SaveBoxer()
{
	new coordsstring[256];
	format(coordsstring, sizeof(coordsstring), "%d,%s,%d", Titel[TitelWins],Titel[TitelName],Titel[TitelLoses]);
	new File: file2 = fopen("boxer.ini", io_write);
	fwrite(file2, coordsstring);
	fclose(file2);
	return 1;
}
public BoxTime() {
 for(new i = 0; i < MAX_PLAYERS; i++) {
        new string[256];
	    if(RoundStarted > 0)
			{
			    if(PlayerBoxing[i] > 0)
			    {
			        new trigger = 0;
			        new Lost = 0;
		        	new Float:angle;
		            new Float:health;
					GetPlayerHealth(i, health);
		            if(health < 12)
					{
					    if(i == Boxer1) { Lost = 1; trigger = 1; }
			            else if(i == Boxer2) { Lost = 2; trigger = 1; }
					}
			        if(health < 28) { GetPlayerFacingAngle(i, angle); SetPlayerFacingAngle(i, angle + 85); }
			        if(trigger)
			        {
			            new winner[MAX_PLAYER_NAME];
			            new loser[MAX_PLAYER_NAME];
			            new titel[MAX_PLAYER_NAME];
			            if(Lost == 1)
			            {
			                if(IsPlayerConnected(Boxer1) && IsPlayerConnected(Boxer2))
			                {
					        	SetPlayerPos(Boxer1, 765.8433,3.2924,1000.7186); SetPlayerPos(Boxer2, 765.8433,3.2924,1000.7186);
					        	SetPlayerInterior(Boxer1, 5); SetPlayerInterior(Boxer2, 5);
			                	GetPlayerName(Boxer1, loser, sizeof(loser));
			                	GetPlayerName(Boxer2, winner, sizeof(winner));
		                		if(MyJob[Boxer1] == JOB_BOXER) { PlayerInfo[Boxer1][pLoses] += 1; }
								if(MyJob[Boxer2] == JOB_BOXER) { PlayerInfo[Boxer2][pWins] += 1; }
			                	if(TBoxer < 255)
			                	{
			                	    if(IsPlayerConnected(TBoxer))
			                	    {
				                	    if(TBoxer != Boxer2)
				                	    {
				                	        if(MyJob[i] == JOB_BOXER)
				                	        {
				                	            TBoxer = Boxer2;
				                	            GetPlayerName(TBoxer, titel, sizeof(titel));
					                	        new nstring[MAX_PLAYER_NAME];
												format(nstring, sizeof(nstring), "%s", titel);
												strmid(Titel[TitelName], nstring, 0, strlen(nstring), 255);
					                	        Titel[TitelWins] = PlayerInfo[TBoxer][pWins];
					                	        Titel[TitelLoses] = PlayerInfo[TBoxer][pLoses];
					                	        SaveBoxer();
							                	format(string, sizeof(string), " + 챔피언 복서 %s 와 신인 복서 %s 의 경기가 끝났습니다.",  titel, loser);
												SendClientMessageToAll(COLOR_WHITE,string);
												cspeed[Boxer1] = 1;
												cspeed[Boxer2] = 1;
				                	        }
										}
										else
										{
										    GetPlayerName(TBoxer, titel, sizeof(titel));
										    format(string, sizeof(string), " + 복서 %s 님과 %s 님의 권투 경기가 끝났습니다.",  titel, loser);
											SendClientMessageToAll(COLOR_WHITE,string);
											Titel[TitelWins] = PlayerInfo[TBoxer][pWins];
				                	        Titel[TitelLoses] = PlayerInfo[Boxer2][pLoses];
				                	        SaveBoxer();
				                	        cspeed[Boxer1] = 1;
											cspeed[Boxer2] = 1;
										}
									}
								}//TBoxer
								format(string, sizeof(string), " + 당신은 %s님 과의 경기에서 졌습니다.", winner);
								SendClientMessage(Boxer1, COLOR_LIGHTBLUE, string);
								GameTextForPlayer(Boxer1, "~r~You lost", 3500, 1);
								format(string, sizeof(string), " + 당신은 %s님과의 경기에서 이겼습니다!", loser);
								SendClientMessage(Boxer2, COLOR_LIGHTBLUE, string);
								GameTextForPlayer(Boxer2, "~r~You won", 3500, 1);
								GiveMoney(Boxer2, 30000);
								if(GetPlayerHealth(Boxer1, health) < 20)
								{
								    SetPlayerHealth(Boxer1, 30.0);
								}
								else
								{
								    SetPlayerHealth(Boxer1, 50.0);
								}
								if(GetPlayerHealth(Boxer2, health) < 20)
								{
							    	SetPlayerHealth(Boxer2, 30.0);
								}
								else
								{
								    SetPlayerHealth(Boxer2, 50.0);
								}
                                GameTextForPlayer(Boxer1, "~g~Match Over", 5000, 1); GameTextForPlayer(Boxer2, "~g~Match Over", 5000, 1);
								if(MyJob[Boxer2] == JOB_BOXER) { PlayerInfo[Boxer2][pBoxSkill] += 1; }
								PlayerBoxing[Boxer1] = 0;
								PlayerBoxing[Boxer2] = 0;
							}
			            }
			            else if(Lost == 2)
			            {
			                if(IsPlayerConnected(Boxer1) && IsPlayerConnected(Boxer2))
			                {
					        	SetPlayerPos(Boxer1, 765.8433,3.2924,1000.7186); SetPlayerPos(Boxer2, 765.8433,3.2924,1000.7186);
					        	SetPlayerInterior(Boxer1, 5); SetPlayerInterior(Boxer2, 5);
			                	GetPlayerName(Boxer1, winner, sizeof(winner));
			                	GetPlayerName(Boxer2, loser, sizeof(loser));
		                		if(MyJob[Boxer2] == JOB_BOXER) { PlayerInfo[Boxer2][pLoses] += 1; }
								if(MyJob[Boxer1] == JOB_BOXER) { PlayerInfo[Boxer1][pWins] += 1; }
			                	if(TBoxer < 255)
			                	{
			                	    if(IsPlayerConnected(TBoxer))
			                	    {
				                	    if(TBoxer != Boxer1)
				                	    {
				                	        if(MyJob[Boxer1] == JOB_BOXER)
				                	        {
					                	        TBoxer = Boxer1;
					                	        GetPlayerName(TBoxer, titel, sizeof(titel));
					                	        new nstring[MAX_PLAYER_NAME];
												format(nstring, sizeof(nstring), "%s", titel);
												strmid(Titel[TitelName], nstring, 0, strlen(nstring), 255);
					                	        Titel[TitelWins] = PlayerInfo[TBoxer][pWins];
					                	        Titel[TitelLoses] = PlayerInfo[TBoxer][pLoses];
					                	        SaveBoxer();
							                	format(string, sizeof(string), " + 챔피언 복서 %s 와  %s 의 경기가 끝났습니다.",  titel, loser);
												SendClientMessageToAll(COLOR_WHITE,string);
												cspeed[Boxer1] = 1;
												cspeed[Boxer2] = 1;
											}
										}
										else
										{
										    GetPlayerName(TBoxer, titel, sizeof(titel));
										    format(string, sizeof(string), " + 복서 %s 와 %s 의 경기가 끝났습니다.",  titel, loser);
											SendClientMessageToAll(COLOR_WHITE,string);
											Titel[TitelWins] = PlayerInfo[TBoxer][pWins];
				                	        Titel[TitelLoses] = PlayerInfo[Boxer1][pLoses];
				                	        SaveBoxer();
				                	        cspeed[Boxer1] = 1;
											cspeed[Boxer2] = 1;
										}
									}
								}//TBoxer
								format(string, sizeof(string), " + 당신은 %s 님과의 경기에서 졌습니다.", winner);
								SendClientMessage(Boxer2, COLOR_LIGHTBLUE, string);
								GameTextForPlayer(Boxer2, "~r~You lost", 3500, 1);
								format(string, sizeof(string), " + 당신은 %s 님과의 경기에서 이겼습니다!!", loser);
								SendClientMessage(Boxer1, COLOR_LIGHTBLUE, string);
								GameTextForPlayer(Boxer1, "~g~You won", 3500, 1);
								GiveMoney(Boxer1, 30000);
								if(GetPlayerHealth(Boxer1, health) < 20)
								{
								    SetPlayerHealth(Boxer1, 30.0);
								}
								else
								{
								    SetPlayerHealth(Boxer1, 50.0);
								}
								if(GetPlayerHealth(Boxer2, health) < 20)
								{
							    	SetPlayerHealth(Boxer2, 30.0);
								}
								else
								{
								    SetPlayerHealth(Boxer2, 50.0);
								}
                                GameTextForPlayer(Boxer1, "~g~Match Over", 5000, 1); GameTextForPlayer(Boxer2, "~g~Match Over", 5000, 1);
								if(MyJob[Boxer1] == JOB_BOXER) { PlayerInfo[Boxer1][pBoxSkill] += 1; }
								PlayerBoxing[Boxer1] = 0;
								PlayerBoxing[Boxer2] = 0;
							}
			            }
			            InRing = 0;
			            RoundStarted = 0;
			            Boxer1 = 255;
			            Boxer2 = 255;
			            TBoxer = 255;
			            trigger = 0;
			        }
			    }
			}
     			if(BoxWaitTime[i] > 0)
			{
			    if(BoxWaitTime[i] >= BoxDelay)
				{
				    BoxDelay = 0;
					BoxWaitTime[i] = 0;
					PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
					GameTextForPlayer(i, "~g~Match Started", 5000, 1);
					TogglePlayerControllable(i, 1);
					RoundStarted = 1;
					ResetPlayerWeapons(i);
				}
			    else
				{
				    format(string, sizeof(string), "%d", BoxDelay - BoxWaitTime[i]);
					GameTextForPlayer(i, string, 1500, 6);
					BoxWaitTime[i] += 1;
					cspeed[i] = 0;
				}
			}
 }
}
public ClearCookingID(playerid, cook)
{
	if(IsPlayerConnected(playerid))
	{
		new string[MAX_PLAYER_NAME];
		format(string, sizeof(string), "없음");
		switch (cook)
		{
		    case 1:
		    {
		        strmid(Cooking[playerid][pCook1], string, 0, strlen(string), 255);
		        Cooking[playerid][pCWeight1] = 0;
		        Cooking[playerid][pCookID1] = 0;
		    }
		    case 2:
		    {
		        strmid(Cooking[playerid][pCook2], string, 0, strlen(string), 255);
		        Cooking[playerid][pCWeight2] = 0;
		        Cooking[playerid][pCookID2] = 0;
		    }
		    case 3:
		    {
		        strmid(Cooking[playerid][pCook3], string, 0, strlen(string), 255);
		        Cooking[playerid][pCWeight3] = 0;
		        Cooking[playerid][pCookID3] = 0;
		    }
		    case 4:
		    {
		        strmid(Cooking[playerid][pCook4], string, 0, strlen(string), 255);
		        Cooking[playerid][pCWeight4] = 0;
		        Cooking[playerid][pCookID4] = 0;
		    }
		    case 5:
		    {
		        strmid(Cooking[playerid][pCook5], string, 0, strlen(string), 255);
		        Cooking[playerid][pCWeight5] = 0;
		        Cooking[playerid][pCookID5] = 0;
		    }
		}
	}
	return 1;
}

public ClearGroceries(playerid)
{
	if(IsPlayerConnected(playerid))
	{
	    Groceries[playerid][pChickens] = 0; Groceries[playerid][pChicken] = 0;
	    Groceries[playerid][pHamburgers] = 0; Groceries[playerid][pHamburger] = 0;
	    Groceries[playerid][pPizzas] = 0; Groceries[playerid][pPizza] = 0;
	}
	return 1;
}
public ProxDetector(Float:radi, playerid, string[],col1,col2,col3,col4,col5)
{
    radi = 1300.0;
    new ch;
    ch =GetPlayerVirtualWorld(playerid);
	if(IsPlayerConnected(playerid))
	{

		new Float:posx, Float:posy, Float:posz;
		new Float:oldposx, Float:oldposy, Float:oldposz;
		new Float:tempposx, Float:tempposy, Float:tempposz;
		GetPlayerPos(playerid, oldposx, oldposy, oldposz);
		//radi = 2.0; //Trigger Radius
		for(new i = 0; i < MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i))
			{
					if(GetPlayerVirtualWorld(i) == ch || ismadmin(i)) {
						GetPlayerPos(i, posx, posy, posz);
						tempposx = (oldposx -posx);
						tempposy = (oldposy -posy);
						tempposz = (oldposz -posz);
						//printf("DEBUG: X:%f Y:%f Z:%f",posx,posy,posz);
						if (((tempposx < radi/16) && (tempposx > -radi/16)) && ((tempposy < radi/16) && (tempposy > -radi/16)) && ((tempposz < radi/16) && (tempposz > -radi/16)))
						{
							SendClientMessage(i, col1, string);
						}
						else if (((tempposx < radi/8) && (tempposx > -radi/8)) && ((tempposy < radi/8) && (tempposy > -radi/8)) && ((tempposz < radi/8) && (tempposz > -radi/8)))
						{
							SendClientMessage(i, col2, string);
						}
						else if (((tempposx < radi/4) && (tempposx > -radi/4)) && ((tempposy < radi/4) && (tempposy > -radi/4)) && ((tempposz < radi/4) && (tempposz > -radi/4)))
						{
							SendClientMessage(i, col3, string);
						}
						else if (((tempposx < radi/2) && (tempposx > -radi/2)) && ((tempposy < radi/2) && (tempposy > -radi/2)) && ((tempposz < radi/2) && (tempposz > -radi/2)))
						{
							SendClientMessage(i, col4, string);
						}
						else if (((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
						{
							SendClientMessage(i, col5, string);
						} else if(ismadmin(i)){
							SendClientMessage(i, col1, string);
						}
					}
			}
		}
	}//not connected
	return 1;
}

public JobMoney() {
	for(new i = 0; i < MAX_PLAYERS; i++) {
	  if(MyJob[i] == JOB_TAXI) {
	  GiveMoney(i, 100000);
	  SendClientMessage(i, COLOR_LIME, " + 당신은 택시협회에서 보조금 $100000 를 받았습니다.");
	  }
   }
}

public FishTimes() {
	for(new playerid = 0; playerid<MAX_PLAYERS; playerid++) {

	    new FishUP[MAX_PLAYERS];
	    if(bokbuy[playerid] == 1)
		{
		    if(boktime[playerid] <= 0)
		    {
		      new bok;
         bok = random(100)+1;
         if(bok == 100){
         new name[MAX_PLAYER_NAME];
		 new str[256];
		GetPlayerName(playerid, name, sizeof(name));
		format(str, sizeof(str), "+  %s 님께서 복권 1등 당첨 되셨습니다! /cong 로 축하드립시다!! 와~~~~", name);
		SendClientMessageToAll(LEONES_COLOR, str);
    	GiveMoney(playerid, 1000000);
   		}
   		else if(bok == 99 || bok == 98){
   		SendClientMessage(playerid,0xFF6347AA," + 2등!!! : $60000 운이 좋으시군요...~~");
    	GiveMoney(playerid, 600000);
   		}
   		else if(bok == 97 || bok == 96 || bok == 95|| bok == 94){
        SendClientMessage(playerid,0xFF6347AA," + 3등 : $30000");
    	GiveMoney(playerid, 300000);
   		}
   		else if(bok >= 77 && bok <= 93){
        SendClientMessage(playerid,0xFF6347AA," + 4등 : $10000");
  		 GiveMoney(playerid, 100000);
   		}
   		else if(bok >= 71 && bok <= 76){
   		SendClientMessage(playerid,0xFF6347AA," + 한번 더 : $3000");
  		 GiveMoney(playerid, 3000);
   		}
   		else if(bok <= 70){
      	SendClientMessage(playerid,0xFF6347AA," + '꽝' 흑흑. 아쉽게 營윱求... 힘 내세요!");
   		}
		bokbuy[playerid] = 0;
			}
   			if(boktime[playerid] == 2)
		    {
 		   		new boktext = random(5);
   				switch(boktext){
       			case 0:{SendClientMessage(playerid,COLOR_MAROON," 과연!!!!!!!!!!!!!!!!!!!!!! 당첨??????");}
 				case 1:{SendClientMessage(playerid,COLOR_ORANGE," 흘흘.... 기대되죠???");}
       			case 2:{SendClientMessage(playerid,COLOR_YELLOW," 흘흘.... 기대되죠???");}
       			case 3:{SendClientMessage(playerid,COLOR_CYAN," 긁는중..... 꽝이 나올지도~~~~ 몰라요~~~ 키득키득");}
       			case 4:{SendClientMessage(playerid,COLOR_GREEN," 빨리 다 긁었으면 좋겠죠???");}
  				}
			}
			boktime[playerid] --;
		}
	    FishUP[playerid] = 0;

	    if(FishTime[playerid] >= 0) {
	    FishTime[playerid] --;
	    }
		if(IsAtFishPlace(playerid))
	        {
	    if(FishTime[playerid] == 2) {

	            new Caught;
	            new rand;
	            new fstring[MAX_PLAYER_NAME];
	            new Level = PlayerInfo[playerid][pFishSkill];
	            if(Level >= 0 && Level <= 50) { Caught = random(20)-7; }
	            else if(Level >= 51 && Level <= 100) { Caught = random(50)-20; }
	            else if(Level >= 101 && Level <= 200) { Caught = random(100)-50; }
	            else if(Level >= 201 && Level <= 400) { Caught = random(160)-60; }
	            else if(Level >= 401) { Caught = random(180)-70; }
	            rand = random(FishNamesNumber);
	            if(Caught <= 0)
	            {
	                SendClientMessage(playerid, COLOR_GREY, " + 줄을 놓쳤습니다.");
					FishUP[playerid] = 1;
	            }
	            else if(rand == 0)
	            {
	                SendClientMessage(playerid, COLOR_GREY, " + 자켓이 걸렸습니다....");
	                FishUP[playerid] = 1;
	            }
	            else if(rand == 4)
	            {
	                SendClientMessage(playerid, COLOR_GREY, "  + 팬티가 걸렸습니다....");
	                FishUP[playerid] = 1;
	            }
	            else if(rand == 7)
	            {
	                SendClientMessage(playerid, COLOR_GREY, " + 물고기를 놓쳤습니다....");
	                FishUP[playerid] = 1;
	            }
	            else if(rand == 10)
	            {
	                SendClientMessage(playerid, COLOR_GREY, " + 신발이 걸렸습니다....");
	                FishUP[playerid] = 1;
	            }
	            else if(rand == 13)
	            {
	                SendClientMessage(playerid, COLOR_GREY, "  + 쓰레기가 걸렸습니다....");
	                FishUP[playerid] = 1;
	            }
	            else if(rand == 20)
	            {
	                new mrand = random(500);
	                format(strings, sizeof(strings), " + 당신의 낚시대에 지갑이 걸려 $%d를 획득했습니다.", mrand);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
	                GiveMoney(playerid, mrand);
	                FishUP[playerid] = 1;
	            }
	            if(FishUP[playerid] == 0) {
		        if(Fishes[playerid][pWeight1] == 0)
		        {
		            PlayerInfo[playerid][pFishSkill] += 1;
		            format(fstring, sizeof(fstring), "%s", FishNames[rand]);
					strmid(Fishes[playerid][pFish1], fstring, 0, strlen(fstring), 255);
					Fishes[playerid][pWeight1] = Caught;
					format(strings, sizeof(strings), " + 당신은 %s 물고기 %dcm 짜리를 잡았습니다!!", Fishes[playerid][pFish1], Caught);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
					Fishes[playerid][pLastWeight] = Caught;
					Fishes[playerid][pLastFish] = 1;
					Fishes[playerid][pFid1] = rand;
					Fishes[playerid][pFishID] = rand;
					if(Caught > PlayerInfo[playerid][pBiggestFish])
					{
					    format(strings, sizeof(strings), " + 당신이 잡은 물고기는 당신의 최고 기록 %dcm 짜리 물고기보다 긴 %dcm 입니다!!", PlayerInfo[playerid][pBiggestFish], Caught);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
						PlayerInfo[playerid][pBiggestFish] = Caught;
					}
     			}
		        else if(Fishes[playerid][pWeight2] == 0)
		        {
		            PlayerInfo[playerid][pFishSkill] += 1;
		            format(fstring, sizeof(fstring), "%s", FishNames[rand]);
					strmid(Fishes[playerid][pFish2], fstring, 0, strlen(fstring), 255);
					Fishes[playerid][pWeight2] = Caught;
					format(strings, sizeof(strings), " + 당신은 %s 물고기 %dcm 짜리를 잡았습니다!!", Fishes[playerid][pFish2], Caught);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
					Fishes[playerid][pLastWeight] = Caught;
					Fishes[playerid][pLastFish] = 2;
					Fishes[playerid][pFid2] = rand;
					Fishes[playerid][pFishID] = rand;
					if(Caught > PlayerInfo[playerid][pBiggestFish])
					{
					    format(strings, sizeof(strings), " + 당신이 잡은 물고기는 당신의 최고 기록 %dcm 짜리 물고기보다 긴 %dcm 입니다!!", PlayerInfo[playerid][pBiggestFish], Caught);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
						PlayerInfo[playerid][pBiggestFish] = Caught;
					}
		        }
		        else if(Fishes[playerid][pWeight3] == 0)
		        {
		            PlayerInfo[playerid][pFishSkill] += 1;
		            format(fstring, sizeof(fstring), "%s", FishNames[rand]);
					strmid(Fishes[playerid][pFish3], fstring, 0, strlen(fstring), 255);
					Fishes[playerid][pWeight3] = Caught;
					format(strings, sizeof(strings), " + 당신은 %s 물고기 %dcm 짜리를 잡았습니다!!", Fishes[playerid][pFish3], Caught);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
					Fishes[playerid][pLastWeight] = Caught;
					Fishes[playerid][pLastFish] = 3;
					Fishes[playerid][pFid3] = rand;
					Fishes[playerid][pFishID] = rand;
					if(Caught > PlayerInfo[playerid][pBiggestFish])
					{
					    format(strings, sizeof(strings), " + 당신이 잡은 물고기는 당신의 최고 기록 %dcm 짜리 물고기보다 긴 %dcm 입니다!!", PlayerInfo[playerid][pBiggestFish], Caught);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
						PlayerInfo[playerid][pBiggestFish] = Caught;
					}
		        }
		        else if(Fishes[playerid][pWeight4] == 0)
		        {
		            PlayerInfo[playerid][pFishSkill] += 1;
		            format(fstring, sizeof(fstring), "%s", FishNames[rand]);
					strmid(Fishes[playerid][pFish4], fstring, 0, strlen(fstring), 255);
					Fishes[playerid][pWeight4] = Caught;
					format(strings, sizeof(strings), " + 당신은 %s 물고기 %dcm 짜리를 잡았습니다!!", Fishes[playerid][pFish4], Caught);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
					Fishes[playerid][pLastWeight] = Caught;
					Fishes[playerid][pLastFish] = 4;
					Fishes[playerid][pFid4] = rand;
					Fishes[playerid][pFishID] = rand;
					if(Caught > PlayerInfo[playerid][pBiggestFish])
					{
					    format(strings, sizeof(strings), " + 당신이 잡은 물고기는 당신의 최고 기록 %dcm 짜리 물고기보다 긴 %dcm 입니다!!", PlayerInfo[playerid][pBiggestFish], Caught);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
						PlayerInfo[playerid][pBiggestFish] = Caught;
					}
		        }
		        else if(Fishes[playerid][pWeight5] == 0)
		        {
		            PlayerInfo[playerid][pFishSkill] += 1;
		            format(fstring, sizeof(fstring), "%s", FishNames[rand]);
					strmid(Fishes[playerid][pFish5], fstring, 0, strlen(fstring), 255);
					Fishes[playerid][pWeight5] = Caught;
					format(strings, sizeof(strings), " + 당신은 %s 물고기 %dcm 짜리를 잡았습니다!!", Fishes[playerid][pFish5], Caught);
					SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
					Fishes[playerid][pLastWeight] = Caught;
					Fishes[playerid][pLastFish] = 5;
					Fishes[playerid][pFid5] = rand;
					Fishes[playerid][pFishID] = rand;
					if(Caught > PlayerInfo[playerid][pBiggestFish])
					{
					    format(strings, sizeof(strings), " + 당신이 잡은 물고기는 당신의 최고 기록 %dcm 짜리 물고기보다 긴 %dcm 입니다!!", PlayerInfo[playerid][pBiggestFish], Caught);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
						PlayerInfo[playerid][pBiggestFish] = Caught;
					}
		        }
		        }
		        else
		        {
		            if(FishUP[playerid] == 0) {
		            SendClientMessage(playerid, COLOR_GREY, " + 당신의 바구니의 공간이 없어서 그냥 놓아주었습니다.");
		            }
		        }
		        	udb_UserSetInt(loginid[playerid],"fishWeight5",Fishes[playerid][pWeight5]);
    udb_UserSetInt(loginid[playerid],"fishWeight4",Fishes[playerid][pWeight4]);
    udb_UserSetInt(loginid[playerid],"fishWeight3",Fishes[playerid][pWeight3]);
    udb_UserSetInt(loginid[playerid],"fishWeight2",Fishes[playerid][pWeight2]);
    udb_UserSetInt(loginid[playerid],"fishWeight1",Fishes[playerid][pWeight1]);
	udb_UserSetInt(loginid[playerid],"fishFid5",Fishes[playerid][pFid5]);
    udb_UserSetInt(loginid[playerid],"fishFid4",Fishes[playerid][pFid4]);
    udb_UserSetInt(loginid[playerid],"fishFid3",Fishes[playerid][pFid3]);
    udb_UserSetInt(loginid[playerid],"fishFid2",Fishes[playerid][pFid2]);
    udb_UserSetInt(loginid[playerid],"fishFid1",Fishes[playerid][pFid1]);
   	udb_UserSet(loginid[playerid],"fish5",Fishes[playerid][pFish5]);
    udb_UserSet(loginid[playerid],"fish4",Fishes[playerid][pFish4]);
    udb_UserSet(loginid[playerid],"fish3",Fishes[playerid][pFish3]);
    udb_UserSet(loginid[playerid],"fish2",Fishes[playerid][pFish2]);
    udb_UserSet(loginid[playerid],"fish1",Fishes[playerid][pFish1]);
    udb_UserSetInt(loginid[playerid],"pFishSkill",PlayerInfo[playerid][pFishSkill]);
    udb_UserSetInt(loginid[playerid],"pBiggestFish",PlayerInfo[playerid][pBiggestFish]);
    exp[playerid]++;
    GetPlayerLevel(playerid);

	            if(PlayerInfo[playerid][pFishSkill] == 50)
				{ SendClientMessage(playerid, COLOR_YELLOW, " + 당신의 낚시 스킬 레벨이 2로 올랐습니다!!!"); }
				else if(PlayerInfo[playerid][pFishSkill] == 250)
				{ SendClientMessage(playerid, COLOR_YELLOW, " + 당신의 낚시 스킬 레벨이 3으로 올랐습니다!!!"); }
				else if(PlayerInfo[playerid][pFishSkill] == 500)
				{ SendClientMessage(playerid, COLOR_YELLOW, " + 당신의 낚시 스킬 레벨이 4로 올랐습니다!!!"); }
				else if(PlayerInfo[playerid][pFishSkill] == 1000)
				{ SendClientMessage(playerid, COLOR_YELLOW, " + 당신의 낚시 스킬 레벨이 5로 올랐습니다!!!"); }
   			  }
		 	}
	        else
	        {
	            if(FishTime[playerid] == 1) {
	            SendClientMessage(playerid, COLOR_GREY, " + 당신은 낚시 전용 구역에 계시고 있지 않습니다 -ㅅ-");
				FishTime[playerid] = 0;
	            }
	        }
		}
 }


public ownedtimes() {
	for(new i=0; i<MAX_POINTS; i++) {
	if(ownedtime[i] >= 1) {
		ownedtime[i]--;
	}
	if(payandspray[i] > 0) {
 		payandspray[i]--;
	}
	}
return 1;
}


stock IsPartOfName(partofname[],playerid) { //이름의 일부가 맞는지 확인
new playername[MAX_PLAYER_NAME];//변수 선언
GetPlayerName(playerid,playername,MAX_PLAYER_NAME);//이름을 얻어낸다
return strcmp(partofname,playername,false,strlen(partofname));//strcmp함수로 전달된 partofname인수의 길이만큼 글자가 일치하는지 알아본다.그리고 그값을 바로 리턴.
}

stock GetClossetPlayerID(partofname[]) {//이름의 일부가 일치하는 아이디 찾기
for(new i=0,j=GetMaxPlayers();i<j;i++) if(IsPlayerConnected(i) && IsPartOfName(partofname,i)==0) return i;//for문을 돌려서 모든 플레이어에게 적용. 접속중이고 이름의 일부가 일치하면 해당 아이디를 돌려준다.
return -1;//그런 플레이어 없더라. -1을 돌려준다.
}

stock IsPartOfNameEx(partofname[],fullname[]) {//이름 일부 맞나 확인하기(한닉도 가능)
return equalC(partofname,ret_memcpy(fullname,0,strlen(partofname));//equalC를 사용한다.
}

