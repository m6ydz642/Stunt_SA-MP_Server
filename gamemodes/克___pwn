							    format(string, sizeof(string), " + 당신은 %s(%d%%완성도) 짜리를 요리했습니다.",Cooking[playerid][pCook5],Cooking[playerid][pCWeight5]);
							    SendClientMessage(playerid,COLOR_LIGHTBLUE, string);
						    }

						    else
						    {
						        SendClientMessage(playerid, COLOR_GREY, " + 음식을 넣을 바구니가 꽉 찼습니다.");
						        return 1;
						    }
				        }
				        else
				        {
				            SendClientMessage(playerid, COLOR_GREY, " + 그 음식의 재료가 없습니다. 재료는 편의점에서 구입할 수 있습니다.");
				            return 1;
				        }
					}
					else
					{
					    SendClientMessage(playerid, COLOR_GREY, " + 당신은 피자집에 있지 않습니다.");
                        SendClientMessage(playerid, COLOR_GREY, " + 당신이 해당 가게에 들어왔는데도 위에 문구가 뜨면 그가게 주방장 쪽 으로 들어가서");
                        SendClientMessage(playerid, COLOR_GREY, " + 해당 명령어를 다시 입력 해 보시길 바랍니다");
						return 1;
					}
				}
				else
				{
				    SendClientMessage(playerid, COLOR_GREY, " + 그런 음식은 없습니다.");
				    return 1;
				}
	        }
	        else
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 당신은 치킨벨 / 버거샷 / 피자가게 에 계시고 있지 않습니다.");
                SendClientMessage(playerid, COLOR_GREY, " + 당신이 해당 가게에 들어왔는데도 위에 문구가 뜨면 그가게 주방장 쪽 으로 들어가서");
                SendClientMessage(playerid, COLOR_GREY, " + 해당 명령어를 다시 입력 해 보시길 바랍니다");
				return 1;
	        }
	    }
	 } else {SendClientMessage(playerid, COLOR_GREY, " + 당신은 요리사가 아닙니다."); return 1;}
	 	udb_UserSetInt(loginid[playerid],"pChicken",Groceries[playerid][pChicken]);
    udb_UserSetInt(loginid[playerid],"pChickens",Groceries[playerid][pChickens]);
    udb_UserSetInt(loginid[playerid],"pHamburger",Groceries[playerid][pHamburger]);
    udb_UserSetInt(loginid[playerid],"pHamburgers",Groceries[playerid][pHamburgers]);
    udb_UserSetInt(loginid[playerid],"pPizza",Groceries[playerid][pPizza]);
    udb_UserSetInt(loginid[playerid],"pPizzas",Groceries[playerid][pPizzas]);

