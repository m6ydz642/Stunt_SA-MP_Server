				} else {
					SendClientMessage(playerid,COLOR_GREEN," + 로그인에 실패했습니다");
					SendClientMessage(playerid,COLOR_GREEN," + 아이디나 비밀번호가 틀리지않은지 확인해주세요");
	//				printf("  |--login failed > %d",playerid); //도스창에 결과
				}
			}
		} else {
