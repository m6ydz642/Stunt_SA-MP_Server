			if(IsPlayerConnected(i)) {
			    new count;
			    count = GetPlayerVirtualWorld(i);
				if(count == 0) channel0++;
				if(count == 1) channel1++;
				if(count == 2) channel2++;
			}
		}
		SendClientMessage(playerid,COLOR_GREEN," + USAGE: /channel [channel]");
		//if((word_h >= 20 || word_h < 4)) {
	    //    format(channelstr,sizeof(channelstr)," + [PK채널] 0채널 (%d명)",channel0);
