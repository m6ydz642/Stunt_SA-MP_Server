	if(strcmp(cmd, "/watch", true) == 0)
	{
	if(isadmin(playerid))
	{
	new Playerid;
	tmp = strtok(cmdtext, idx);

	if(!strlen(tmp))
	{
	SendClientMessage(playerid, COLOR_YELLOW, "USAGE: /watch [id]");
	return 1;
	}
	Playerid = strval(tmp);

	if(!strlen(tmp))
	{
	SendClientMessage(playerid, COLOR_YELLOW, "USAGE: /watch [id]");
	return 1;
	}
	TogglePlayerSpectating(playerid, 1);
	PlayerSpectatePlayer(playerid, Playerid);
