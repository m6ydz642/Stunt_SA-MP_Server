loginsystem(playerid,moneysave) {
	if(loginflag[playerid] == 0) return 1;
	new Float:X,Float:Y,Float:Z;
	new Float:hp;
	new nadmin;
	new ip[120];

	GetPlayerPos(playerid,X,Y,Z);

	udb_UserSetInt(loginid[playerid],"quest",quest[playerid]);

	if(moneysave == 1) udb_UserSetInt(loginid[playerid],"money",moneyhack[playerid]);
    udb_UserSetInt(loginid[playerid],"bank",bank[playerid]);
    udb_UserSetInt(loginid[playerid],"isJail",isJail[playerid]);
    udb_UserSetInt(loginid[playerid],"JailTime",JailTime[playerid]);
//    udb_UserSetInt(loginid[playerid],"Int",GetPlayerInterior(playerid));
    udb_UserSetInt(loginid[playerid],"bounty",bounty[playerid]);
    GetPlayerIp(playerid,ip,120);
    udb_UserSet(loginid[playerid],"ip",ip);
	udb_UserSetFloat(loginid[playerid],"X",X);
	udb_UserSetFloat(loginid[playerid],"Y",Y);
	udb_UserSetFloat(loginid[playerid],"Z",Z);
	GetPlayerHealth(playerid,hp);
	udb_UserSetFloat(loginid[playerid],"Health",hp);
	udb_UserSetInt(loginid[playerid],"Int",GetPlayerInterior(playerid));

	udb_UserSetInt(loginid[playerid],"pBoxSkill",PlayerInfo[playerid][pBoxSkill]);
	udb_UserSetInt(loginid[playerid],"pWins",PlayerInfo[playerid][pWins]);
	udb_UserSetInt(loginid[playerid],"pLoses",PlayerInfo[playerid][pLoses]);

	nadmin = 0;
	if(IsPlayerAdmin(playerid)) nadmin = 3;
	if(madmin[playerid]==1) nadmin = 2;
	if(madmin[playerid]==2) nadmin = 3;
	if(madmin[playerid]==3) nadmin = 4;

	udb_UserSetInt(loginid[playerid],"admin",nadmin);
	printf(" + %d번 님 정보 저장이 완료되었습니다 (자동저장)",playerid);
	return 1;
}
//이건 맨 아래 아무곳에나~~
public notice() {
	new notice100 = random(18);
	if(notice100 == 0) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ /quest 로 자신의 퀘스트 목록을 볼 수 있다하오~");
//		SetGravity(0.008);
	} else if(notice100 == 1) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 도박은 불가능하오..... (유저분들이 돈핵방지를 요구해서)");
//		SetGravity(0.004);
	} else if(notice100 == 2) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 차량 잠금은 /lock 이라 하오....");
//		SetGravity(0.001);
	} else if(notice100 == 3) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 대쉬보드(속도계) 작동은 /dash 이라하오~~");
//		SetGravity(0.0009);
	} else if(notice100 == 4) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 차량기름은 로그아웃했다 접속해도 저장된다하오 (리붓제외)");
//		SetGravity(0.01);
	} else if(notice100 == 5) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 직업마다 할 수 있는 명령어가 다르다오~");
//		SetGravity(00.0005);
	} else if(notice100 == 6) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 아이디나 비밀번호를 1등등등 너무 단순하게 하지마시오 ~ (책임안짐)");
	} else if(notice100 == 7) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 차소환이 안되세요?? '/carspawn 차량번호' 한번해보고 다시 소환해보시오..");
	} else if(notice100 == 8) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 계정 비밀번호를 바꾸고 싶으세요? /chpass [현재비밀번호] [바꿀비밀번호] 를 입력해보시오~");
	} else if(notice100 == 9) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 차량에 타고 시동만 걸어놔도 기름이 떨어진다하오 ~");
	} else if(notice100 == 10) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 전화 연결 하기는 /phone {playerid} 라 하오~~!");
	} else if(notice100 == 11) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 외치기, 귓말 돈든다오. -_-");
	} else if(notice100 == 12 || notice100 == 13 || notice100 == 14) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ PK 타임은 없다하오.");
	} else if(notice100 == 15 || notice100 == 16 || notice100 == 17) {
		SendClientMessageToAll(COLOR_PINK," + ★[Tip]★ 채널 변경하기 : /channel (0채널 = 기본, 1채널 = PK 채널) 이라 하오~");
	}
}


//==============================================================================

public RemoveRamp(playerid)
{
	if (rampid[playerid] != -1) {
	    DestroyPlayerObject(playerid,rampid[playerid]);
	    rampid[playerid] = -1;
	}
}
public Float:GetOptimumRampDistance(playerid)
{
	new ping = GetPlayerPing(playerid), Float:dist;
	dist = floatpower(ping, 0.25);
	dist = dist*4.0;
	dist = dist+5.0;
	return dist;
}
public Float:GetXYInFrontOfPlayer(playerid, &Float:x, &Float:y, Float:distance)
{
	new Float:a;
	GetPlayerPos(playerid, x, y, a);
	if (IsPlayerInAnyVehicle(playerid)) GetVehicleZAngle(GetPlayerVehicleID(playerid), a);
	else GetPlayerFacingAngle(playerid, a);
	x += (distance * floatsin(-a, degrees));
	y += (distance * floatcos(-a, degrees));
	return a;
}
public OnPlayerPickUpPickup(playerid, pickupid)
{
	DestroyPickup(pickupid);
		if(	picklist[0] == pickupid || picklist[1] == pickupid || picklist[2] == pickupid || picklist[3] == pickupid) {
		    if(picklist[0] == pickupid) {
		        OnPlayerCommandText(playerid,"/sv");
		    } else if(picklist[1] == pickupid) {
		    } else if(picklist[2] == pickupid) {
		    } else if(picklist[3] == pickupid) {
			}
		    return 0;
		}
        if(PickUpMoney[pickupid]) {
			GiveMoney(playerid, 200); // Will give you 500$ when you pickup money
		}
        if(DropPick[pickupid])SetTimerEx("PickDestroy", 500, 0, "d", pickupid); // Destroying pickups
        return 1;
}

public PickDestroy(o) {
	for(new i = 0; i < 32; i++) {
		if(o == i) {
			return;
		}
	}
	DestroyPickup(o);
}


public mtime() //싱글시간
{
	word_m = word_m + 1;
	if(word_m == 60) {
	    word_m = 0;
	    word_h = word_h + 1;
	    SetWorldTime(word_h);
	    if(word_h == 24) {
	        word_h = 0;
		}
	}
}


public SyncVirtualTime() { //싱글시간
    // Even if there's no "big" differences, we remove them every 2 minutes
    // (see timer above)

//    CalculateVirtualTime(h,m);
    for(new i = 0; i < MAX_PLAYERS; i++) {
        if(IsPlayerConnected(i)) {
            SetPlayerTime(i,word_h,word_m);
        }
    }
}

/*
CalculateVirtualTime(&hour,&minute) {
    // This is just for Creating "a" time that is based on the Server's time,
    // so it'll calculate all the time FINE for everyone, no matter when he
    // joins, it'll be the same
    new h,i,s;
    gettime(h,i,s);
    new time = (h * 24 * 60) + i * 60 + s;
    time = time % (24 * 60);
    minute = time % 60;
    hour = (time - minute) / 60;
}
*/

public randomweather() {
	new weathers = random(20);
	switch(weathers) {
		case 0: SetWeather(0);
		case 1: SetWeather(0);
		case 2: SetWeather(0);
		case 3: SetWeather(0);
		case 4: SetWeather(1);
		case 5: SetWeather(1);
		case 6: SetWeather(1);
		case 7: SetWeather(10);
		case 8: SetWeather(3);
		case 9: SetWeather(3);
		case 10: SetWeather(3);
		case 11: SetWeather(1);
		case 12: SetWeather(14);
		case 13: SetWeather(14);
		case 14: SetWeather(14);
		case 15: SetWeather(1);
		case 16: SetWeather(3);
		case 17: SetWeather(0);
		case 18: SetWeather(0);
		case 19: SetWeather(1);
		case 20: SetWeather(1);
	}
}



public lotto()
{
new str[256];
new i;
new playername[MAX_PLAYER_NAME];

   	 	if(lottosay==0){
  		SendClientMessageToAll(IVORY," ");
  		SendClientMessageToAll(IVORY," + 곧 로또 추첨을 시작 하겠 습니다.");
  		//printf(" + 곧 로또 추첨 을 시작 하겠습니다");
  		SendClientMessageToAll(IVORY," ");
  		SendClientMessageToAll(IVORY," + 빨리 로또를 사세요!");
  		SendClientMessageToAll(IVORY," + 로또사기: /lotto");
  		lottosay=1;
  		SetTimer("lotto",8000,0);
 	}
 	else if(lottosay==1){
  		SendClientMessageToAll(IVORY," ");
  		SendClientMessageToAll(IVORY," + 기다리고 기다리던 로또 추첨을 좀있다 시작할꺼에요~~!");
  		//printf(" + 기다리고 기다리던 로또 추첨을 좀있다 시작할꺼에요~~!");
  		SendClientMessageToAll(IVORY," ");
  		SendClientMessageToAll(IVORY," + 빨리 로또를 사세요!");
  		SendClientMessageToAll(IVORY," + 로또사기: /lotto");
  		SendClientMessageToAll(IVORY," ");
  		lottosay=2;
  		SetTimer("lotto",8000,0);
 	}
 else if(lottosay==2){
   SendClientMessageToAll(IVORY," ");
  SendClientMessageToAll(IVORY," + 이제 로또 추첨을 시작하겠습니다.");
  //printf(" + 이제 로또 추첨을 시작하겠습니다");
  SendClientMessageToAll(PINK," + 지금 부터는 로또를 살 수 없습니다.");
  //printf(" + 지금 부터는 로또를 살 수 없습니다");
  SendClientMessageToAll(IVORY," ");
  SendClientMessageToAll(IVORY," + 오늘의 번호는 뭘까요?");
  //printf(" + 오늘의 번호는 뭘까요?");
  SendClientMessageToAll(COLOR_ORANGE," ");
  lottosay=3;
  SetTimer("lotto",6000,0);
 }
 else if(lottosay==3){
     Lotto=random(25)+1;
  SendClientMessageToAll(IVORY," ");
  format(str,sizeof(str)," +       %d번 입니다!!!!",Lotto);
  //printf(" + %d번 입니다!!!!",Lotto);
  SendClientMessageToAll(IVORY,str);
  SendClientMessageToAll(IVORY," ");
  SendClientMessageToAll(IVORY," + 오늘의 당첨자는 몇명 일까요??");
  //printf(" + 오늘의 당첨자는 몇명 일까요??");
  SendClientMessageToAll(IVORY," ");
  lottosay=4;
  SetTimer("lotto",6000,0);
 }
 else if(lottosay==4){
     for(i=0;i<MAX_PLAYERS;i++){
   if(lottonumber[i]==Lotto){
       Number++;
   }
  }
  lottotest += 100000; //로또 금액 시작
  SendClientMessageToAll(IVORY," ");
  format(str,sizeof(str)," +       %d명 입니다!",Number);
  //printf(" + 오늘의 당첨자는 %d명 입니다",Number); //도스창에 결과출력
  SendClientMessageToAll(IVORY,str);
  SendClientMessageToAll(IVORY," ");
  SendClientMessageToAll(IVORY," + 당첨금은 당첨자 끼리 똑같이 나누어 가지게 됩니다.");
  //printf(" + 당첨금은 당첨자 끼리 똑같이 나누어 가지게 됩니다. "); //도스창에 결과출력
  format(str,sizeof(str)," + 이번 당첨금은 : $%d입니다. (정부 지원금 포함)",lottotest);
  //printf(" + 이번 당첨금은 : $%d입니다. (정부 지원금 포함)",lottotest); //도스창에 결과출력
  SendClientMessageToAll(IVORY,str);
  SendClientMessageToAll(IVORY," + 당첨자가 0명 인경우 누적금은 다음 회에 누적됩니다.");
  //printf(" + 당첨자가 0명 인경우 누적금은 다음 회에 누적됩니다. "); //도스창에 결과출력
  
    for(i=0;i<MAX_PLAYERS;i++){
    if(lottonumber[i]==Lotto){
    GetPlayerName(i, playername, sizeof(playername));
  	format(str,sizeof(str)," + %s 님 께서 로또에 당첨되셨습니다.",playername);
  	//printf(" + %s 님 께서 로또에 당첨되셨습니다",playername); //도스창에 결과출력
  	SendClientMessageToAll(COLOR_LIMEGREEN,str);
	GiveMoney(i, lottotest/Number);
//	exp[i] += 100; //경험치 100추가

	lottotest=1000000; //이정도 로또 금액 까지 (그 이상은 누적되서 자동으로 늘어남)
   }
   lottonumber[i]=0;
  }
  Number=0;
  lottosay=0;
 }
}
