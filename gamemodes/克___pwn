//------------------------------------------------------------------------------------------------------

public OnPlayerStateChange(playerid, newstate, oldstate)
{
  new playername[MAX_PLAYER_NAME];
  GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
  
  if(IsPlayerInVehicle(playerid, 277))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 439))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 440))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  
  if(IsPlayerInVehicle(playerid, 492))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 493))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 494))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 495))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  if(IsPlayerInVehicle(playerid, 496))
  {
  SendClientMessage(playerid,COLOR_LAWNGREEN," + 당신은 피자 오토바이를 타셨습니다. 주기적으로 달러를 받습니다.");
  }
  
  
  if(newstate == PLAYER_STATE_DRIVER) {
  SetVehicleVirtualWorld(GetPlayerVehicleID(playerid),GetPlayerVirtualWorld(playerid));
  }
  if(newstate == PLAYER_STATE_PASSENGER) {
	SetVehicleVirtualWorld(GetPlayerVehicleID(playerid),GetVehicleVirtualWorld(GetPlayerVehicleID(playerid)));
  }
  if(event_value == 0 && event_flag == 1 && playerid == runaway_ID) {
	  if(GetVehicleType(GetPlayerVehicleID(playerid)) == VT_AIR) {
		RemovePlayerFromVehicle(playerid);
		}
  }
if 	((newstate==PLAYER_STATE_ONFOOT)&&((PlayerIsDancing[playerid]==1)))
	    {
	    SendClientMessage(playerid,0xFFFF00AA," + 당신은 로우라이더 댄싱을 그만 두었습니다.");
	    PlayerIsDancing[playerid]=0;
	    }
if	(newstate==PLAYER_STATE_DRIVER)
	    {
		if	(GetVehicleModel(GetPlayerVehicleID(playerid)) == 525)
	        {
	        IsTowTrucker[playerid]=1;
	        TowTruckers++;
			SendClientMessage(playerid,0xFFFF00AA," + 당신은 다른 차를 견인할 수 있습니다. Ctrl 키로 견인이 가능합니다.");
		    }
		else
		    {
		    if	(TowTruckers>0)
		        {
		    	SendClientMessage(playerid,0xFFFF00AA," + /towme 를 입력하면 다른 사람이 당신의 차를 견인하러 올 것 입니다. 필요할때만 쓰세요.");
		    	}
		    }
	    }
	if 	((newstate==PLAYER_STATE_ONFOOT)&&(IsTowTrucker[playerid]==1))
	    {
	    IsTowTrucker[playerid]=0;
	    TowTruckers--;
	    }
  if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER) {
    if(gUseVText[playerid] == 1) {
	TextDrawHideForPlayer(playerid,gVText[playerid]);
	TextDrawDestroy(gVText[playerid]);
	}
	gVText[playerid] = TextDrawCreate(470.0,320.0,aVehicleNames[GetVehicleModel(GetPlayerVehicleID(playerid)) - 400]);
	TextDrawFont(gVText[playerid], 2);
//	TextDrawTextSize(gVText[playerid], 60.0, 10.0);
	TextDrawColor(gVText[playerid],  COLOR_VIOLET);
	TextDrawSetOutline(gVText[playerid], 1);
	TextDrawSetShadow(gVText[playerid], 0);
	//TextDrawSetProportional(gVText[playerid], true);
	TextDrawShowForPlayer(playerid,gVText[playerid]);
	gUseVText[playerid] = 1;
	gVTextNum[playerid] = 1;
	}
    if(newstate == PLAYER_STATE_DRIVER) {
        if(!isadmin(playerid) && adminzonekill == 1) {
			if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 432) {
			RemovePlayerFromVehicle(playerid);
			}
			if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 520) {
			RemovePlayerFromVehicle(playerid);
			}
			if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 425) {
			RemovePlayerFromVehicle(playerid);
			}
		}
		if(gastank[GetPlayerVehicleID(playerid)] == 0 && gassystem == 1) {
            SendClientMessage(playerid, COLOR_GREENYELLOW, " + 차에 기름이 없습니다.");
            SendClientMessage(playerid, COLOR_GREENYELLOW, " + 견인차를 불러서 차를 주유소 까지 끌고가거나");
            SendClientMessage(playerid, COLOR_GREENYELLOW, " + 자신의 차량인경우 주유소에서 차량을 소환해서 기름을 넣도록 하세요");
			gaslock[playerid] = 1;
			TogglePlayerControllable(playerid,0);
			lock[GetPlayerVehicleID(playerid)] = playerid;
		}
	}

  if(newstate == PLAYER_STATE_DRIVER && GetPlayerVehicleID(playerid) == 121) {
            if(MyJob[playerid] == JOB_SPEEDCAR) {
			GetPlayerName(playerid,playername,sizeof(playername));
			format(strings, sizeof(strings), " + %s 님이 속도 단속을 시작하셨습니다!", playername);
			SendClientMessageToAll(COLOR_GREENYELLOW, strings);
			SendClientMessageToAll(COLOR_GREENYELLOW, " + 160 Km/h 이상의 속도를 내다가 걸리면 벌금을 물게 됩니다.");
			HighwayOnMission[playerid] = 1;
			HighwayMission = 1;
			PatrolCountingUp = 1;
			patroler = playerid;
			} else {
			SendClientMessage(playerid, COLOR_GREENYELLOW, " + 당신은 단속반이 아닙니다.");
			RemovePlayerFromVehicle(playerid);
			}
	}
  if(oldstate == PLAYER_STATE_DRIVER && patroler == playerid) {
  		    HighwayOnMission[playerid] = 0;
			format(strings, sizeof(strings)," + %s 님께서 속도 단속을 그만 하셨습니다. 이제 속도 위반 해도 괜찮을꺼에요^^",playername);
			SendClientMessageToAll(COLOR_GREENYELLOW,strings);
			OutOfRanger[playerid]=0;
			HighwayMission = 0;

			patroler = -1;
			new colorchanging=udb_UserInt(loginid[playerid],"color");

			if(colorchanging == 1)SetPlayerColor(playerid, 0xA587DE0BA354);
			if(colorchanging == 2)SetPlayerColor(playerid, 0x9CDE7180);
			if(colorchanging == 3)SetPlayerColor(playerid, 0xFF99AADD);
			if(colorchanging == 4)SetPlayerColor(playerid, 0xA7105DEF);
			if(colorchanging == 5)SetPlayerColor(playerid, 0x33CCFFAA);
			if(colorchanging == 6)SetPlayerColor(playerid, 0x24FF0AB9);
			if(colorchanging == 7)SetPlayerColor(playerid, 0xDDDD2357);
			if(colorchanging == 8)SetPlayerColor(playerid, 0xE9370DFC);
			if(colorchanging == 9)SetPlayerColor(playerid, 0xFFFF00AA);
			if(colorchanging == 10)SetPlayerColor(playerid, 0x12900BBF);
			if(colorchanging == 11)SetPlayerColor(playerid, 0x63AFF00A);
			if(colorchanging == 12)SetPlayerColor(playerid, 0x800080AA);
			if(colorchanging == 13)SetPlayerColor(playerid, 0x99934EFA);
			if(colorchanging == 14)SetPlayerColor(playerid, 0xFF66FFAA);
			if(colorchanging == 15)SetPlayerColor(playerid, 0xFF9900AA);
			if(colorchanging == 16)SetPlayerColor(playerid, 0x00000000);
			if(colorchanging == 17)SetPlayerColor(playerid, 0x9955DEEE);
			if(colorchanging == 18)SetPlayerColor(playerid, 0xAAFFCC33);
			if(colorchanging == 19)SetPlayerColor(playerid, 0x46BBAA00);
			if(colorchanging == 20)SetPlayerColor(playerid, 0xABCDEF01);

	}


  if(oldstate == PLAYER_STATE_DRIVER) {
//    lock[savelockcar[playerid]] = -1;
    savelockcar[playerid] = -1;
    MedicalOfficerOnMission[playerid] = 0;
if(taxi[playerid][custom]>=0){
		SendClientMessage(playerid,RBLUE," + 택시에서 내렸으므로 택시 시스템이 종료됩니다.");
		SendClientMessage(taxi[playerid][driver],RBLUE," + 택시기사가 택시에서 내렸으므로 택시 시스템이 종료됩니다.");
		taxi[taxi[playerid][custom]][driver]=-1;
		taxi[playerid][custom]=-1;

	}
  }
  if(oldstate == PLAYER_STATE_PASSENGER) {
  	if(taxi[playerid][driver]>=0){
		SendClientMessage(playerid,RBLUE," + 택시에서 내렸으므로 택시 시스템이 종료됩니다.");
		SendClientMessage(taxi[playerid][custom],RBLUE," + 손님이 택시에서 내렸으므로 택시 시스템이 종료됩니다.");
		taxi[taxi[playerid][driver]][custom]=-1;
		taxi[playerid][driver]=-1;
	}
  }
  if(newstate == PLAYER_STATE_DRIVER && GetPlayerVehicleID(playerid) == 278) {
  			SendClientMessage(playerid,COLOR_IVORY," + 구급미션을 시작했습니다.");
			GetPlayerName(playerid,playername,sizeof(playername));
			format(strings, sizeof(strings)," + %s 님이 구급미션을 시작하셨습니다. 체력회복을 원하시면 %s 님을 불러주세요.",playername,playername);
			SendClientMessageToAll(COLOR_GREEN,strings);
			MedicalOfficerOnMission[playerid]=1;
			medicalreward[0]=2500;
			AmbuHealing[0]=playerid;
  }
  if(oldstate == PLAYER_STATE_DRIVER && MedicalOfficerOnMission[playerid] == 1) {
	GetPlayerName(playerid,playername,sizeof(playername));
	format(strings, sizeof(strings)," + %s 님은 구급 미션을 그만 두셨습니다.",playername);
	SendClientMessageToAll(COLOR_GREEN,strings);
	MedicalOfficerOnMission[playerid]=0;
	medicalreward[0]=5000;
	AmbuHealing[0]=999;
  }
  if(newstate == PLAYER_STATE_DRIVER && !ismadmin(playerid)) {
    if(lock[GetPlayerVehicleID(playerid)] != -1){
      if(lock[GetPlayerVehicleID(playerid)] == playerid) {
      } else {
        new Float:X, Float:Y, Float:Z;
        SendClientMessage(playerid,COLOR_YELLOW," + 차문이 잠겨 있습니다.");
        GetPlayerPos(playerid,X,Y,Z);
        CreateExplosion(Float:X+3,Float:Y+2,Float:Z,3,5);
        CreateExplosion(Float:X,Float:Y,Float:Z,1,5);
        Z = Z+0.5;
        Y = Y+0.5;
        X = X+0.5;
        SetPlayerPos(playerid,X,Y,Z);
        SetPlayerPos(playerid,X,Y,Z);
        SetPlayerPos(playerid,X,Y,Z);
      }
    } else {
	    lock[GetPlayerVehicleID(playerid)] = playerid;
	 }
  }
}
public OnPlayerConnect(playerid) //플레이어 접속
{
	new playername[MAX_PLAYER_NAME];
	new i;

    GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
 	
	SKIP_WEAPONCHECK[playerid]=true; // v1.2 - 접속시 체크를 중단합니다.
	SetPlayerColor(playerid,playerColors[playerid]);
	moneyhack[playerid] = 0;
	isgasup[playerid] = 0;
	tgasup[playerid] = 0;
	wp_hack[playerid] = 0;
    for(new l=0; l<MAX_PLAYERS; l++){
        IgnorePlayerlist[l][playerid] = 0;
    }
   	adminzone_check[playerid] = 0;
	adminzone_count[playerid] = 0;
	OLDHEALTH[playerid]=100.0;// 기존체력 초기화

    connected[playerid] = 4;
	jusicbuy[playerid] = 0;
	jusicbuy2[playerid] = 0;
	jusicbuy3[playerid] = 0;
	jusicbuy4[playerid] = 0;
	jusicbuy5[playerid] = 0;
	jusicbuy6[playerid] = 0;
	gaslock[playerid] = 0;
	
   	if(	HighwayOnMission[playerid] == 1){
	    HighwayMission = 0;
		HighwayOnMission[playerid]=0;
		patroler = -1;
	}
    vote[playerid] = 0;
    savelockcar[playerid] = -1;
    MedicalOfficerOnMission[playerid] = 0;
    killcount[playerid] = MAX_KILLCOUNT;
    chatkick[playerid] = 0;
	IS_PLAYER_SUICIDED[playerid]=false; //v1.1 - 수정.
	bounty[playerid] = 0;
	taxi[playerid][custom]=-1;
	taxi[playerid][driver]=-1;
    nickchange[playerid] = 0;
    madmin[playerid] = 0;
	ospawn[playerid] = 0;
	isJail[playerid] = 0;
	JailTime[playerid] = 0;
//	proget[playerid] = 0;
	boomcheck[playerid] = 0;
	booms[playerid] = 0;
	playerCheckpoint[playerid] = 999;
	GameTextForPlayer(playerid,"~r~Welcome to free Server",5000,5);
	SendClientMessage(playerid,COLOR_ECHO, " -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=");
	SendClientMessage(playerid,COLOR_ECHO, " Welcome to 경제 and 자유 서버 (모드출처 : 클지모드) (강성재)");
	SendClientMessage(playerid,COLOR_ECHO, " -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=");
	SendClientMessage(playerid,COLOR_RED, "  + 계정생성 : /reg {아이디} {비밀번호}");
	SendClientMessage(playerid,COLOR_RED, "  + 로그인 : /log {아이디} {비밀번호}");
	SendClientMessage(playerid,COLOR_RED, "  + register : /reg {ID} {Password}");
	SendClientMessage(playerid,COLOR_RED, "  + Login : /log {Your ID} {Password}");
	// SendClientMessage(playerid,COLOR_RED, "  + 계정생성 : /reg {아이디} {비밀번호}");
	// SendClientMessage(playerid,COLOR_RED, "  + Join : /reg {ID} {Password}");
	SendClientMessage(playerid,COLOR_SPRINGGREEN, "  + 로딩중 {VineWood 나올때} 에는 아무 동작도 하지마세요.");
	SendClientMessage(playerid,COLOR_ECHO, " -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=");
//	format( szSend, 256, "JOIN %i %s",playerid, playername);
//	GUIRCON_SendToAll( szSend );

//	crazyText[playerid] = TextDrawCreate(5.0, 431.0, "                        ~g~Crazy Server mode - ~w~By ~p~Crazy Master");
//	TextDrawUseBox(crazyText[playerid], true);
//	TextDrawShowForPlayer(playerid, crazyText[playerid]);

//	TextDrawDestroy(NameText[playerid]);

	bank[playerid] = 0;
	gActivePlayers[playerid]++;
	gLastGaveCash[playerid] = GetTickCount();
    cspeed[playerid] = 1;
    login_spawn_check[playerid] = 0;
    lock_bb[playerid] = 0;
    spawn_de[playerid] = 0;
    mspeed[playerid] = 0;
    testhack[playerid] = 0;
	loginflag[playerid] = 0;
	loginid[playerid] = "";
	loginpw[playerid] = "";
//	loginse[playerid] = 0;
	cashDel[playerid] = 0;
	cashtDel[playerid] = 0;
	god[playerid] = 0;
	madmin[playerid] = 0;
	mobile_state[playerid] = 0;
	mobile_call[playerid] = -1;
	mobile_rf[playerid] = 0;
	mobile_rfn[playerid] = 0;
	mobile_rfw[playerid] = 0;
	mobile_battery[playerid] = 0;
	TogglePlayerClock(playerid,1);
	SetPlayerTime(playerid,word_h,word_m); //싱글시간

	for(new h = 0; h < MAX_GANG_ZONE; h++) {
		gang_zone_player[playerid][h] = 0;
	}
	for(i=0; i<MAX_GET_CAR; i++) {
		if(lock[i] == playerid) {
			lock[i] = -1;
		}
	}
	return 1;
}
public OnRconCommand(cmd[])
{
	new command[255];
	format(command,sizeof(command)," + 서버 운영자 님의 말 : %s",cmd);

	for(new i=0; i<MAX_PLAYERS; i++)
	{
	    if(IsPlayerConnected(i)) {
	        if(isadmin(i)) {
				SendClientMessage(i, COLOR_SPRINGGREEN, command);
			}
		}
	}
}
//------------------------------------------------------------------------------------------------------
public OnPlayerDisconnect(playerid,reason)
{

  new i;
  new str[256],playername[MAX_PLAYER_NAME]; GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
  new reas[50];

  if(loginflag[playerid] == 1) {
	 loginsystem(playerid,1);
  }
  
  if(event_flag == 1 && event_value == 0 && runaway_ID == playerid)
  {
  SendClientMessageToAll(COLOR_ORANGE, " + 타겟 ID의 접속이 종료되어 이벤트를 종료합니다. 지금부터 PK 금지!");
  event_flag = 0;
  event_value = random(3);
  murder_flag = 0;
  run_time = RUNAWAY_TIME;
  for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
  player_cp[i2] = 0;
  }
  }

  PlayerLeaveoz(playerid);
  player_stop[playerid] = 0;
  
  	if(gara[playerid] == 1) {
	new racedowncheck;
	racedowncheck = 0;
		for(i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 1) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted = 0;
		posti = 1;
		status = 1;
		racestarted = 0;
		for(i = 0;i < MAX_PLAYERS;i++) {
		racecheck[i] = 0;
			if(gara[i] == 1) {
			gara[i] = 0;
			}

		}
		SendClientMessageToAll(COLOR_BLUE, " + 산피에로 길거리 레이싱이 끝났습니다.");
		KillTimer(ResetRace);
		}
	}
	if(gara[playerid] == 2) {
	new racedowncheck;
	racedowncheck = 0;
		for(i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 2) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
  		racestarted2 = 0;
		posti2 = 1;
		status2 = 1;
		racestarted2 = 0;
		for(i = 0;i < MAX_PLAYERS;i++) {
		racecheck2[i] = 0;
			if(gara[i] == 2) {
			gara[i] = 0;
			}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 산피에로 다운힐이 끝났습니다.");
		KillTimer(ResetRace2);
		}
	}
	if(gara[playerid] == 3) {
	new racedowncheck;
	racedowncheck = 0;
		for(i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 3) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted3 = 0;
		posti3 = 1;
		status3 = 1;
		racestarted3 = 0;
		for(i = 0;i < MAX_PLAYERS;i++) {
		racecheck3[i] = 0;
		if(gara[i] == 3) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 빅이어 드리프트가 끝났습니다.");
		KillTimer(ResetRace3);
		}
	}
	if(gara[playerid] == 4) {
	new racedowncheck;
	racedowncheck = 0;
		for(i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 4) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted4 = 0;
		posti4 = 1;
		status4 = 1;
		racestarted4 = 0;
		for(i = 0;i < MAX_PLAYERS;i++) {
		racecheck4[i] = 0;
		if(gara[i] == 4) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 외딴마을 드래그가 끝났습니다.");
		KillTimer(ResetRace4);
		}
	}
   	gara[playerid] = 0;
 	racecheck[playerid] = 0;
 	
    BankOpen[playerid] = 0;
  	BankPass[playerid] = 0;
  	
  	eaglematchplayers[playerid] = 0;
	chainmatchplayer[playerid] = 0;
	chainmatchplayers[playerid] = 0;
	minimatchplayer[playerid] = 0;
	minimatchplayers[playerid] = 0;
	uzimatchplayer[playerid] = 0;
	uzimatchplayers[playerid] = 0;
	
if(PlayerBoxing[playerid] > 0)
	{
	    if(Boxer1 == playerid)
	    {
	        if(IsPlayerConnected(Boxer2))
	        {
	        	PlayerBoxing[Boxer2] = 0;
	        	SetPlayerPos(Boxer2, 765.8433,3.2924,1000.7186);
	        	SetPlayerInterior(Boxer2, 5);
	        	SendClientMessageToAll(COLOR_LIME, " + 상대선수가 나가셔서 경기는 종료 되었습니다.");
			}
	    }
	    else if(Boxer2 == playerid)
	    {
	        if(IsPlayerConnected(Boxer1))
	        {
	        	PlayerBoxing[Boxer1] = 0;
	        	SetPlayerPos(Boxer1, 765.8433,3.2924,1000.7186);
	        	SetPlayerInterior(Boxer1, 5);
	        	SendClientMessageToAll(COLOR_LIME, " + 상대선수가 나가셔서 경기는 종료 되었습니다.");
			}
	    }

	    InRing = 0;
     	RoundStarted = 0;
		Boxer1 = 255;
		Boxer2 = 255;
		TBoxer = 255;
	}
	BoxWaitTime[playerid] = 0;
	BoxOffer[playerid] = 999;
	PlayerBoxing[playerid] = 0;
	PlayerInfo[playerid][pBoxSkill] = 0;
	
	SpawnSet[playerid] = 0;
	PlayerIsDancing[playerid]=0;
 	score[playerid]=0;
 	Waring[playerid] = 0;
 	
  if(IsTowTrucker[playerid]==1)
  {
  IsTowTrucker[playerid]=0;
  TowTruckers--;
  }
  
  for(new h = 0; h < MAX_GANG_ZONE; h++) {
	gang_zone_player[playerid][h] = 0;
  }

  	bokbuy[playerid] = 0;
	exp[playerid] = 0;
  	quest[playerid] = 0;
  	if(gUseVText[playerid] == 1) {
		TextDrawHideForPlayer(playerid,gVText[playerid]);
	  	TextDrawDestroy(gVText[playerid]);
		gUseVText[playerid] = 0;
		gVTextNum[playerid] = 0;
	}

  if(reason == 0) reas = "오류보고로 팅김";
  if(reason == 1) reas = "종료";
  if(reason == 2) reas = "킥&밴";
	SKIP_WEAPONCHECK[playerid]=false;// v1.2 - 접속시 체크를 중단합니다.
	OLDHEALTH[playerid]=0.0;// 기존체력 초기화

  //tgastank = 1; //차량기름 그대로 둠
  FishTime[playerid] = 0;
  rampOff[playerid] = 0;
  moneyhack[playerid] = 0;
//  format( szSend, 256, "PART %i %i", playerid, reason );
//  GUIRCON_SendToAll( szSend );
	if(loginflag[playerid] == 1) {
  format(str, sizeof(str), " + %s(%d) 님이 퇴장 하셨습니다. (로그인ID : %s) (이유: %s)", playername ,playerid,loginid[playerid],reas);
  SendClientMessageToAll(COLOR_MEDIUMAQUA, str);


  }
  else
  {
  format(str, sizeof(str), " + %s(%d) 님이 퇴장 하셨습니다. (이유: %s)", playername ,playerid,reas);
  SendClientMessageToAll(COLOR_MEDIUMAQUA, str);
}
/*
loginsystem(playerid,1);
new loginstr[255];
format(loginstr,sizeof(loginstr)," + 현재 돈 : %d",moneyhack[playerid]);
SendClientMessage(playerid,COLOR_GREEN,loginstr);
SendClientMessage(playerid,COLOR_GREEN," + 정상적으로 저장되었습니다.");
printf(" + %d번 님의 정보가 저장되었습니다.",playerid);
*/




  gaslock[playerid] = 0;
  
  for(i=0; i < MAX_PLAYERS; i++) {
    if(salary[playerid][i] >= 1) {
    salary[playerid][i] = 0;
    format(str, sizeof(str), " + %s(%d)님이 나가셔서 그 분에게 월급을 받지 않습니다.", playername ,playerid);
   	SendClientMessage(i,COLOR_IVORY, str);
   	}
  }
  for(i=0; i < MAX_PLAYERS; i++) {
    if(salary[i][playerid] >= 1) {
      format(str, sizeof(str), " + %s(%d)님이 나가셔서 그 분에게 월급을 지급하지 않습니다.", playername ,playerid);
	  SendClientMessage(i,COLOR_IVORY, str);
	  salary[i][playerid] = 0;
	}
  }
  if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid)==PLAYER_STATE_DRIVER) {
    lock[GetPlayerVehicleID(playerid)]=-1;
  }
  for(i=0; i<MAX_VEHICLES; i++) {
	if(lock[i] == playerid) {
	  lock[i] = -1;
	}
  }
	testhack[playerid] = 0;
/*  if(reason == 0) {
	udb_UserSetFloat(loginid[playerid],"X",0);
	udb_UserSetFloat(loginid[playerid],"Y",0);
	udb_UserSetFloat(loginid[playerid],"Z",0);
	udb_UserSetInt(loginid[playerid],"Int",0);
  } */
    login_spawn_check[playerid] = 0;
    MyJob[playerid] = 0;
    lock_bb[playerid] = 0;
    spawn_de[playerid] = 0;
	PlayerInfo[playerid][pFishSkill] = 0;
	PlayerInfo[playerid][pBiggestFish] = 0;
	PlayerInfo[playerid][pCookSkill] = 0;
	PlayerInfo[playerid][pWins] = 0;
    PlayerInfo[playerid][pLoses] = 0;
	ClearCooking(playerid);
	ClearGroceries(playerid);

  	TestFishes[playerid] = 0; FishCount[playerid] = 0; Fishes[playerid][pLastFish] = 0; Fishes[playerid][pFishID] = 0;
  	Fishes[playerid][pLastWeight] = 0; Fishes[playerid][pFishID] = 0; Fishes[playerid][pLastFish] = 0;
 	Fishes[playerid][pFid1] = 0; Fishes[playerid][pFid2] = 0; Fishes[playerid][pFid3] = 0; Fishes[playerid][pFid4] = 0; Fishes[playerid][pFid5] = 0;
 	Fishes[playerid][pWeight1] = 0; Fishes[playerid][pWeight2] = 0; Fishes[playerid][pWeight3] = 0;
 	Fishes[playerid][pWeight4] = 0; Fishes[playerid][pWeight5] = 0;
 	Fishes[playerid][pFish1] = 0; Fishes[playerid][pFish2] = 0; Fishes[playerid][pFish3] = 0; Fishes[playerid][pFish4] = 0; Fishes[playerid][pFish5] = 0;
  	ClearFishes(playerid);

  	CarSprayX[i] = 0;
	CarSprayY[i] = 0;
	CarSprayZ[i] = 0;
	CarSprayVehicleid[i] = 0;
	CarSprayCountdown[i] = 0;
	payandspray[i] = 0;
	askedpayandspray[i] = 0;
	readypayandspray[i] = 0;

  	stunt1[playerid] = 0;
	stunt2[playerid] = 0;

  	InAndrom[playerid]=0;
  	InShamal[playerid]=0;
  	boomcheck[playerid]=0;
  	booms[playerid] = 0;
  	Chairlifter1[playerid] = 0;

	if(mobile_state[playerid] == 4) {
		if( call_data[mobile_call[playerid]][cstate] == 1 ) {
			for(i=0; i < MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i)) {
					if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
						mobile_call[i] = -1;
						mobile_state[i] = 3;
					}
				}
			}
			mobile_state[playerid] = 3;
			call_data[mobile_call[playerid]][use] = false;
			call_data[mobile_call[playerid]][cid] = -1;
			call_data[mobile_call[playerid]][cstate] = 0;
			call_data[mobile_call[playerid]][time] = 0;
			call_data[mobile_call[playerid]][time2] = 0;
			call_data[mobile_call[playerid]][check] = false;
			SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
			mobile_call[playerid] = -1;
		} else if(call_data[mobile_call[playerid]][cstate] == 2) {
			for(i=0; i < MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i)) {
					if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
						SendClientMessage(i,COLOR_RED," + 통화가 종료되었습니다.");
						mobile_call[i] = -1;
						mobile_state[i] = 3;
						SetPlayerSpecialAction(i,SPECIAL_ACTION_STOPUSECELLPHONE);
					}
				}
			}
			mobile_state[playerid] = 3;
			SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
//			new money;
//			money = 15*call_data[mobile_call[playerid]][time];
//			GiveMoney(call_data[mobile_call[playerid]][cid],-money);
//					new str[255];
//					format(str,sizeof(str)," + 통화 요금으로 $%d 달러 를 지불 하셨 습니다. - CTF -",money);
//					SendClientMessage(call_data[mobile_call[playerid]][cid],COLOR_RED,str);
			call_data[mobile_call[playerid]][use] = false;
			call_data[mobile_call[playerid]][check] = false;
			call_data[mobile_call[playerid]][cstate] = 0;
			call_data[mobile_call[playerid]][time] = 0;
			call_data[mobile_call[playerid]][time2] = 0;
			call_data[mobile_call[playerid]][cid] = -1;
			mobile_call[playerid] = -1;
		}
	}
	mobile_state[playerid] = 0;
	mobile_call[playerid] = -1;
	mobile_rf[playerid] = 0;
	mobile_rfn[playerid] = 0;
	mobile_rfw[playerid] = 0;
	mobile_battery[playerid] = 0;
	Chairliftcounter1[playerid] = 0;
	Chairlifter2[playerid] = 0;
	Chairliftcounter2[playerid] = 0;
	wp_hack[playerid] = 0;
  	gActivePlayers[playerid]--;
	chatkick[playerid] = 0;
	bounty[playerid] = 0;
	taxi[playerid][custom]=-1;
	taxi[playerid][driver]=-1;
	nickchange[playerid] = 0;
	madmin[playerid] = 0;
	god[playerid] = 0;
	ospawn[playerid] = 0;
	isJail[playerid] = 0;
	JailTime[playerid] = 0;
//	proget[playerid] = 0;
	isgasup[playerid] = 0;
	tgasup[playerid] = 0;
	playerCheckpoint[playerid] = 999;

}
//------------------------------------------------------------------------------------------------------

dcmd_eaglematch(playerid,params[])  {
	#pragma unused params
	new playername[MAX_PLAYER_NAME];
		if(isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 감옥에서는 할 수 없습니다.");
		    return 1;
		}
		if(eaglematchinprogress == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 그 데스매치는 시작되었습니다.");
			return 1;
		}
		if(minimatchplayer[playerid] == 1 || chainmatchplayer[playerid] == 1 || eaglematchplayers[playerid] == 1 || uzimatchplayer[playerid] == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가했습니다!");
			return 1;
		}
	 	if(eaglematchinprogress == 0){
	        if(playerseaglematch < 2){
	            if(playerseaglematch == 0){
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					GiveMoney(playerid, -10000);
					eaglecountdownseconds = 30;
					GetPlayerName(playerid,playername,sizeof(playername));
					format(strings, sizeof(strings), " %s 님이 데져트이글 데스매치에 참가하셨습니다. 참가하고 싶으신 분은 /eaglematch 를 쳐주세요.", playername);
					SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
					eaglematch[0]=1;
					eaglematchstarted=1;
					eaglematchplayers[playerid]=1;
					playerseaglematch=1;
					return 1;
				} else {
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					GiveMoney(playerid, -10000);
					eaglematch[1]=1;
					eaglematchplayers[playerid]=1;
					playerseaglematch=2;
					format(strings, sizeof(strings), " 데스매치가 시작될려면 %d초 남았으니 기다려주십시오.", eaglecountdownseconds);
					SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
				}
			} else {
				SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 인원이 다 채워져서 못합니다.");
			}
		} else {
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가하셨습니다. 잠시만 기다려 주십시오.");
		}
	return 1;
}

dcmd_chainmatch(playerid,params[])  {
	#pragma unused params
	new playername[MAX_PLAYER_NAME];
		if(isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 감옥에서는 할 수 없습니다.");
		    return 1;
		}
		if(chainmatchinprogress == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 그 데스매치는 시작되었습니다.");
			return 1;
		}
		if(minimatchplayer[playerid] == 1 || chainmatchplayer[playerid] == 1 || eaglematchplayers[playerid] == 1 || uzimatchplayer[playerid] == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가했습니다!");
			return 1;
		}
	    if(chainmatchinprogress == 0){
	        if(playerschainmatch<4){
	            if(playerschainmatch==0){
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					GiveMoney(playerid, -10000);
					GetPlayerName(playerid,playername,sizeof(playername));
					format(strings, sizeof(strings), " %s 님이 전기톱 데스매치에 참가하셨습니다. 참가하고 싶으신 분은 /chainmatch 를 쳐주세요.", playername);
					SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
					chainmatch[0]=1;
					chainmatch[1]=0;
					chainmatch[2]=0;
					chainmatch[3]=0;
					chaincountdownseconds = 30;
					chainmatchstarted=1;
					playerschainmatch=1;
					chainmatchprice=10000;
					chainmatchplayers[playerid]=1;
					return 1;
				} else {
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					for(new i=0;i<4;i++){
					    if(chainmatch[i] == 0){
							GiveMoney(playerid, -10000);
							chainmatchprice=chainmatchprice+10000;
							chainmatch[i]=1;
							playerschainmatch++;
							chainmatchplayers[playerid]=1;
							chainmatchprice=chainmatchprice+10000;
							return 1;
						}
					}
				}
			} else {
				SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 인원이 다 채워져서 못합니다.");
			}
		} else {
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가하셨습니다. 잠시만 기다려 주십시오.");
		}
	return 1;
}

dcmd_minimatch(playerid,params[])  {
	#pragma unused params
	new playername[MAX_PLAYER_NAME];
		if(isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 감옥에서는 할 수 없습니다.");
		    return 1;
		}
		if(minimatchinprogress == 1){
				SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 그 데스매치는 시작되었습니다.");
			return 1;
		}
		if(minimatchplayer[playerid] == 1 || chainmatchplayer[playerid] == 1 || eaglematchplayers[playerid] == 1 || uzimatchplayer[playerid] == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가했습니다!");
			return 1;
		}
	    if(minimatchinprogress == 0){
	        if(playersminimatch<6){
	            if(playersminimatch==0){
					if(GetPlayerMoney(playerid)<25000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					GiveMoney(playerid, -25000);
					GetPlayerName(playerid,playername,sizeof(playername));
					format(strings, sizeof(strings), " %s 님이 미니건 데스매치에 참가하셨습니다. 참가하고 싶으신 분은 /minimatch 를 쳐주세요.", playername);
					SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
					minimatch[0]=1;
					minimatch[1]=0;
					minimatch[2]=0;
					minimatch[3]=0;
					minimatch[4]=0;
					minimatch[5]=0;
					minicountdownseconds = 30;
					minimatchstarted=1;
					playersminimatch=1;
					minimatchprice=25000;
					minimatchplayers[playerid]=1;
					return 1;
				} else {
					if(GetPlayerMoney(playerid)<25000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					for(new i=0;i<6;i++){
			    		if(minimatch[i] == 0){
							GiveMoney(playerid, -25000);
							minimatch[i]=1;
							playersminimatch++;
							minimatchplayers[playerid]=1;
							minimatchprice=minimatchprice+25000;
							return 1;
						}
					}
				}
			} else {
				SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 인원이 다 채워져서 못합니다.");
			}
		} else {
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가하셨습니다. 잠시만 기다려 주십시오.");
		}
	return 1;
}

dcmd_uzimatch(playerid,params[])  {
	#pragma unused params
	new playername[MAX_PLAYER_NAME];
		if(isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 감옥에서는 할 수 없습니다.");
		    return 1;
		}
		if(uzimatchinprogress == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 그 데스매치는 시작되었습니다.");
		}
		if(minimatchplayer[playerid] == 1 || chainmatchplayer[playerid] == 1 || eaglematchplayers[playerid] == 1 || uzimatchplayer[playerid] == 1){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가했습니다!");
			return 1;
		}
	    if(uzimatchinprogress == 0){
	        if(playersuzimatch<6){
	            if(playersuzimatch==0){
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					GiveMoney(playerid, -10000);
					GetPlayerName(playerid,playername,sizeof(playername));
					format(strings, sizeof(strings), " %s 님이 우지 데스매치에 참가하셨습니다. 참가하고 싶으신 분은 /uzimatch 를 쳐주세요.", playername);
					SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
					uzimatch[0]=1;
					uzimatch[1]=0;
					uzimatch[2]=0;
					uzimatch[3]=0;
					uzimatch[4]=0;
					uzimatch[5]=0;
					uzicountdownseconds = 30;
					uzimatchstarted=1;
					playersuzimatch=1;
					uzimatchprice=10000;
					uzimatchplayers[playerid]=1;
					return 1;
				} else {
					if(GetPlayerMoney(playerid)<10000){
						SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 돈이 부족합니다.");
						return 1;
					}
					for(new i=0;i<6;i++){
			    		if(uzimatch[i] == 0){
							GiveMoney(playerid, -10000);
							uzimatch[i]=1;
							playersuzimatch++;
							uzimatchplayers[playerid]=1;
							uzimatchprice=uzimatchprice+10000;
							return 1;
						}
					}
				}
			} else {
				SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 이미 인원이 다 채워져서 못합니다.");
			}
		} else {
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 이미 참가하셨습니다. 잠시만 기다려 주십시오.");
		}
	return 1;
}
dcmd_spray(playerid,params[])  {
	new playername[24];
	#pragma unused params
	if(loginflag[playerid] == 1){
		if(payandspray[playerid] != 0){
			format (strings, sizeof(strings), " + %d분 후에 다시 명령어를 입력해 주시기 바랍니다.",payandspray[playerid]);
			SendClientMessage(playerid, COLOR_SYSTEM_GM , strings);
			return 1;
		}
		if(GetMoney(playerid)<25000){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 $25000 가 필요합니다.");
			return 1;
		}
		if(!IsPlayerInAnyVehicle(playerid)){
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 당신은 차에 타고 있지 않습니다.");
			return 1;
		}
		if(IsPlayerDriver(playerid)){
			GetPlayerName(playerid,playername,sizeof(playername));
            CarSprayCountdown[playerid]=3;
            payandspray[playerid]=1;
			askedpayandspray[playerid]=1;
			GiveMoney(playerid, -25000);
			GetPlayerName(playerid,playername,sizeof(playername));
			format(strings, sizeof(strings), " + %s 님은 $25000 를 내고 도색을 하였습니다.", playername);
			SendClientMessageToAll(COLOR_SYSTEM_PM, strings);
		} else {
			SendClientMessage(playerid, COLOR_SYSTEM_GM , " + 운전석에 타고 계셔야 합니다.");
		}
	}
	return 1;
}

dcmd_liftme(playerid,params[]){
	#pragma unused params
	new playername[MAX_PLAYER_NAME];
	if(loginflag[playerid] == 1){
	    if(IsPlayerInCheckpoint(playerid) == 0){
	        SendClientMessage(playerid, COLOR_SYSTEM_PW, " + 산 피에로 대교의 체크포인트에서 할 수 있습니다.");
			return 1;
		}
		if(Chairlifter1[playerid] == 1){
	        SendClientMessage(playerid, COLOR_SYSTEM_PW, " + 당신은 이미 금문교로 가고 있습니다.");
			return 1;
		}
		if(getCheckpointType(playerid) == CP_LIFTMECHIL){
			GetPlayerName(playerid,playername, sizeof(playername));
			format(strings, sizeof(strings)," + %s 님은 바위산 입구에서 바위산 정상으로 갑니다~",playername);
			SendClientMessageToAll(COLOR_SYSTEM_GM , strings);
			SendClientMessage(playerid,COLOR_SYSTEM_PM, " + 잠시만 기다려 주십시오 가고 있습니다.");
			Chairliftcounter1[playerid] = 0;
			Chairlifter1[playerid] = 1;
			ChairliftDelay[playerid] = 0;
			return 1;
		}
		if(getCheckpointType(playerid) == CP_LIFTMEGATE){
			GetPlayerName(playerid,playername, sizeof(playername));
			format(strings, sizeof(strings)," + %s 님은 산 피에로 대교에서 금문교로 갑니다~",playername);
			SendClientMessageToAll(COLOR_SYSTEM_GM , strings);
			SendClientMessage(playerid,COLOR_SYSTEM_PM, " + 잠시만 기다려 주십시오 가고 있습니다.");
			Chairliftcounter2[playerid] = 0;
			Chairlifter2[playerid] = 1;
			ChairliftDelay[playerid] = 0;
			return 1;
		}
        SendClientMessage(playerid, COLOR_SYSTEM_PW, " + 리프트용 체크포인트 안에서 가능합니다.");
        SendClientMessage(playerid, COLOR_SYSTEM_PW, " + 바위산 입구와, 산 피에로 대교에 있습니다.");
	}
	return 1;
}


dcmd_carcolor(playerid,params[]) {
	if(isadmin(playerid)) {
	    new tmp[256],tmp2[256],Index; tmp = strtok(params,Index), tmp2 = strtok(params,Index);
	    if(!strlen(tmp)||!(strval(tmp) >= 0 && strval(tmp) <= 126)||!IsNumeric(tmp)||!IsNumeric(tmp2)) return SendClientMessage(playerid,red,"Syntax Error: \"/CARCOLOR <COLOR 1> (<COLOR 2>)\".");
		if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid,red," + 차에 타셔야죠........");
		if(!strlen(tmp2)) tmp2 = tmp;
		new string[256],name[24]; GetPlayerName(playerid,name,24);
		format(string,256," + 차 색깔이 바뀜: [Color 1: %d || Color 2: %d]",strval(tmp),strval(tmp2));
		SendClientMessage(playerid,COLOR_RED,string);
		return ChangeVehicleColor(GetPlayerVehicleID(playerid),strval(tmp),strval(tmp2));
	} else return SendClientMessage(playerid,red," + 당신은 어드민이 아니여잉~");
}

public OnPlayerCommandText(playerid, cmdtext[]) //명령어 입력
{
//	cuteascii(cmdtext);

	new string[256];
	new playermoney;
	new sendername[MAX_PLAYER_NAME];
	new giveplayer[MAX_PLAYER_NAME];
	new cmd[256];
	new giveplayerid, moneys, idx;
	new tmp[256];
	new weaponid;
	new giveplayername[MAX_PLAYER_NAME];
	new thisplayer[MAX_PLAYER_NAME];
	new dancestyle;
	new name[MAX_PLAYER_NAME];
	new playername[MAX_PLAYER_NAME];
	new player_num;

	cmd = strtok(cmdtext, idx);
	player_stop[playerid] = 0;
	if(loginflag[playerid] == 0) {
	    if(strcmp( stringslice(cmdtext,0), "/reg", true)==0 || strcmp( stringslice(cmdtext,0), "/log", true)==0 || strcmp(cmdtext, "/logincom", true) == 0) {
	    } else {
	  		SendClientMessage(playerid, COLOR_GREEN, " + 로그인이 필요합니다.");
		  	SendClientMessage(playerid, COLOR_GREEN, " + 로그인 도움말을 보려면 /logincom 을 참고해주세요");
			return 1;
	    }
	}
	GetPlayerName(playerid,name,MAX_PLAYER_NAME);
	format(strings, sizeof(strings), " + 명령어사용 [%d] %s : %s ",playerid,name,cmdtext);
	printf(strings);
	//위에꺼 도스창에 보임
	
	if (strcmp(cmd, "/noevent", true) == 0)
	{
	if(event_value==0)
	{
	noevent[playerid] = 1;
	SendClientMessage(playerid, COLOR_ORANGE, " + 당신은 이번 RUNAWAY 이벤트 대상에서 제외됩니다.");
	return 1;
	}
	else
	{
	SendClientMessage(playerid, COLOR_ORANGE, " + RUNAWAY 이벤트 중에만 사용할 수 있는 명령어입니다.");
	return 1;
	}
	}

	if (strcmp("/event", cmd) == 0)
	{
	if (ismadmin(playerid))
	{
	if(event == 0)
	{
	SendClientMessage(playerid, 0xFF0000AA, " + 이벤트(Eric)를 활성화 시킵니다.");
	event = 1;
	}
	else if(event == 1)
	{
	SendClientMessage(playerid, 0xFF0000AA, " + 이벤트(Eric)를 비활성화 시킵니다.");
	event = 0;
	}
	}
	else SendClientMessage(playerid, 0xFF0000AA, " + 당신은 어드민이 아닙니다.");
	return 1;
	}

	if (strcmp("/goevent", cmd) == 0)
	{
	new iTime;
	if (ismadmin(playerid))
	{
	if(event == 0)
	{
	SendClientMessage(playerid, 0xFF0000AA, " + 현재 이벤트 스위치가 꺼져있습니다.");
	return 1;
	}
	else if(event_flag == 1)
	{
	SendClientMessage(playerid, 0xFF0000AA, " + 현재 진행중인 이벤트가 있습니다.");
	return 1;
	}
	tmp = strtok(cmdtext, idx);
	if(!strlen(tmp) || !IsNumeric(tmp))
	{
	SendClientMessage(playerid, 0xFF0000AA, " + USAGE : /goevent [num]  0: runaway, 1:바위산등산, 2: 드카 레이싱");
	return 1;
	}
	iTime = strval(tmp);
	if(iTime < 0 || iTime > 2)
	{
	SendClientMessage(playerid, 0xFF0000AA, " + USAGE : /goevent [num]  0: runaway, 1:바위산등산, 2:드카 레이싱");
	return 1;
	}
	SendClientMessage(playerid, 0xFF0000AA, " + 이벤트 OK!");
	if(iTime == 0)
	{
	for(new i=0 ; i<MAX_PLAYERS ; i++)
	noevent[i] = 0;
	}
	run_time = 65;
	event_value = iTime;
	return 1;
	}
	else SendClientMessage(playerid, 0xFF0000AA, " + 당신은 어드민이 아닙니다.");
	return 1;
	}
	
	if(strcmp(cmd, "/oz", true) == 0) {
		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, "USAGE: /oz [create/join/invite/quit/kick/tele/rtele] [name/number]");
			SendClientMessage(playerid, COLOR_YELLOW,"조직 도움말: /ozcom");
			return 1;
		}
		giveplayerid = strval(tmp);

		if(strcmp(tmp, "create", true)==0)
		    ozcmd = 1;
		else if(strcmp(tmp, "invite", true)==0)
		    ozcmd = 2;
		else if(strcmp(tmp, "kick", true)==0)
		    ozcmd = 3;
		else if(strcmp(tmp, "join", true)==0)
		    ozcmd = 4;
		else if(strcmp(tmp, "quit", true)==0)
		    ozcmd = 5;
		else if(strcmp(tmp, "tele", true)==0)
		    ozcmd = 6;
		else if(strcmp(tmp, "rtele", true)==0)
		    ozcmd = 7;

		tmp = strtok(cmdtext, idx);
		if(ozcmd < 4 && !strlen(tmp)) {
		    if(ozcmd==0)
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /oz [create/join/invite/kick/quit] [name/number]");
			else if(ozcmd==1)
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /oz create [name]");
			else if(ozcmd==2)
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /oz invite [playerID]");
			else if(ozcmd==3)
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /oz kick [playerID]");
			return 1;
		}

		//Create oz//
		if(ozcmd==1) {
		    if(playeroz[playerid]>0) {
				SendClientMessage(playerid, COLOR_RED, " + 당신은 이미 조직에 있습니다.");
				return 1;
		    }
		    	for(new i = 1; i < MAX_OZS; i++) {
				if(ozInfo[i][0]==0) {
				    //name oz
					format(ozNames[i], MAX_OZ_NAME, "%s", tmp);
					//oz exists
					ozInfo[i][0]=1;
					//There is one member
					ozInfo[i][1]=1;
					//oz color is player's color
					ozInfo[i][2]=GetPlayerColor(playerid);
					//Player is the first oz member
					ozMembers[i][0] = playerid;
					format(string, sizeof(string)," + 당신은 조직 '%s' (id: %d)을 만들었습니다.", ozNames[i], i);
					SendClientMessage(playerid, COLOR_GREEN, string);
    				playeroz[playerid] = i;

					return 1;
				}
			}
			return 1;
		//Invite to oz//
		} else if (ozcmd==2) {
	 		giveplayerid = strval(tmp);
			if(playeroz[playerid]==0) {
				SendClientMessage(playerid, COLOR_RED, " + 당신은 아무 조직도 아닙니다.");
				return 1;
			}
			if(ozMembers[playeroz[playerid]][0]!=playerid) {
				SendClientMessage(playerid, COLOR_RED, " + 조직의 두목만이 다른 플레이어의 초대가 가능합니다.");
				return 1;
			}
			if(IsPlayerConnected(giveplayerid)) {
				if(playeroz[giveplayerid] == playeroz[playerid]) {
					SendClientMessage(playerid, COLOR_RED, " + 그는 이미 당신의 조직원입니다.");
					return 1;
				}
				if(playeroz[giveplayerid] != 0) {
					SendClientMessage(playerid, COLOR_RED, " + 그는 이미 다른 조직에 가입되있습니다.");
					return 1;
				}
				GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
				GetPlayerName(playerid, sendername, sizeof(sendername));
				format(string, sizeof(string)," + 당신은 %s 에게 조직초대메시지를 보냈습니다.", giveplayer);
				SendClientMessage(playerid, COLOR_GREEN, string);
				format(string, sizeof(string)," + 당신은 %s 로부터  '%s' (id: %d)조직의 초대요청을 받았습니다.", sendername, ozNames[playeroz[playerid]],playeroz[playerid]);
				SendClientMessage(giveplayerid, COLOR_GREEN, string);

				ozInvite[giveplayerid]=playeroz[playerid];

			} else SendClientMessage(playerid, COLOR_RED, " + 존재하지 않는 플레이어입니다.");
			return 1;

		//Kick from oz//
		} else if (ozcmd==3) {
			giveplayerid = strval(tmp);
			if(ozMembers[playeroz[playerid]][0] != playerid) {
				SendClientMessage(playerid, COLOR_RED, " + 조직의 두목만이 조직원의 추방이 가능합니다.");
				return 1;
			}
			if(playeroz[playerid] != playeroz[giveplayerid]) {
				SendClientMessage(playerid, COLOR_RED, " + 그는 당신의 조직원이 아닙니다.");
				return 1;
			}
			GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
			format(string, sizeof(string), " + %s (ID : %d) 를 조직에서 추방했습니다. ", giveplayer, giveplayerid);
			SendClientMessage(playerid, COLOR_GREEN, string);
			PlayerLeaveoz(giveplayerid);
			return 1;

		//Join oz//
		} else if (ozcmd==4) {
	 		oznum = ozInvite[playerid];

		    if(playeroz[playerid]>0) {
				SendClientMessage(playerid, COLOR_RED, " + 당신은 이미 조직에 있습니다.");
				return 1;
		    }
	 		if(ozInvite[playerid]==0) {
				SendClientMessage(playerid, COLOR_RED, " + 조직에 들어가기 위해서 조직의 초대가 있어야 합니다.");
				return 1;
			}
			if(ozInfo[oznum][0]==0) {
				SendClientMessage(playerid, COLOR_RED, " + 존재하지 않는 조직입니다.");
				return 1;
			}

			if(ozInfo[oznum][1] < MAX_OZ_MEMBERS) {
			    new i;
			    i = ozInfo[oznum][1];

				ozInvite[playerid]=0;

				ozMembers[oznum][i] = playerid;

			    	GetPlayerName(playerid, sendername, MAX_PLAYER_NAME);
				format(string, sizeof(string)," + %s 님이 당신의 조직에 들어왔습니다.", sendername);
				for(new j = 0; j < ozInfo[oznum][1]; j++)
					SendClientMessage(ozMembers[oznum][j], COLOR_ORANGE, string);
				ozInfo[oznum][1]++;
				playeroz[playerid] = oznum;
				SetPlayerColor(playerid,ozInfo[oznum][2]);
				format(string, sizeof(string)," + 당신은 '%s' (id: %d) 조직에 가입했습니다.", ozNames[oznum], oznum);
				SendClientMessage(playerid, COLOR_GREEN, string);
				return 1;
			}

			SendClientMessage(playerid, COLOR_RED, " + 조직의 인원이 가득 찼습니다.");
			return 1;

		//Leave oz//
		} else if (ozcmd==5) {
		    PlayerLeaveoz(playerid);
		} else if (ozcmd==6) {
			if(playeroz[playerid]==0) {
				SendClientMessage(playerid, COLOR_RED, " + 당신은 아무 조직도 아닙니다.");
				return 1;
			}
			if(oztele[playeroz[playerid]][0] == 0 && oztele[playeroz[playerid]][1] == 0 && oztele[playeroz[playerid]][2] == 0) {
				SendClientMessage(playerid, COLOR_RED, " + 조직두목이 /oz rtele 를 해야 사용가능합니다.");
				return 1;
			}
			SetPlayerInterior(playerid,0);
			if(IsPlayerInAnyVehicle(playerid)) {
		    	SetVehiclePos(GetPlayerVehicleID(playerid),oztele[playeroz[playerid]][0],oztele[playeroz[playerid]][1],oztele[playeroz[playerid]][2]);
			} else {
				SetPlayerPos(playerid,oztele[playeroz[playerid]][0],oztele[playeroz[playerid]][1],oztele[playeroz[playerid]][2]);
			}
			SendClientMessage(playerid, COLOR_RED, " + 조직 두목이 등록한 위치로 이동하였습니다.");
			return 1;
		} else if (ozcmd==7) {
			if(playeroz[playerid]==0) {
				SendClientMessage(playerid, COLOR_RED, " + 당신은 아무 조직도 아닙니다.");
				return 1;
			}
			if(ozMembers[playeroz[playerid]][0]!=playerid) {
				SendClientMessage(playerid, COLOR_RED, " + 조직의 두목만이 텔레포트 위치를 등록할수 있습니다.");
				return 1;
			}
			if(GetPlayerInterior(playerid) != 0) {
				SendClientMessage(playerid, COLOR_RED, " + 건물밖에서만 등록이 가능합니다.");
				return 1;
			}
			new Float:X;
			new Float:Y;
			new Float:Z;
			GetPlayerPos(playerid,X,Y,Z);
			oztele[playeroz[playerid]][0] = X;
			oztele[playeroz[playerid]][1] = Y;
			oztele[playeroz[playerid]][2] = Z;
			SendClientMessage(playerid, COLOR_RED, " + 성공적으로 등록되었습니다.");
		    for(new j = 0; j < ozInfo[oznum][1]; j++) {
				SendClientMessage(ozMembers[oznum][j], COLOR_ORANGE, " + 두목님이 텔레포트위치를 등록했습니다.");
				SendClientMessage(ozMembers[oznum][j], COLOR_ORANGE, " + /oz tele 하시면 등록한 곳으로 갈수있습니다.");
			}
		}

		return 1;
	}

	//------------------- /ozinfo

	else if(strcmp(cmd, "/ozinfo", true) == 0) {
		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp) && playeroz[playerid]==0) {
			SendClientMessage(playerid, COLOR_WHITE, "USAGE: /ozinfo [number]");
			return 1;
		} else if (!strlen(tmp))
			oznum = playeroz[playerid];
		else
			oznum = strval(tmp);

		if(ozInfo[oznum][0]==0) {
			SendClientMessage(playerid, COLOR_RED, " + 존재하지 않는 조직입니다.");
			return 1;
		}

		format(string, sizeof(string),"'%s' (id: %d)의 조직원 리스트", ozNames[oznum], oznum);
		SendClientMessage(playerid, COLOR_GREEN, string);

		for(new i = 0; i < ozInfo[oznum][1]; i++) {
			GetPlayerName(ozMembers[oznum][i], giveplayer, sizeof(giveplayer));
			if(i==0) format(string, sizeof(string),"%s (%d) : 두목", giveplayer, ozMembers[oznum][i]);
 			else format(string, sizeof(string),"%s (%d)", giveplayer, ozMembers[oznum][i]);
			SendClientMessage(playerid, COLOR_YELLOW, string);
		}

		return 1;
	}

 //------------------- /ozs

 if(strcmp(cmd, "/ozs", true) == 0)
 {
  new x;

  SendClientMessage(playerid, COLOR_GREEN, " 현재 조직들:");
     for(new i=0; i < MAX_OZS; i++) {
   if(ozInfo[i][0]==1) {
    format(string, sizeof(string), " + %s%s(%d) - %d명", string,ozNames[i],i,ozInfo[i][1]);

    x++;
    if(x > 2) {
        SendClientMessage(playerid, COLOR_YELLOW, string);
        x = 0;
     format(string, sizeof(string), " ");
    } else {
     format(string, sizeof(string), "%s, ", string);
    }
   }
  }

  if(x <= 2 && x > 0) {
   string[strlen(string)-2] = '.';
      SendClientMessage(playerid, COLOR_YELLOW, string);
  }

  return 1;
 }

 if(strcmp(cmd, "/ozcom", true) == 0) {
    SendClientMessage(playerid, COLOR_RED, " + 사용법: /oz [create/join/quit/invite/kick] [name/number]");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 구성조직확인:/ozs");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직생성:/oz create 조직명");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직가입:/oz join");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직초대:/oz invite 번호");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직추방:/oz kick 번호");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직탈퇴:/oz quit");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직정보:/ozinfo");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직텔포:/oz tele");
		SendClientMessage(playerid, COLOR_YELLOW,"  - 조직텔포등록:/oz rtele");
    return 1;
      }
	if(strcmp(cmd, "/startrace", true) == 0)
{
	if(gara[playerid] == 1 && racestarted == 0) {
	gRoundTimer = SetTimer("CountdownRace", 1000, 1);
	}
	if(gara[playerid] == 2 && racestarted2 == 0) {
	gRoundTimer2 = SetTimer("CountdownRace2", 1000, 1);
	}
	if(gara[playerid] == 3 && racestarted3 == 0) {
	gRoundTimer3 = SetTimer("CountdownRace3", 1000, 1);
	}
	if(gara[playerid] == 4 && racestarted4 == 0) {
	gRoundTimer4 = SetTimer("CountdownRace4", 1000, 1);
	}
    return 1;
}

if(strcmp(cmd, "/raceexit", true) == 0)
{
	if(gara[playerid] == 0) {
	SendClientMessage(playerid,COLOR_WHITE, " + 당신은 레이스에 참가하지 않았습니다.");
	return 1;
	}
	if(gara[playerid] == 1) {
	new racedowncheck;
	racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 1) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted = 0;
		posti = 1;
		status = 1;
		racestarted = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck[i] = 0;
			if(gara[i] == 1) {
			gara[i] = 0;
			}

		}
		SendClientMessageToAll(COLOR_BLUE, " + 산피에로 길거리 레이싱이 끝났습니다.");
		KillTimer(ResetRace);
		}
	}
	if(gara[playerid] == 2) {
	new racedowncheck;
	racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 2) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
  		racestarted2 = 0;
		posti2 = 1;
		status2 = 1;
		racestarted2 = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck2[i] = 0;
			if(gara[i] == 2) {
			gara[i] = 0;
			}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 산피에로 다운힐이 끝났습니다.");
		KillTimer(ResetRace2);
		}
	}
	if(gara[playerid] == 3) {
	new racedowncheck;
	racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 3) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted3 = 0;
		posti3 = 1;
		status3 = 1;
		racestarted3 = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck3[i] = 0;
		if(gara[i] == 3) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 빅이어 드리프트가 끝났습니다.");
		KillTimer(ResetRace3);
		}
	}
	if(gara[playerid] == 4) {
	new racedowncheck;
	racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 4) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted4 = 0;
		posti4 = 1;
		status4 = 1;
		racestarted4 = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck4[i] = 0;
		if(gara[i] == 4) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 외딴마을 드래그가 끝났습니다.");
		KillTimer(ResetRace4);
		}
	}
	racecheck[playerid] = 0;
	gara[playerid] = 0;
	DisablePlayerCheckpoint(playerid);
	TogglePlayerControllable(playerid, 1);
	SendClientMessage(playerid,COLOR_WHITE, " + 레이스를 포기했습니다.");
	return 1;
}
if(strcmp(cmd, "/racejoin4", true) == 0)
{
if(!IsPlayerInAnyVehicle(playerid)){
SendClientMessage(playerid,COLOR_WHITE, " + 자동차에 타야 합니다.");
}else if(gara[playerid] >= 1) {
SendClientMessage(playerid,COLOR_WHITE, " + 당신은 이미 레이스를 하고있습니다.");
}else if(isJail[playerid] == 1) {
SendClientMessage(playerid,COLOR_WHITE, " + 감옥에선 할 수 없습니다.");
}else if(racestarted4 == 1){
SendClientMessage(playerid,COLOR_WHITE, " + 그 경기는 이미 시작되었습니다. 끝날 때 까지 기다려주세요.");
}else{
 switch(posti4){
  case 1:{
  SetVehiclePos(GetPlayerVehicleID(playerid),632.2496,305.7714,19.5079);
  SetVehicleZAngle(GetPlayerVehicleID(playerid), 273);
  posti4++;
  TogglePlayerControllable(playerid, 0);
  gara[playerid] = 4;
  SendClientMessageToAll(COLOR_WHITE, " + 잠시 후 외딴마을 드래그가 개최됩니다. 참가하실려면 /racejoin4 을 입력하세요.");
  SendClientMessage(playerid,COLOR_WHITE, " + 사람이 모이면 /startrace 를 입력해주세요.");
  }
  case 2:{
  SetVehiclePos(GetPlayerVehicleID(playerid),631.6179,311.9312,19.6418);
  SetVehicleZAngle(GetPlayerVehicleID(playerid), 273);
  TogglePlayerControllable(playerid, 0);
  gara[playerid] = 4;
  SendClientMessageToAll(COLOR_WHITE, " + 잠시 후 외딴마을 드래그가 개최됩니다. 참가하실려면 /racejoin4 을 입력하세요.");
  SendClientMessage(playerid,COLOR_WHITE, " + 사람이 모이면 /startrace 를 입력해주세요.");
  posti4++;
  }
    case 3:{
  SetVehiclePos(GetPlayerVehicleID(playerid),641.4308,306.5002,19.5425);
  SetVehicleZAngle(GetPlayerVehicleID(playerid), 273);
  TogglePlayerControllable(playerid, 0);
  gara[playerid] = 4;
  SendClientMessageToAll(COLOR_WHITE, " + 잠시 후 외딴마을 드래그가 개최됩니다. 참가하실려면 /racejoin4 을 입력하세요.");
  SendClientMessage(playerid,COLOR_WHITE, " + 사람이 모이면 /startrace 를 입력해주세요.");
  posti4++;
  }
    case 4:{
  SetVehiclePos(GetPlayerVehicleID(playerid),641.1144,312.5117,19.5419);
  SetVehicleZAngle(GetPlayerVehicleID(playerid), 273);
  gara[playerid] = 4;
  TogglePlayerControllable(playerid, 0);
  SendClientMessage(playerid,COLOR_WHITE, " + /startrace 를 입력해주세요.");
  posti4++;
  }
    case 5:{
  SendClientMessage(playerid,COLOR_WHITE, " + 인원이 이미 다 차있습니다.");
  }
 }
}
return 1;
}
