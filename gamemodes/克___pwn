    {
        SetVehiclePos(GetPlayerVehicleID(playerid), 90.0166,1920.5559,17.9359); // 여기에다가 좌표적고
    }
    else
    {
        SetPlayerPos(playerid, 90.0166,1920.5559,17.9359); // 여기도 좌표
    }

    return 1;
}

