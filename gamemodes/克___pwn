	return 1;
	}
	 if(getCheckpointType(playerid) == CP_BANK || getCheckpointType(playerid) == CP_BANK3 || getCheckpointType(playerid) == CP_BANK4 || getCheckpointType(playerid) == CP_BANK5
        || getCheckpointType(playerid) == CP_BANK6){
