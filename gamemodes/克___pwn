	if(strcmp(cmd, "/hangup", true) == 0)
	{
		if(IsPlayerConnected(playerid))
		{
			if(mobile_state[playerid] == 4) {
				if( call_data[mobile_call[playerid]][cstate] == 1 ) {
					for(new i=0; i < MAX_PLAYERS; i++) {
						if(IsPlayerConnected(i)) {
							if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
								mobile_call[i] = -1;
								mobile_state[i] = 3;
							}
						}
					}
					call_data[mobile_call[playerid]][use] = false;
					call_data[mobile_call[playerid]][check] = false;
					call_data[mobile_call[playerid]][cid] = -1;
					call_data[mobile_call[playerid]][cstate] = 0;
					call_data[mobile_call[playerid]][time] = 0;
					call_data[mobile_call[playerid]][time2] = 0;
					SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 전화가 끊겼습니다.");
					mobile_call[playerid] = -1;
					mobile_state[playerid] = 3;
					SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
				} else if(call_data[mobile_call[playerid]][cstate] == 2) {
					for(new i=0; i < MAX_PLAYERS; i++) {
						if(IsPlayerConnected(i)) {
							if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
								SendClientMessage(i,COLOR_RED," + [PHONE] 통화가 종료되었습니다.");
								mobile_call[i] = -1;
								mobile_state[i] = 3;
								SetPlayerSpecialAction(i,SPECIAL_ACTION_STOPUSECELLPHONE);
							}
						}
					}
					mobile_state[playerid] = 3;
					SendClientMessage(playerid,COLOR_RED," + [PHONE] 통화가 종료되었습니다.");
					SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
					new money;
					money = 15*call_data[mobile_call[playerid]][time];
					GiveMoney(call_data[mobile_call[playerid]][cid],-money);
					new str[255];
					format(str,sizeof(str)," + [PHONE] 통화 요금으로 $%d 달러 를 지불 하셨 습니다. - LG TeleCom -",money);
					SendClientMessage(call_data[mobile_call[playerid]][cid],COLOR_RED,str);
					call_data[mobile_call[playerid]][use] = false;
					call_data[mobile_call[playerid]][check] = false;
					call_data[mobile_call[playerid]][cstate] = 0;
					call_data[mobile_call[playerid]][time] = 0;
					call_data[mobile_call[playerid]][time2] = 0;
					call_data[mobile_call[playerid]][cid] = -1;
					mobile_call[playerid] = -1;
				} else SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 알수 없는 오류입니다.");
			} else SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 당신은 통화중이 아닙니다.");
		}
		return 1;
	}
		if(strcmp(cmd, "/폰정보", true) == 0) {
			if(mobile_state[playerid] == 0) {
				SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 알수없는 오류입니다.");
				return 1;
			} else if(mobile_state[playerid] == 1) {
				SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 가까운 대리점에서 폰을 구입하십시오");
				return 1;
            } else if(mobile_state[playerid] == 2) {
              	SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 개통이 필요합니다.");
				return 1;
			} else if(mobile_state[playerid] == 5) {
				SendClientMessage(playerid, COLOR_GRAD2, " + [PHONE] 폰이 꺼져있는데 -_-");
				return 1;
			} else {
		 		new vstr[256];
		 	 	//SendClientMessage(playerid, COLOR_YELLOW, " + 폰도움말 and 정보");
