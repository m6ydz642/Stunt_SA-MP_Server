isadmin(playerid)
{
	if(IsPlayerAdmin(playerid)) {
	 return 1;
	} else if(madmin[playerid] == 1) {
	return 1;
	} else if(madmin[playerid] == 2) {
	return 1;
	} else if(madmin[playerid] == 3) {
	return 1;
	} else {
	return 0;
	}
}
ismadmin(playerid)
{
	if(IsPlayerAdmin(playerid)) {
	 return 1;
	} else if(madmin[playerid] == 2) {
	return 1;
	} else if(madmin[playerid] == 3) {
	return 1;
	} else {
	return 0;
	}
}
isoadmin(playerid)
{
	if(madmin[playerid] == 3) {
	return 1;
	} else {
	return 0;
	}
}
ishadmin(playerid)
{
	if(udb_UserInt(loginid[playerid],"hideadmin")==1) {
		return 1;
	} else {
		return 0;
	}
}
issubadmin(playerid)
{
	if(madmin[playerid] == 1) {
	 return 1;
	} else if(madmin[playerid] == 2) {
	return 1;
	} else if(madmin[playerid] == 3) {
	return 1;
	} else if(IsPlayerAdmin(playerid)) {
	return 1;
	} else {
	return 0;
	}
}
