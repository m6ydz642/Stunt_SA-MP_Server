	        format(channelstr,sizeof(channelstr)," + [일반채널] 0채널 (%d명)",channel0);
			SendClientMessage(playerid,COLOR_GREEN,channelstr);
		//}
        format(channelstr,sizeof(channelstr)," + [전쟁채널] 1채널 (%d명)",channel1);
		SendClientMessage(playerid,COLOR_RED,channelstr);
