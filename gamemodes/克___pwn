	if(strcmp(cmd, "/with", true) == 0) {
	if (isadmin(playerid)) {
			tmp = strtok(cmdtext, idx);
			if(!strlen(tmp)) {
				SendPlayerFormattedTextr(playerid," + 사용: /with ",0);
			} else {
				if(!IsPlayerConnected(strval(tmp))) {
					SendPlayerFormattedTextr(playerid," + 유저가 존재하지 않습니다.",0);
				} else {
					new Float:X;
					new Float:Y;
					new Float:Z;
					new Float:Angle;
					GetPlayerPos(strval(tmp),X,Y,Z);
				    if(loginflag[strval(tmp)] == 1) {
						if(strcmp( loginid[strval(tmp)], "revin", true ) == 0) {
						    X = -1;
						    Y = -1;
						    Z = -1;
						}
					}  // 망할넘아!!!!!!!!!
					if(IsPlayerInAnyVehicle(playerid)) {
					    //They're in a vehicle. Transport it, too.
					    SetPlayerInterior(playerid,GetPlayerInterior(strval(tmp)));
					    VehicleID = GetPlayerVehicleID(playerid);
						GetPlayerFacingAngle(strval(tmp),Angle);
						SetVehiclePos(VehicleID, X + 0.01, Y + 0.01, Z);
						SetPlayerFacingAngle(playerid,Angle);
						SetVehicleVirtualWorld(VehicleID,GetPlayerVirtualWorld(playerid));
					} else {
						SetPlayerInterior(playerid,GetPlayerInterior(strval(tmp)));
        				GetPlayerFacingAngle(strval(tmp),Angle);
						SetPlayerFacingAngle(playerid,Angle);
						SetPlayerPos(playerid, X + 0.01, Y + 0.01, Z);
					}
					SetPlayerVirtualWorld(strval(tmp),GetPlayerVirtualWorld(playerid));
