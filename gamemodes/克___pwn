  	  SendClientMessage(playerid,COLOR_GREEN," + 라스벤츄라 근처 공사장의 체크포인트에가서 /work 를 친 후");
  	  SendClientMessage(playerid,COLOR_GREEN," + 목적지로 가십시오.");
	  }
	  else if(quest[playerid] == 3) {
	  SendClientMessage(playerid,COLOR_YELLOW," +++ 당신이 해야할 퀘스트 : 낚시 해보기");
	  SendClientMessage(playerid,COLOR_GREEN," + 바닷가에서 /fish 를 치시고 기다리시면 고기가 올라옵니다.");
	  }
	  else if(quest[playerid] == 4) {
