 	if(strcmp(cmd, "/wcar", true) == 0)
	 {
//위의 방식 체제에 따라 { 하나때문에 오류가 생길수도있음
		if(ismadmin(playerid)) //어드민만 가능 Rcon 로그인 말고
		{

    if(IsPlayerInAnyVehicle(playerid) == 1)
    {
        SetVehiclePos(GetPlayerVehicleID(playerid), -1979.7834,250.1295,34.8756); //차량
    }
    else
    {
        SetPlayerPos(playerid, -1993.2650,241.3471,36.6864); //플레이어
    }

		SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 왕카 텔포 완료");
        SendClientMessage(playerid, COLOR_YELLOW, " + 다른텔포 목록을 보려면 [/admintele]");
		}
		else
		{
        SendClientMessage(playerid, COLOR_RED, " + Admin만 사용 가능해요");
        }
		return 1;
	}
	
 	if(strcmp(cmd, "/banks", true) == 0)
	 {
//위의 방식 체제에 따라 { 하나때문에 오류가 생길수도있음
		if(ismadmin(playerid)) //어드민만 가능 Rcon 로그인 말고
		{

    if(IsPlayerInAnyVehicle(playerid) == 1)
    {
        SetVehiclePos(GetPlayerVehicleID(playerid), -1972.1738,615.5448,34.8365); //차량
    }
    else
    {
        SetPlayerPos(playerid, -1972.1738,615.5448,34.8365); //플레이어
