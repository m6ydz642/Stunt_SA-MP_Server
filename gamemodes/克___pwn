    udb_UserSetInt(loginid[playerid],"cookID3",Cooking[playerid][pCookID3]);
    udb_UserSetInt(loginid[playerid],"cookID2",Cooking[playerid][pCookID2]);
    udb_UserSetInt(loginid[playerid],"cookID1",Cooking[playerid][pCookID1]);
	udb_UserSetInt(loginid[playerid],"pCookSkill",PlayerInfo[playerid][pCookSkill]);
	PlayerInfo[playerid][pCookSkill] += 1;

   	return 1;
	}
