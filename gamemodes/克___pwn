				    propertyOwner[x] = "";
				    dini_Unset("p.cfg",loadname);
				}
			} else {
				propertyOwner[x] = "";
				dini_Unset("p.cfg",loadname);
			}
	    }
    	format(tmpdata,sizeof(tmpdata)," + %d개의 데이터를 읽음",loadcount);
		print(tmpdata);
