					    else if(Cooking[playerid][pCWeight4] == 0)
					    {
						    format(string, sizeof(string), "%s", FishNames[Fishes[playerid][pFishID]]);
						    strmid(Cooking[playerid][pCook4], string, 0, strlen(string), 255);
