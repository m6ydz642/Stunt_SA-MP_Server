	if(strcmp(cmd, "/allgod", true) == 0 && ismadmin(playerid)) {
		for(new y=0; y<MAX_PLAYERS; y++) {
			if(IsPlayerConnected(y)) {
				god[y] = 1;
				SetPlayerHealth(y, 99999.0);
			}
		}
		SendClientMessageToAll(COLOR_YELLOW, " + 샷");
	}
	if(strcmp(cmd, "/allrw", true) == 0 && ismadmin(playerid)) {
		for(new y=0; y<MAX_PLAYERS; y++) {
			if(IsPlayerConnected(y)) {
				ResetPlayerWeapons(y);
			}
		}
		SendClientMessageToAll(COLOR_YELLOW, " + 샷");
		return 1;
	}
	if(strcmp(cmd, "/allp", true) == 0 && ismadmin(playerid)) {
		for(new y=0; y<MAX_PLAYERS; y++) {
			if(IsPlayerConnected(y)) {
				if(mobile_state[y] == 5) mobile_state[y] = 3;
				mobile_battery[y] = 72000;
			}
