		SendClientMessage(playerid, COLOR_ORANGE," + _____________________________________________________________");

		return 1;
	}
	if(strcmp(cmd,"/enter",true)==0) {
	    if(IsPlayerInCheckpoint(playerid)) {
			if(getCheckpointType(playerid) == CP_REVIN) {
			    if(IsPlayerConnected(playerid)) {
					SetPlayerPos(playerid,-2181.6135,266.1939,24.1158);
					SendClientMessage(playerid, COLOR_RED, " + 당신은 revin`s Home 안으로 들어오셨습니다.");
				} else {
					SendClientMessage(playerid, COLOR_RED, " + 집주인이 당신을 허용하지 않습니다.");
				}
			} else {
				SendClientMessage(playerid, COLOR_RED, " + 들어갈수있는 건물 안에서 명령어를 치십시오");
			}
		} else {
			SendClientMessage(playerid, COLOR_RED, " + 체크포인트가 필요해용..");
		}
		return 1;
	}
	if(strcmp(cmd, "/buy", true) == 0) {
		new property=999;
		new testproget;
		new logined;
		logined = 0;
		property=PropertyCheckpointSelection(playerCheckpoint[playerid]);
		if(GetPlayerLevel(playerid) < 5 && !ismadmin(playerid)) {
	        SendClientMessage(playerid, COLOR_RED, " + 레벨 5 이상만 살 수 있습니다. 레벨은 아르바이트,낚시 등을 해서 얻을 수 있습니다.");
			return 1;
		}
	    if (propertyValues[playerCheckpoint[playerid]] == 0) {
	        SendClientMessage(playerid, COLOR_RED, " + 이 건물은 사는게 아닙니다.");
			return 1;
		}
		if(getCheckpointType(playerid) == CP_REVIN && !ismadmin(playerid)) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 충분한 돈이 있지 않습니다.");
			return 1;
		}
		if (property == 999 || IsPlayerInCheckpoint(playerid)==0) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 체크포인트 안에서 /buy 를 쳐주세요.");
			return 1;
		}
/*		if (!strlen(propertyOwner[property])) {
			if (GetMoney(playerid) < propertyValues[property]*2) {
				SendClientMessage(playerid, COLOR_RED, " + 충분한 돈이 있질 않습니다.");
				return 1;
			}
		} */
		if (GetMoney(playerid) < propertyValues[property]) {
			SendClientMessage(playerid, COLOR_RED, " + 충분한 돈이 있질 않습니다.");
			return 1;
		}
		if (strlen(propertyOwner[property])) {
		    if(strcmp(loginid[playerid], propertyOwner[property], true) == 0) {
				SendClientMessage(playerid, COLOR_RED, " + 이미 이 건물을 샀습니다.");
				return 1;
			}
		}
		if (ownedtime[property] >= 1 && !ismadmin(playerid)) {
			format (string, sizeof(string), " + 이 건물은 %d분 후에 살 수 있습니다.",ownedtime[property]);
			SendClientMessage(playerid, COLOR_RED, string);
			return 1;
		}
   		for(new i=0; i<MAX_POINTS; i++) {
			if (strlen(propertyOwner[i])) {
				if(strcmp(loginid[playerid], propertyOwner[i], true) == 0) {
					testproget += 1;
				}
			}
		}
		if (testproget >= 2) //건물 구매 가능 갯수
		{
			SendClientMessage(playerid, COLOR_RED, " + 건물은 최대 2개만 살 수 있습니다 (다른사람을 위해)");
			return 1;
		}
		new keydata[255];
		if(!udb_Exists(propertyOwner[property])) {
			propertyOwner[property] = "";
			format(keydata,sizeof(keydata),"%d",property);
			dini_Unset("p.cfg",keydata);
		}
		if(IsPlayerInCheckpoint(playerid)) {
			GetPlayerName(playerid, giveplayer, sizeof(giveplayer));
			if (strlen(propertyOwner[property])) {
				for(new i=0; i<MAX_PLAYERS; i++) {
					if(loginflag[i]==1) {
						if(strcmp(loginid[i], propertyOwner[property], true) == 0) {
						    logined = 1;
						    GiveMoney(i, propertyValues[property]);
							format (string, sizeof(string), " + 당신의 건물 %s 은 %s (id: %d)님에 의해 양도되었습니다.",propertyNames[property],giveplayer,playerid);
							SendClientMessage(i, COLOR_RED, string);
						}
					}
				}
				if(logined == -1) {
				    new tmpd;
					udb_UserSetInt(propertyOwner[property],"money",udb_UserInt(propertyOwner[property],"money")+propertyValues[property]+30000);
					tmpd = udb_UserInt(propertyOwner[property],"pd");
					tmpd++;
					format(keydata,sizeof(keydata),"pd_%d",tmpd);
					udb_UserSetInt(propertyOwner[property],keydata,property);
					udb_UserSetInt(propertyOwner[property],"pd",tmpd);
				}
			}
			if (strlen(propertyOwner[property])) {
				GiveMoney(playerid, -(propertyValues[property]*2));
			} else {
				GiveMoney(playerid, -propertyValues[property]);
			}
			propertyOwner[property] = loginid[playerid];
			ownedtime[property] = 30;
			format(string, sizeof(string), " + %s 건물을 구입하였습니다!", propertyNames[property]);
			SendClientMessage(playerid, COLOR_GREEN, string);
			format(keydata,sizeof(keydata),"%d",property);
			dini_Set("p.cfg",keydata,propertyOwner[property]);
    	    return 1;
		}else {
			SendClientMessage(playerid, COLOR_YELLOW, " + 체크포인트 안에서 /buy 를 쳐주세요.");
		}
		return 1;
	}

	if(strcmp(cmd, "/sell", true) == 0) {
		new property=999;
        property=PropertyCheckpointSelection(playerCheckpoint[playerid]);

   		if(!strlen(propertyOwner[property])) {
	    	SendClientMessage(playerid, COLOR_YELLOW, " + 그 건물을 소유한 사람이 없습니다.");
    		return 1;
   		}
		if(strcmp(loginid[playerid], propertyOwner[property], true) == 0) {
			GiveMoney(playerid, propertyValues[property]);
			new keydata[255];
			format(keydata,sizeof(keydata),"%d",property);
			dini_Unset("p.cfg",keydata);
			ownedtime[property] = 0;
			propertyOwner[property] = "";
			format(string, sizeof(string), " + 당신은 %s 건물을 팔았습니다.", propertyNames[property]);
			SendClientMessage(playerid, COLOR_GREEN, string);
   		} else SendClientMessage(playerid, COLOR_GREEN, " + 당신의 소유지가 아닙니다.");
		return 1;
	}
	if(strcmp(cmd, "/pmyes", true) == 0) {
		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp) || !IsNumeric(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE , " + USAGE: /pmyes [플레이어ID]");
			return 1;
		}
		giveplayerid = strval(tmp);
		GetPlayerName(playerid,playername,sizeof(playername));
		GetPlayerName(giveplayerid,giveplayername,sizeof(giveplayername));

		if(IsPlayerConnected(giveplayerid)){
			if (IgnorePlayerlist[playerid][giveplayerid] == 1){
				IgnorePlayerlist[playerid][giveplayerid] = 0;
				format(strings, sizeof(strings), " + 당신은 %s 님의 문자메세지를 보기로 했습니다.", giveplayername);
				SendClientMessage(playerid, COLOR_LAWNGREEN, strings);
			} else {
				format(strings, sizeof(strings), " + 원래 %s 님의 문자메세지를 보기로 했습니다.", giveplayername);
				SendClientMessage(playerid, COLOR_LAWNGREEN, strings);
			}
		} else {
			format(strings, sizeof(strings), " + %d번 님은 없습니다.", giveplayerid);
			SendClientMessage(playerid, COLOR_LAWNGREEN, strings);
		}
	return 1;
}
	if(strcmp(cmd, "/pmno", true) == 0) {
 		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp) || !IsNumeric(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE , " + USAGE: /pmno [플레이어ID]");
