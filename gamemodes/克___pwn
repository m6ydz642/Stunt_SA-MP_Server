//------------------------------------------------------------------------------------------------------
public OnPlayerExitVehicle(playerid,vehicleid) {
	if(gUseVText[playerid] == 1) {
		gUseVText[playerid] = 0;
		gVTextNum[playerid] = 0;
		TextDrawHideForPlayer(playerid,gVText[playerid]);
		TextDrawDestroy(gVText[playerid]);
	}
	SetVehicleVirtualWorld(vehicleid,0);
	return 1;
}
public OnVehicleSpawn(vehicleid)
{
    lock[vehicleid]=-1;
//    gastank[vehicleid] = tgastank[vehicleid]/2 + tgastank[vehicleid]/4;
    LinkVehicleToInterior(vehicleid,0);
	SetVehicleVirtualWorld(vehicleid,0);
    return 1;
}

public OnVehicleDeath(vehicleid, killerid) {
    lock[vehicleid]=-1;

}
public OnPlayerRequestClass(playerid, classid)
{
	SetupPlayerForClassSelection(playerid);
 	ApplyAnimation(playerid, "benchpress", "gym_bp_celebrate", 4.000000, 1, 1, 1, 1, -1);
 	return 1;
}
public OnPlayerRequestSpawn(playerid)
{
	if(loginflag[playerid] == 1) {
	
		return 1;
	} else {
	    if(spawn_de[playerid] == 0) {
			PlayerPlaySound(playerid,1056,258.4893,-41.4008,1002.0234);
	 		SendClientMessage(playerid,GetPlayerColor(playerid)," + 로그인을 해야 접속하실수 있습니다.");
		  	SendClientMessage(playerid, COLOR_GREEN, " + 로그인을 하시려면 /logincom 을 참고해주세요");
		  	spawn_de[playerid] = 1;
		}
		return 0;
	}

}

public SetupPlayerForClassSelection(playerid)
{
 	SetPlayerInterior(playerid,0);
	SetPlayerPos(playerid,-3235.1689,433.2996,26.0032);
	SetPlayerFacingAngle(playerid, 270.0);
	SetPlayerCameraPos(playerid,-3232.4358,431.0729,25.3254);
	SetPlayerCameraLookAt(playerid,-3235.1689,433.2996,26.0032);
	classing[playerid] = 1;
}

public GameModeExitFunc()
{
	GameModeExit();
}
public cdelobject() {
	for(new i = 0; i < MAX_M_OBJECTS; i++) {
	    if(ob_s[i] != -1) {
			if(!IsValidObject(ob_s[i]) ) {
				ob_s[i] = -1;
			}
		}
	    if(ob_s[i] != -1) { DestroyObject(ob_s[i]); ob_s[i] = -1; }
	}
}
public cobject() {

	for(new i = 0; i < MAX_M_OBJECTS; i++) {
	    if(ob_s[i] != -1) {
			if(!IsValidObject(ob_s[i])) {
				ob_s[i] = -1;
			}
		}
	    if(ob_s[i] != -1) DestroyObject(ob_s[i]);
		ob_s[i] = CreateObject(ob_m[i],ob_data[i][0],ob_data[i][1],ob_data[i][2],ob_data[i][3],ob_data[i][4],ob_data[i][5]);

	}
}


public OnGameModeInit() //서버오픈
{
	new tmp[256];
	if (!dini_Exists("jusic.txt"))
	{
		dini_Create("jusic.txt");
		dini_IntSet("jusic.txt", "jusic" , 90000);
		dini_IntSet("jusic.txt", "jusic2" , 70000);
		dini_IntSet("jusic.txt", "jusic3" , 80000);
		dini_IntSet("jusic.txt", "jusic4" , 100000);
		dini_IntSet("jusic.txt", "jusic5" , 110000);
		dini_IntSet("jusic.txt", "jusic6" , 120000);
		dini_IntSet("jusic.txt", "jusic7" , 130000);


		jusic = 90000; //주식
 		jusic2 = 70000;
 		jusic3 = 80000;
 		jusic4 = 100000;
 		jusic5 = 110000;
 		jusic6 = 120000;
 		jusic7 = 130000;
	} else {
	tmp = dini_Get("jusic.txt", "jusic");
	jusic = strval(tmp);
    tmp = dini_Get("jusic.txt", "jusic2");
	jusic2 = strval(tmp);
	tmp = dini_Get("jusic.txt", "jusic3");
	jusic3 = strval(tmp);
	tmp = dini_Get("jusic.txt", "jusic4");
	jusic4 = strval(tmp);
	tmp = dini_Get("jusic.txt", "jusic5");
	jusic5 = strval(tmp);
	tmp = dini_Get("jusic.txt", "jusic6");
	jusic6 = strval(tmp);
	tmp = dini_Get("jusic.txt", "jusic7");
	jusic7 = strval(tmp);
    }
    

	vehiclebar[0] = TextDrawCreate(549.0, 59.0, " ");
