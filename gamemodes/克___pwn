			SendClientMessage(playerid,COLOR_GREEN," + /selleat : 음식 팔기(물고기, 음식)");
		}
		case CP_BANK2: {
			SendClientMessage(playerid, COLOR_YELLOW, " + 입금: /bank 금액 ");
			SendClientMessage(playerid, COLOR_YELLOW, " + 출금: /get 금액, 통장확인: /balance");
		}
		case CP_BANK3: {
			SendClientMessage(playerid, COLOR_GREEN, " + 입금: /bank 금액 ");
			SendClientMessage(playerid, COLOR_GREEN, " + 출금: /get 금액, 통장확인: /balance");
			SendClientMessage(playerid,COLOR_GREEN," + /camerabuy : 카메라 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /buyeat : 음식재료 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /selleat : 음식 팔기(물고기, 음식)");
			SendClientMessage(playerid,COLOR_GREEN," + /bok : 복권 긁기($3000)");
		}
		case CP_BANK4: {
			SendClientMessage(playerid, COLOR_GREEN, " + 입금: /bank 금액 ");
			SendClientMessage(playerid, COLOR_GREEN, " + 출금: /get 금액, 통장확인: /balance");
			SendClientMessage(playerid,COLOR_GREEN," + /camerabuy : 카메라 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /buyeat : 음식 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /selleat : 음식 팔기(물고기, 음식)");
			SendClientMessage(playerid,COLOR_GREEN," + /bok : 복권 긁기($3000)");
		}
		case CP_BANK5: {
			SendClientMessage(playerid, COLOR_GREEN, " + 입금: /bank 금액 ");
			SendClientMessage(playerid, COLOR_GREEN, " + 출금: /get 금액, 통장확인: /balance");
			SendClientMessage(playerid,COLOR_GREEN," + /camerabuy : 카메라 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /buyeat : 음식 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /selleat : 음식 팔기(물고기, 음식)");
			SendClientMessage(playerid,COLOR_GREEN," + /bok : 복권 긁기($3000)");
		}
		case CP_BANK6: {
			SendClientMessage(playerid, COLOR_GREEN, " + 입금: /bank 금액 ");
			SendClientMessage(playerid, COLOR_GREEN, " + 출금: /get 금액, 통장확인: /balance");
			SendClientMessage(playerid,COLOR_GREEN," + /camerabuy : 카메라 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /buyeat : 음식 사기");
			SendClientMessage(playerid,COLOR_GREEN," + /selleat : 음식 팔기(물고기, 음식)");
			SendClientMessage(playerid,COLOR_GREEN," + /bok : 복권 긁기($3000)");
		}

		case CP_PIRATE: {
		    if(quest[playerid] == 1) {
			SendClientMessage(playerid,COLOR_CRIMSON," ");
			SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
			SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $5000");
			GiveMoney(playerid, 5000);
			quest[playerid] = 2;
			}
			SendClientMessage(playerid, COLOR_YELLOW, " + 해적선 청소 아르바이트 입니다. 주기적으로 돈을 받을 수 있습니다.");
            SendClientMessage(playerid, COLOR_ORANGE, " + 주인:청소 깨끗이 안하면 국물도 없을줄 알아! (가만히 서있으면되요)");
		}
		case CP_AMMU: {
			SendClientMessage(playerid, COLOR_GREEN, " + 당신은 돈만있다면 이곳에서 언제든지 무기구입을 할 수 있습니다.");
			SendClientMessage(playerid, COLOR_YELLOW, " + 무기구입: /bwmelee, /bwpistol, /bwshotgun, /bwsmg, /bwrifle , 무기목록:/weaponlist ");
		}
		case CP_AMMU2: {
			SendClientMessage(playerid, COLOR_GREEN, " + 당신은 돈만있다면 이곳에서 언제든지 무기구입을 할 수 있습니다.");
			SendClientMessage(playerid, COLOR_YELLOW, " + 무기구입: /bwmelee, /bwpistol, /bwshotgun, /bwsmg, /bwrifle , 무기목록:/weaponlist ");
		}
		case CP_REST: {
  			if(quest[playerid] == 1) {
			SendClientMessage(playerid,COLOR_CRIMSON," ");
			SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
			SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $5000");
			GiveMoney(playerid, 5000);
			quest[playerid] = 2;
			}
			SendClientMessage(playerid, COLOR_YELLOW, " + 음식점 청소 아르바이트 입니다. 주기적으로 돈을 받을 수 있습니다.");
            SendClientMessage(playerid, COLOR_ORANGE, " + 주인:먼지 하나라도 있으면 돈안줘!!!! (가만히 서있으면되요)");
		}
  		case CP_RESEARCH: {
 		    if(quest[playerid] == 1) {
			SendClientMessage(playerid,COLOR_CRIMSON," ");
			SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
			SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $5000");
			GiveMoney(playerid, 10000);
			quest[playerid] = 2;
			}
    		SendClientMessage(playerid, COLOR_YELLOW, " + 연구소 보조일을 하는중... ");
            SendClientMessage(playerid, COLOR_ORANGE, " + 연구소장:실험기구 안깨뜨리도록 잘하게! (가만히 서있으면되요)");
		}
		case CP_LIFTMECHIL: {
			SendClientMessage(playerid, COLOR_MESSAGE , " + 당신은 /liftme 명령어로 바위산 정상에 갈 수 있습니다.");
		}
		case CP_LIFTMEGATE: {
			SendClientMessage(playerid, COLOR_MESSAGE , " + 당신은 /liftme 명령어로 금문교에 갈 수 있습니다.");
		}
	}

		if(propertyValues[playerCheckpoint[playerid]] >= 1)
		{
		format(string, sizeof(string), " + 당신은 %s 를 /buy 로 $%d 에 구입할수 있고, /sell 로 되팔 수 있습니다.", propertyNames[playerCheckpoint[playerid]], propertyValues[playerCheckpoint[playerid]]);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		format(string, sizeof(string), " + 이 건물의 수익금은 $%d 입니다. ", propertyValues[playerCheckpoint[playerid]]/40);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		if(IsVehicleAtStation(playerid) != 999) SendClientMessage(playerid, COLOR_YELLOW, " + 이곳에서 /fillup 명령어로 기름을 채우실수 있습니다.");
		if(getCheckpointType(playerid) == CP_REVIN) SendClientMessage(playerid, COLOR_ECHO, " + 당신은 /enter 명령어로 건물안에 들어가실수 있습니다.");
			if (strlen(propertyOwner[playerCheckpoint[playerid]])) {
			    new logined;
				logined = -1;
				for (new x=0;x<MAX_PLAYERS;x++) {
				    if(loginflag[x] == 1) {
				        if(strcmp(loginid[x], propertyOwner[playerCheckpoint[playerid]], true) == 0) {
				            logined = x;
				            break;
						}
					}
				}
				if(logined != -1) GetPlayerName(logined, ownplayer, sizeof(ownplayer));
				format(string, sizeof(string), " + 이 건물의 현재주인은 (로그인아이디 : %s) 입니다. 주인있는 건물을 마음대로 사면 영밴됩니다", propertyOwner[playerCheckpoint[playerid]]);
				SendClientMessage(playerid, COLOR_RED, string);
				if(logined != -1) {
					format(string, sizeof(string), " + %s님은 현재 %s(%d)로 접속중입니다.^-^", propertyOwner[playerCheckpoint[playerid]],ownplayer,logined);
					SendClientMessage(playerid, COLOR_RED, string);
				}
			}
		}
	return 1;
	}
	
public OnPlayerEnterRaceCheckpoint(playerid)
{

new string[256];
if(IsPlayerInArea(playerid, rock_cp[player_cp[playerid]][0]-30, rock_cp[player_cp[playerid]][1]-30, rock_cp[player_cp[playerid]][0]+30, rock_cp[player_cp[playerid]][1]+30)) {
if(event_flag == 1 && event_value == 1)
{
if(player_cp[playerid] == 17)
{
DisablePlayerRaceCheckpoint(playerid);
format(string, sizeof(string),  " + 바위산 등반에 성공하셨습니다! $%d 의 상금 획득!! ", ROCK_MONEY);
SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
GameTextForPlayer(playerid, "~y~Great!", 3000,5);
GiveMoney(playerid, ROCK_MONEY);
player_cp[playerid]++;
}
else
{
player_cp[playerid]++;
DisablePlayerRaceCheckpoint(playerid);
if(player_cp[playerid] == 17){
SetPlayerRaceCheckpoint(playerid,4, rock_cp[player_cp[playerid]][0], rock_cp[player_cp[playerid]][1], rock_cp[player_cp[playerid]][2],rock_cp[player_cp[playerid]][0], rock_cp[player_cp[playerid]][1], rock_cp[player_cp[playerid]][2], 10);
} else {
SetPlayerRaceCheckpoint(playerid,0, rock_cp[player_cp[playerid]][0], rock_cp[player_cp[playerid]][1], rock_cp[player_cp[playerid]][2],rock_cp[player_cp[playerid]+1][0], rock_cp[player_cp[playerid]+1][1], rock_cp[player_cp[playerid]+1][2], 10);
}
}
}
//else DisablePlayerRaceCheckpoint(playerid);
return 1;
}

if(IsPlayerInArea(playerid, race_cp[player_cp[playerid]][0]-30, race_cp[player_cp[playerid]][1]-30, race_cp[player_cp[playerid]][0]+30, race_cp[player_cp[playerid]][1]+30)) {
if(event_flag == 1 && event_value == 2)
{
if(player_cp[playerid] == 17 && race_lap[playerid] == RACE_LAP)
{
DisablePlayerRaceCheckpoint(playerid);
player_cp[playerid]++;

for(new i=0 ; i<3 ; i++)
{
if(race_win[i] == -1)
{
new ownplayer[MAX_PLAYER_NAME];
GetPlayerName(playerid, ownplayer,sizeof(ownplayer));
format(string, sizeof(string),  " + %s님이 드카 레이싱에서 %d등으로 $%d 의 상금 획득!! ", ownplayer, i+1, race_money[i]);
SendClientMessageToAll(COLOR_LIGHTBLUE, string);
GameTextForPlayer(playerid, "~y~Great!", 3000,5);
GiveMoney(playerid, race_money[i]);
race_win[i] = playerid;
if(i==2)
{
SendClientMessageToAll(COLOR_ORANGE, " + 순위가 정해졌으므로 드카 레이싱 이벤트를 종료합니다.");
event_flag = 0;
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
}
run_time = RUNAWAY_TIME;
for(new j=0 ; j<3 ; j++)
{
if(race_win[j] != -1)
{
if(IsPlayerConnected(race_win[j]))
{
GetPlayerName(race_win[j], ownplayer,sizeof(ownplayer));
format(string, sizeof(string),  " + %d등 : %s님 , 상금 $%d",j+1, ownplayer, race_money[j]);
SendClientMessageToAll(COLOR_LIGHTBLUE, string);
}
}
}

}
return 1;
}
}
SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 당신은 순위권 밖입니다. 수고하셨습니다.");
DisablePlayerRaceCheckpoint(playerid);
GameTextForPlayer(playerid, "~w~Good", 3000,5);
}
else
{
player_cp[playerid]++;
DisablePlayerRaceCheckpoint(playerid);
if(player_cp[playerid] == 18)
{
player_cp[playerid] = 0;
race_lap[playerid]++;
format(string, sizeof(string), " ~y~%d ~r~lap ~w~left", RACE_LAP - race_lap[playerid]+1);
GameTextForPlayer(playerid, string, 3000, 5);
}
if(race_lap[playerid] == RACE_LAP && player_cp[playerid] == 17) {
SetPlayerRaceCheckpoint(playerid,4, race_cp[player_cp[playerid]][0], race_cp[player_cp[playerid]][1], race_cp[player_cp[playerid]][2],race_cp[player_cp[playerid]][0], race_cp[player_cp[playerid]][1], race_cp[player_cp[playerid]][2], 10);
} else {
SetPlayerRaceCheckpoint(playerid,0, race_cp[player_cp[playerid]][0], race_cp[player_cp[playerid]][1], race_cp[player_cp[playerid]][2],race_cp[player_cp[playerid]+1][0], race_cp[player_cp[playerid]+1][1], race_cp[player_cp[playerid]+1][2], 10);
}
}
}
//else DisablePlayerRaceCheckpoint(playerid);
return 1;
}

OnPlayerEnterCheckpoint(playerid);
return 1;
}

