  SendClientMessageToAll(COLOR_WHITE, " + 잠시 후 산피에로 길거리 레이싱이 개최됩니다. 참가하실려면 /racejoin1 을 입력하세요.");
  SendClientMessage(playerid,COLOR_WHITE, " + 사람이 모이면 /startrace 를 입력해주세요.");
  posti++;
  }
    case 4:{
  SetVehiclePos(GetPlayerVehicleID(playerid),-2116.2883,205.7404,35.0577);
  SetVehicleZAngle(GetPlayerVehicleID(playerid), 90);
  gara[playerid] = 1;
  TogglePlayerControllable(playerid, 0);
