	if(strcmp(cmd, "/fight", true) == 0)
	{
	
	    if(IsPlayerConnected(playerid))
	    {
	        if(MyJob[playerid] != JOB_BOXER)
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 당신은 복서가 아닙니다.");
	            return 1;
	        }
	        if(InRing > 0)
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 경기는 끝나지 않았습니다.");
	            return 1;
	        }
	        if(PlayerBoxing[playerid] > 0)
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 당신은 복싱중입니다.");
	            return 1;
	        }
	        if(!PlayerToPoint(20.0,playerid,765.9343,0.2761,1000.7173))
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 당신은 체육관 안에 있지 않습니다.");
	            return 1;
	        }
	        tmp = strtok(cmdtext, idx);
			if(!strlen(tmp)) {
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /fight [Playerid/PartOfName]");
				return 1;
			}
			
			giveplayerid = strval(tmp);

			if(MyJob[giveplayerid] != JOB_BOXER)
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 상대는 복서가 아닙니다.");
	            return 1;
	        }
		    if(IsPlayerConnected(giveplayerid))
			{
			    if(giveplayerid != INVALID_PLAYER_ID)
			    {
			        if (ProxDetectorS(8.0, playerid, giveplayerid))
					{
					    if(giveplayerid == playerid) { SendClientMessage(playerid, COLOR_GREY, " + 자기와 싸울수는 없져~~!"); return 1; }
					    GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
						GetPlayerName(playerid, sendername, sizeof(sendername));
						format(string, sizeof(string), " + 당신은 %s 님에게 결투 신청을 했습니다!", giveplayer);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						format(string, sizeof(string), " + 복서 %s 님께서 당신과 복싱경기를 원하고 있습니다. (승낙: /accept)", sendername);
						SendClientMessage(giveplayerid, COLOR_LIGHTBLUE, string);
				        BoxOffer[giveplayerid] = playerid;
					}
					else
					{
						SendClientMessage(playerid, COLOR_GREY, " + 상대가 당신 근처에 있지 않습니다.");
						return 1;
					}
			    }
			}
			else
			{
			    SendClientMessage(playerid, COLOR_GREY, " + 접속중인 유저가 아닙니다.");
			    return 1;
			}
	    }
	    return 1;
