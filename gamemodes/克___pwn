				SendClientMessage(playerid,COLOR_GREEN," + 비밀번호가 틀립니다.!");
				return 1;
			}
			udb_UserSetInt(loginid[playerid],"password_hash",udb_hash(aloginpass));
			SendClientMessage(playerid,COLOR_GREEN," + 비밀번호가 바뀌었습니다.");
			return 1;
