public HealingInAmbu(){
	new Float:health;
	new CurrentHealth;

	for (new i=0;i<MAX_PLAYERS;i++){
	    if(IsPlayerConnected(i)){
	        if((IsPlayerInAnyVehicle(i)) && (GetPlayerState(i) != PLAYER_STATE_DRIVER)){
	            if(IsPlayerInVehicle(i,278)){
	                if(AmbuHealing[0]!=999){
		                TogglePlayerControllable(i,0);
						GetPlayerHealth(i, health);
						CurrentHealth=floatround(health);
						if ( CurrentHealth < 100 ){
						    if (CurrentHealth > 95 ){
						        SetPlayerHealth(i, 100.0);
								SendClientMessage(i, COLOR_IVORY , " + 당신의 체력이 다 회복되서 차에서 내려졌습니다.");
								if(GetMoney(i) > 5000){
									GiveMoney(i, -5000);
									SendClientMessage(i, COLOR_IVORY , " + 치료비용으로 $5000 를 냈습니다.");
								} else {
									SendClientMessage(i, COLOR_IVORY , " + 치료비용을 내지 못했습니다.");
								}
				                TogglePlayerControllable(i,1);
				                RemovePlayerFromVehicle(i);
								format(strings, sizeof(strings), " + 당신은 구급미션을 해서 $%d 를 벌었습니다.", medicalreward[0]);
								SendClientMessage(AmbuHealing[0], COLOR_IVORY , strings);
								GiveMoney(AmbuHealing[0], medicalreward[0]);
								if(medicalreward[0] != 100000){
									medicalreward[0]=medicalreward[0]+2500;
								} else {
									medicalreward[0]=2500;
								}
							} else {
							    SetPlayerHealth(i, health+20.0);
								format(strings, sizeof(strings), " + 당신의 체력이 %d(으)로 회복되었습니다.", CurrentHealth);
								SendClientMessage(i, COLOR_GREEN , strings);
							}
						} else {
							TogglePlayerControllable(i,1);
							SendClientMessage(i, COLOR_IVORY , " + 당신은 치료 받을 필요가 없습니다.");
				            TogglePlayerControllable(i,1);
				            RemovePlayerFromVehicle(i);
				        }
				    } else {
						SendClientMessage(i, COLOR_IVORY , " + 당신은 구급차에서 내려졌습니다.");
			            TogglePlayerControllable(i,1);
			            RemovePlayerFromVehicle(i);
					}
				}
			}
		}
	}
}

public auto_kick()
{

new string[128];
new name[MAX_PLAYER_NAME];
new i;


for(i=0;i<MAX_PLAYERS;i++)
{
if(IsPlayerConnected(i) && !ismadmin(i))
{
GetPlayerPos(i, kickpos1[i][0], kickpos1[i][1], kickpos1[i][2]);

if(kickpos1[i][0] == kickpos2[i][0] && kickpos1[i][1] == kickpos2[i][1] && kickpos1[i][2] == kickpos2[i][2])
{

player_stop[i] += 10;


if(player_stop[i] > KICK_TIME)
{
GetPlayerName(i, name, sizeof(name));
format(strings, sizeof(strings), " + %s(%d) 님이 잠수를 타서 킥되셨습니다.",name,i);
SendClientMessageToAll(COLOR_IVORY, strings);
Kick(i);
}

//Print Message
if(player_stop[i] > KICK_TIME*1/2)
{
format(string, sizeof(string), " + 당신은 아무 행동도 하고있지 않습니다. 계속 행동을 하지 않으면 %d초 후 킥 됩니다.",(KICK_TIME - player_stop[i]));
SendClientMessage(i, COLOR_LEMON, string);
}

}
else player_stop[i] = 0;


kickpos2[i][0] = kickpos1[i][0];
kickpos2[i][1] = kickpos1[i][1];
kickpos2[i][2] = kickpos1[i][2];
}
}
}

public urlsay() {
 	SendClientMessageToAll(COLOR_CHARTREUSE," + ★★ 경제서버 공식카페는 http://infernus.gg.gg (스턴트 서버포함) 입니다. ★★");
}
public VoteUpdate()
{
	new Name[MAX_PLAYER_NAME];
	new string[256];
	new i, vote_num, player_num;
	new str[256];
	
	// ---------------------/vote

	if(vote_flag == 1)
	{
		if(IsPlayerConnected(vote_ID) == 0)
		{
			SendClientMessageToAll(COLOR_RED, " + 투표 대상이 방을 나갔습니다. 투표를 중단합니다.");
			vote_flag = 0;
			for(i=0 ; i<MAX_PLAYERS ; i++)
				vote[i] = 0;
			return 1;
		}
		else
		{
			GetPlayerName(vote_ID, Name, MAX_PLAYER_NAME);
			vote_num = 0;
			player_num = 0;
			for(i=0 ; i<MAX_PLAYERS ; i++)
				if(IsPlayerConnected(i)==1)player_num++;
			for(i=0 ; i<MAX_PLAYERS ; i++)
				if(vote[i] == 1)vote_num++;
			if(vote_num > player_num/3 )
			{
				format(string, sizeof(string), " + %s 님의 강퇴 찬성표는 총 %d 표중에 %d 표입니다. ", Name, player_num-1, vote_num);
				SendClientMessageToAll(COLOR_ORANGE, string);
				format(string, sizeof(string), " + %s 님은 투표의 결과로 방에서 강퇴됩니다. 투표를 종료합니다.", Name);
				SendClientMessageToAll(COLOR_RED, string);
				if(IsPlayerConnected(vote_ID) == 1)Kick(vote_ID);
				vote_flag = 0;
				for(i=0 ; i<MAX_PLAYERS ; i++)
					vote[i] = 0;
				return 1;
			}
			else if( vote_time%10 == 0 )
			{
				format(string, sizeof(string), " + %s 님의 강퇴 찬성표는 총 %d 표중에 %d 표입니다. ", Name, player_num-1, vote_num);
				SendClientMessageToAll(COLOR_ORANGE, string);
				if( vote_time <= 0 )
				{
					SendClientMessageToAll(COLOR_RED, " + 시간이 초과하여 투표를 중단합니다. ");
					vote_flag = 0;
					for(i=0 ; i<MAX_PLAYERS ; i++)
						vote[i] = 0;
					return 1;
				}
				else
				{
					format(string, sizeof(string), " + 남은시간 %d 초. 강퇴를 찬성하시는 분은 /yes 를 눌러주세요. ",vote_time);
					SendClientMessageToAll(COLOR_ORANGE, string);
				}
			}
			vote_time--;
		}
	}
	
	// - event
	
if(event == 1) run_time--;
else if(event == 0) run_time = RUNAWAY_TIME;
if(event_flag == 1)
{
if(event_value == 0)
{

GetPlayerName(runaway_ID, Name, sizeof(Name));
if(event == 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 방장의 요청으로 이벤트를 종료합니다. 지금부터 PK 금지!");
event_flag = 0;
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
}
murder_flag = 0;
run_time = RUNAWAY_TIME;
}
else if(run_time <= 0)
{
format(str, sizeof(str), " + %s 님이 도망에 성공했습니다! %s 님께는 상금 $%d를 드립니다.", Name, Name, RUNAWAY_MONEY);
SendClientMessageToAll(COLOR_ORANGE, str);
SendClientMessageToAll(COLOR_ORANGE, " + 지금부터 PK 금지! ");
if(IsPlayerConnected(runaway_ID) == 1)
{
GiveMoney(runaway_ID,RUNAWAY_MONEY);
}
event_flag = 0;
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
}
murder_flag = 0;
run_time = RUNAWAY_TIME;
}
else if((run_time % 10) == 0)
{
format(str, sizeof(str), " + ID %d, 닉넴 %s 인 님을 제거하세요 ! 남은시간 %d 초 !", runaway_ID, Name, run_time);
SendClientMessageToAll(COLOR_YELLOW, str);
}
}
else if(event_value == 1)
{
if(event == 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 방장의 요청으로 이벤트를 종료합니다.");
event_flag = 0;
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
}
run_time = RUNAWAY_TIME;
}
else if(run_time <= 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 바위산 등산 이벤트를 종료합니다! 수고하셨습니다! ");
event_flag = 0;
event_value = random(3);
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
DisablePlayerRaceCheckpoint(i2);
}
run_time = RUNAWAY_TIME;
}
else if((run_time % 10) == 0)
{
format(str, sizeof(str), " + 바위산을 등반하세요! 남은시간 %d 초!", run_time);
SendClientMessageToAll(COLOR_YELLOW, str);
}
}
else if(event_value == 2)
{
if(event == 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 방장의 요청으로 이벤트를 종료합니다.");
event_flag = 0;
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
}
run_time = RUNAWAY_TIME;
}
else if(run_time <= 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 드카 레이싱 이벤트를 종료합니다! 수고하셨습니다. ");
event_flag = 0;
event_value = random(3);
for(new i2 = 0; i2 < MAX_PLAYERS; i2++) {
player_cp[i2] = 0;
DisablePlayerRaceCheckpoint(i2);
}
run_time = RUNAWAY_TIME;
for(i=0 ; i<3 ; i++)
{
if(race_win[i] != -1)
{
if(IsPlayerConnected(race_win[i]))
{
GetPlayerName(race_win[i], Name,sizeof(Name));
format(str, sizeof(str),  " + %d등 : %s님 , 상금 $%d",i+1, Name, race_money[i]);
SendClientMessageToAll(COLOR_LIGHTBLUE, str);
}
}
}
}
else if((run_time % 30) == 0)
{
format(str, sizeof(str), " + 드카 레이싱 경기 남은시간 %d 초!", run_time);
SendClientMessageToAll(COLOR_YELLOW, str);
}
}
}
else if(run_time <= 0 && event == 1)
{
new num;
for(i=0, num=0 ; i<MAX_PLAYERS ; i++)
if(IsPlayerConnected(i) == 1)num++;
if(num < 2 && event_value == 0)
{
SendClientMessageToAll(COLOR_YELLOW, " + RUN AWAY 이벤트는 2인 이상 있을 시에 가능하므로 보류됩니다. ");
event_value = random(3);
run_time = RUNAWAY_TIME;
}
else if(num < 3 && event_value == 2)
{
SendClientMessageToAll(COLOR_YELLOW, " + 드카 레이싱 이벤트는 3인 이상 있을 시에 가능하므로 보류됩니다. ");
event_value = random(3);
run_time = RUNAWAY_TIME;
}
else
{
if(event_value == 0)
{
runaway_ID=random(MAX_PLAYERS);
if(IsPlayerConnected(runaway_ID) != 1 || noevent[runaway_ID] == 1)return 1;
SendClientMessageToAll(COLOR_ORANGE, " + 지금부터 PK가 허용되는 'RUN AWAY' 이벤트를 시작합니다! ");
GetPlayerName(runaway_ID, Name, MAX_PLAYER_NAME);
format(str, sizeof(str), " + 여러분이 살해할 ID 는 %d, 닉넴은 %s 입니다. 제한시간 %d분, 도망치세요! ", runaway_ID, Name, PK_TIME/60);
SendClientMessageToAll(COLOR_ORANGE, str);
format(str, sizeof(str), "~y~ID:~w~%d ~y~NICK:~w~%s ~r~KILL HIM", runaway_ID, Name);
SendClientMessageToAll(COLOR_ORANGE, " + 도망자는 비행기, 헬기 등 을 못탑니다.");
GameTextForAll(str,5000,5);
if(GetVehicleType(GetPlayerVehicleID(runaway_ID)) == VT_AIR) {
RemovePlayerFromVehicle(runaway_ID);
}
for(i=1 ; i<=254 ; i++)
event_flag = 1;
murder_flag = 1;
run_time = PK_TIME;
}
else if(event_value == 1)
{
SendClientMessageToAll(COLOR_ORANGE, " + 지금부터 바위산 등반 이벤트를 시작합니다! ");
GameTextForAll("~w~Go! ~y~Go! ~r~Go!", 5000, 5);
for(i=0 ; i<MAX_PLAYERS ; i++) player_cp[i] = 0;
run_time = ROCK_TIME;
event_flag = 1;
}
else if(event_value == 2)
{
SendClientMessageToAll(COLOR_ORANGE, " + 지금부터 드카 레이싱 이벤트를 시작합니다! ");
GameTextForAll("~w~Go! ~y~Go! ~r~Go!", 5000, 5);
for(i=0 ; i<MAX_PLAYERS ; i++)
{
player_cp[i] = 0;
race_lap[i] = 1;
}
for(i=0 ; i<3 ; i++) race_win[i] = -1;
run_time = RACE_TIME;
event_flag = 1;
}
else run_time = RUNAWAY_TIME;
}
}
else if(run_time < 10)
{
if(event_value == 2)
{
format(str, sizeof(str), " ~y~Race ~r~Count ~r~%d", run_time);
GameTextForAll(str, 5000, 5);
}
}
else if(run_time == 30 && event == 1)
{
if(event_value == 0)
{
SendClientMessageToAll(COLOR_ORANGE, " + 약 30초 후에 'RUN AWAY' 이벤트가 열립니다.");
SendClientMessageToAll(COLOR_ORANGE, " + 서버에서 지정해준 한명의 ID 를 다른 사용자들이 ");
format(str, sizeof(str), " + 제한시간 안에 제거하면 제거한 사람은 $%d의 상금을 얻습니다. ", RUNAWAY_MONEY_KILLER);
SendClientMessageToAll(COLOR_ORANGE, str);
format(str, sizeof(str), " + 도망에 성공한 한명은 $%d의 상금을 얻습니다.", RUNAWAY_MONEY);
SendClientMessageToAll(COLOR_ORANGE, str);
}
else if(event_value == 1)
{
