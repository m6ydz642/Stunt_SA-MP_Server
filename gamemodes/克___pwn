		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/astop [playerid] [toggle]");
		    return 1;
	    }
	    adoors = strval(stringslice(cmdtext,1));
		data = strval(stringslice(cmdtext,2));
	    if(IsPlayerConnected(adoors)) {
			if(data == 1) {
				TogglePlayerControllable(adoors,1);
			} else if(data == 0) {
				TogglePlayerControllable(adoors,0);
			} else {
				SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/astop [playerid] [toggle]");
			    return 1;
			}
			    SendClientMessage(playerid, COLOR_GREEN, " + OK");
