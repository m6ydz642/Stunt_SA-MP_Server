	SendClientMessage(playerid,COLOR_YELLOW," + 보상: $5000, EXP : 20, 다음 퀘스트 지원금 : $40000");
	quest[playerid] = 8;
	exp[playerid] += 20;
	GetPlayerLevel(playerid);
	GiveMoney(playerid, 5000);
	GiveMoney(playerid, 40000);
