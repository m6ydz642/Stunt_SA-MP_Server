#define MAX_CALL 255
enum CALL_SYSTEM
{
 bool:use, // 사용중인지 체크
 cstate, // cstate = 0 초기화 전 cstate = 1 연결 대기 cstate = 2 통화중
 cid, // 전화건 인간 playerid
 time, // 총 전화 시간
 time2, // 연결중 시간
 bool:check
};

new call_data[MAX_CALL][CALL_SYSTEM];
 


