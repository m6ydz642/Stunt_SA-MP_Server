	udb_UserSetInt(loginid[playerid],"fishFid5",Fishes[playerid][pFid5]);
    udb_UserSetInt(loginid[playerid],"fishFid4",Fishes[playerid][pFid4]);
    udb_UserSetInt(loginid[playerid],"fishFid3",Fishes[playerid][pFid3]);
    udb_UserSetInt(loginid[playerid],"fishFid2",Fishes[playerid][pFid2]);
    udb_UserSetInt(loginid[playerid],"fishFid1",Fishes[playerid][pFid1]);
		}
