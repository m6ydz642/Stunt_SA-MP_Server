public OnPlayerPrivmsg(playerid, recieverid, text[]){
	new receivername[MAX_PLAYER_NAME];
	new sendername[MAX_PLAYER_NAME];

	player_stop[playerid] = 0;
	if(IsPlayerConnected(playerid)){
	    for(new s=0,total=sizeof(Yells);s<total;s++)
    	{
        new pos;
		while((pos = strfind(text,Yells[s],true)) != -1) for(new i = pos, j = pos + strlen(Yells[s]); i < j; i++) text[i] = '*';
		}
		GetPlayerName(recieverid, receivername, sizeof(receivername));
		format(strings, sizeof(strings), " + '문자메세지 보냄' %s (id: %d) ▷ %s", receivername, recieverid, text);
		SendClientMessage(playerid, COLOR_IRC2, strings);
		if(IgnorePlayerlist[recieverid][playerid] == 0) {
			GetPlayerName(playerid, sendername, sizeof(sendername));
			format(strings, sizeof(strings), " + '문자메세지 받음' %s (id: %d) ▷ %s", sendername, playerid, text);
			SendClientMessage(recieverid, COLOR_IRC, strings);
			GiveMoney(playerid, -30);
			}
		}
	chatkick[playerid] ++;
//	format( szSend, 512, "PRIVMSG %i %i %s", playerid, recieverid, text );
//	GUIRCON_SendToAll( szSend );
	return 0;
}
public IsNumeric(const string[])
{
	for (new i = 0, j = strlen(string); i < j; i++) {
	    if (string[i] > '9' || string[i] < '0') {
			return 0;
		}
	}

	return 1;
}

PlaySoundForPlayer(playerid, soundid)
{
  new Float:pX, Float:pY, Float:pZ;
  GetPlayerPos(playerid, pX, pY, pZ);
  PlayerPlaySound(playerid, soundid,pX, pY, pZ);
}

public pingkick() {

  for(new i=0;i<MAX_PLAYERS;i++){
  new playername[MAX_PLAYER_NAME];
  if(god[i] == 1 && IsPlayerInAnyVehicle(i)) SetVehicleHealth(GetPlayerVehicleID(i),1000);
   if(GetPlayerPing(i) >= 900){
    if(connected[i] == 0) {
  	GetPlayerName(i, playername, sizeof(playername));
  	format(strings, sizeof(strings), " + %s(%d) 님은 핑이 900 이상이셔셔 킥되셨습니다. ", playername,i);
  	SendClientMessageToAll(COLOR_GREEN, strings);
  	Kick(i);
    }
   }
  }
}


SoundForAll(sound)
{
    for (new i = 0, j = MAX_PLAYERS; i < j; i ++)
    {
        if (IsPlayerConnected(i)) PlayerPlaySound(i,sound,0.0,0.0,0.0);
    }
}
public autosave()
{
	SendClientMessageToAll(COLOR_GREEN, " + 자동 저장중입니다. 잠시렉이 발생할수 있습니다");
	for (new i = 0; i < MAX_PLAYERS; i++) {
		if (IsPlayerConnected(i) && login_spawn_check[i] == 3){
			loginsystem(i,1);
		}
	}
	SendClientMessageToAll(COLOR_GREEN, " + 자동으로 저장되었습니다");
	SendClientMessageToAll(COLOR_GREEN, " + 웬만해선 돈은 저금하고 필요한돈만 가지고 다니는걸 권장합니다");
	//printf(" + %d번님 정보 저장 완료",playerid);

}

public speedofplayer()
{
	new string[256];
	new diffspeed;
	new playername[MAX_PLAYERS];
	new VehicleClosed[16];
	new h, m, s;
	new AP[26];
	new timestring[255];
	new tran = random(2);

	if(tran == 0) {
		format(timestring,sizeof(timestring),"~b~");
	} else if(tran == 1) {
		format(timestring,sizeof(timestring),"~y~");
	} else if(tran == 2) {
		format(timestring,sizeof(timestring),"~p~");
	}
	for (new i = 0; i < MAX_PLAYERS; i++) {



		if (IsPlayerConnected(i)){
			new Float:x,Float:y,Float:z;
			new Float:distance,value;
			GetPlayerPos(i, x, y, z);
			distance = floatsqroot(floatpower(floatabs(floatsub(x,SavePlayerPos[i][LastX])),2)+floatpower(floatabs(floatsub(y,SavePlayerPos[i][LastY])),2)+floatpower(floatabs(floatsub(z,SavePlayerPos[i][LastZ])),2));
			value = floatround(distance * 11000);
			mspeed[i] = floatround(value/2200);
			if(taxi[i][custom]>=0){
			    if(GetMoney(taxi[i][custom]) <= 0) {
			    RemovePlayerFromVehicle(taxi[i][custom]);
			    } else {
    			expup[i] ++;
	  			if(expup[i] == 15) {
	  			exp[i] ++;
	  			expup[i] = 0;
	  			GetPlayerLevel(i);
	  			}
				GiveMoney(i,70);
				GiveMoney(taxi[i][custom],-10);
				}
			}
			if(IsPlayerInAnyVehicle(patroler)){
				if(IsPlayerDriver(patroler) && VehicleID == 121){
					if(HighwayMission == 1){
					    GetPlayerPos(patroler,X250[patroler],Y250[patroler],Z250[patroler]);
						if(IsPlayerInAnyVehicle(i) && GetPlayerState(i) == PLAYER_STATE_DRIVER){
							if(IsPlayerInSphere(i,X250[patroler],Y250[patroler],Z250[patroler],100) && GetVehicleType(GetPlayerVehicleID(i)) == VT_CAR || GetVehicleType(GetPlayerVehicleID(i)) == VT_HEAVY){
							    if(i != patroler) {
								if(mspeed[i] >= 160 && GetVehicleType(GetPlayerVehicleID(i)) == VT_BIKE || GetVehicleType(GetPlayerVehicleID(i)) == VT_HEAVY || GetVehicleType(GetPlayerVehicleID(i)) == VT_CAR){
									diffspeed=floatround((mspeed[i] - 130)/10);
									if(diffspeed > 19) diffspeed = 19;
									fine[i]=Fines[diffspeed];
									PlayerPlaySound(i,1132,X250[i],Y250[i],Z250[i]);
									PlayerPlaySound(patroler,1132,X250[patroler],Y250[patroler],Z250[patroler]);
									SendClientMessage(i, COLOR_CARDIVE, " + 속도가 너무 빠릅니다!");
									format(strings, sizeof(strings)," + 당신의 속도는 %d km/h 입니다. 당신은 벌금 $%d 를 냈습니다.", mspeed[i], fine[i]/2);
									SendClientMessage(i, COLOR_CARDIVE, strings);
									GetPlayerName(i, playername, sizeof(playername));
									format(strings, sizeof(strings)," + %s 님의 속도는 %d km/h 여서 벌금 $%d 를 냈습니다.", playername, mspeed[i], fine[i]);
									SendClientMessage(patroler, COLOR_CARDIVE, strings);
									GiveMoney(i, -fine[i]/2);
									GiveMoney(patroler, 5000);
									Fined[i]=2;
									}
								}
							}
						}
						if(i == patroler){
							if(HighwayOfficerColor == 0){
								HighwayOfficerColor ++;
								SetPlayerColor(i,COLOR_CYAN);
							} else {
								HighwayOfficerColor --;
								SetPlayerColor(i,COLOR_WHITE);
							}
						}
					}
				}
			}
			gettime(h,m,s);
			if(h>=13) {
				h -= 12;
				AP = "~r~PM";
			} else {
				AP = "~w~AM";
			}
			new htmp[255];
			new mtmp[255];
			new stmp[255];
			if(h<10) {
				format(htmp,sizeof(htmp),"0%d",h);
			} else {
				format(htmp,sizeof(htmp),"%d",h);
			}
			if(m<10) {
				format(mtmp,sizeof(mtmp),"0%d",m);
			} else {
				format(mtmp,sizeof(mtmp),"%d",m);
			}
			if(s<10) {
				format(stmp,sizeof(stmp),"0%d",s);
			} else {
				format(stmp,sizeof(stmp),"%d",s);
			}
			new kmstring[255];
			new tspeed;
			if(IsPlayerInAnyVehicle(i)) {
			    tspeed = floatround(value/2200); //차량기름 값 (돈말고)
	    		if(GetVehicleType(GetPlayerVehicleID(i)) == VT_AIR || GetVehicleType(GetPlayerVehicleID(i)) == VT_SEA || GetVehicleType(GetPlayerVehicleID(i)) == VT_BIKE  || GetVehicleType(GetPlayerVehicleID(i)) == VT_TRAIN){
	    		} else if(tspeed >= 1000) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0;
	    		} else if(tspeed >= 800) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 1;
				} else if(tspeed >= 600) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 1;
				} else if(tspeed >= 500) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 1;
				} else if(tspeed >= 400) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
				} else if(tspeed >= 350) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 340) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 320) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 300) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 270) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 250) {
	   				gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.150;
			    } else if(tspeed >= 220) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 200) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 170) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 150) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 130) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 100) {
	    			gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
				} else if(tspeed >= 90) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
				} else if(tspeed >= 80) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
	   			} else if(tspeed >= 70) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
	    		} else if(tspeed >= 60) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 50) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 30) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed >= 10) {
					gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.115;
			    } else if(tspeed == 0) { //아무런 동작을 하지않고 시동만 켠상태
			    	gastank[GetPlayerVehicleID(i)] = gastank[GetPlayerVehicleID(i)] - 0.10;
			    }
			    if(gastank[GetPlayerVehicleID(i)] < 0) gastank[GetPlayerVehicleID(i)] = 0;
			    
				new tankstring[255];
				new PetrolInTank;
				PetrolInTank=floatround(floatdiv(floatmul(gastank[GetPlayerVehicleID(i)],100),tgastank[GetPlayerVehicleID(i)]));
				//모든차량 의 기름
				//gastank : 차량에 있는 기름
				//tgastank : 사용 기름
				if(admin_checkcount == false) {
					if(lock[GetPlayerVehicleID(i)] == i){
						format(VehicleClosed, sizeof(VehicleClosed), "~r~closed");
					} else {
						format(VehicleClosed, sizeof(VehicleClosed), "~b~open");
					}
					if(tspeed >= 250) {
						format(kmstring,sizeof(kmstring),"~r~");
					} else if(tspeed >= 200) {
					    format(kmstring,sizeof(kmstring),"~y~");
					} else if(tspeed >= 150) {
					    format(kmstring,sizeof(kmstring),"~b~");
					} else if(tspeed >= 20) {
					    format(kmstring,sizeof(kmstring),"~g~");
					} else {
					    format(kmstring,sizeof(kmstring),"~w~");
					}
					if(PetrolInTank >= 75) {
						format(tankstring,sizeof(tankstring),"~b~%d",PetrolInTank);
		   		 	} else if(PetrolInTank >= 41) {
						format(tankstring,sizeof(tankstring),"~g~%d",PetrolInTank);
					} else if(PetrolInTank >= 26) {
						format(tankstring,sizeof(tankstring),"~w~%d",PetrolInTank);
					} else if(PetrolInTank >= 11) {
						format(tankstring,sizeof(tankstring),"~y~%d",PetrolInTank);
					} else if(PetrolInTank == 10) {
						format(tankstring,sizeof(tankstring),"~r~%d",PetrolInTank);
					} else if(PetrolInTank < 10) {
						format(tankstring,sizeof(tankstring),"~r~0%d",PetrolInTank);
					}
				}
				if(PetrolInTank <= 3 && gassystem == 1) {
					PlayerPlaySound(i,1056,SavePlayerPos[i][LastX],SavePlayerPos[i][LastY],SavePlayerPos[i][LastZ]);
				}
				if(admin_checkcount == false) {
					if(GetVehicleType(GetPlayerVehicleID(i)) == VT_AIR || GetVehicleType(GetPlayerVehicleID(i)) == VT_SEA || GetVehicleType(GetPlayerVehicleID(i)) == VT_BIKE || GetVehicleType(GetPlayerVehicleID(i)) == VT_TRAIN){
						format(string, sizeof(string), "~n~~n~~n~~n~~n~~n~~n~~n~%s%d Km/h ~n~ ~g~Doors: %s~n~~g~NOW Time: %s%s %s:%s:%s",kmstring,floatround(value/2200),VehicleClosed,AP,timestring,htmp,mtmp,stmp);
						} else {
						format(string, sizeof(string), "~n~~n~~n~~n~~n~~n~~n~~n~%s%d Km/h ~n~~g~fuel: %s%% ~g~Doors: %s~n~~g~NOW Time: %s%s %s:%s:%s",kmstring,floatround(value/2200),tankstring,VehicleClosed,AP,timestring,htmp,mtmp,stmp);
					}
				}
			} else {
			    if(admin_checkcount == false) {
					if(isJail[i] == 1) {
						format(string, sizeof(string), "~n~~n~~w~%d ~b~Sec Left~n~~n~~n~~n~~n~~n~~n~~n~~g~NOW Time: %s%s %s:%s:%s",JailTime[i],AP,timestring,htmp,mtmp,stmp);
					} else {
						format(string, sizeof(string), "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~g~NOW Time: %s%s %s:%s:%s",AP,timestring,htmp,mtmp,stmp);
//					format(string, sizeof(string), "~w~I ~p~Crazy Server ~w~I ~y~www.crazy.wo.tc ~w~I ~r~NOW Time ~w: ~g~%s%s %s:%s:%s",AP,timestring,htmp,mtmp,stmp);
				//	SendClientMessageToAll(COLOR_RED,string);
					}
				}
			}
			if(cspeed[i] == 1 && admin_checkcount == false) {
		   		GameTextForPlayer(i,string,1000,3);
			}
			//----------------------------------------

	   	    SavePlayerPos[i][LastX] = x;
	        SavePlayerPos[i][LastY] = y;
	        SavePlayerPos[i][LastZ] = z;
		}
	}
}


public chatkicks() {
	new playername[MAX_PLAYER_NAME];
	new string[256];

    for(new i=0; i<MAX_PLAYERS; i++) {
        		    //----------------------------------------
    if(chatkick[i] >= 10) //해당 횟수를 한 만큼 킥
	{
	GetPlayerName(i, playername, MAX_PLAYER_NAME);
	format(string, sizeof(string), " + %d번 %s 님은 도배를 해서 킥 營윱求.", i,playername);
	printf("+ %d번 %s 님은 도배를 해서 킥 營윱求 ", i,playername);
	SendClientMessageToAll(COLOR_RED, string);
	Kick(i);
    }
	chatkick[i] = 0;
    }
}
