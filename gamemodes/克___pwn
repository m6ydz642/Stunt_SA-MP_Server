	}
	
	if(strcmp(cmd, "/joblist", true) == 0) {
		SendClientMessage(playerid, COLOR_RED, " + 직업을 버리고 싶으면 /joblost 를 하면 됩니다. 그러나 $100000의 돈을 내야합니다.");
		SendClientMessage(playerid, COLOR_RED, " + 레벨 3 이상만 가질 수 있어요!!!!!!!!!!!!!!");
		SendClientMessage(playerid, COLOR_WHITE, " +++-------------------- 택시기사 ---------------------+++");
		SendClientMessage(playerid, COLOR_GRAD5, " + 택시기사가 되려면 먼저 $50000 가 필요해. 그리고 정기적으로 보조금 $3000을 받아.");
		SendClientMessage(playerid, COLOR_GRAD5, " + 택시기사가 되면 할 수 있는 명령어를 보려면 /taxihelp 를 쳐봐.");
		SendClientMessage(playerid, COLOR_GRAD5, " + 택시기사 직업은 /taxijob 으로 얻을 수 있어.");
		SendClientMessage(playerid, COLOR_GRAD2, " + 다음 장 보기 : /joblist2");
	return 1;
	}
	if(strcmp(cmd, "/joblist2", true) == 0) {
		SendClientMessage(playerid, COLOR_WHITE, " +++--------------------- 요리사 ----------------------+++");
		SendClientMessage(playerid, COLOR_GRAD5, " + 요리사는 요리도구살 돈 $10000 가 필요해. 요리를 하다보면 완성도가 높은 음식이 나와");
		SendClientMessage(playerid, COLOR_GRAD5, " + 물고기로도 요리를 할 수 있다구!");
		SendClientMessage(playerid, COLOR_GRAD5, " + 택시기사 직업은 /cookjob 으로 얻을 수 있어.");
		SendClientMessage(playerid, COLOR_WHITE, " +++--------------------- 속도단속반 ----------------------+++");
		SendClientMessage(playerid, COLOR_GRAD5, " + 사막마을에 있는 FBI차 같이 생긴 차가 있어. 그걸 타고 과속하는 차량 주위에 가면 찍히는거야.");
		SendClientMessage(playerid, COLOR_GRAD5, " + 보수는 적지만 월급이 $50000 이라구! /speedjob 으로 얻을 수 있어. $70000 이 필요하단다~~");
		SendClientMessage(playerid, COLOR_WHITE, " +++--------------------- 복서 ----------------------+++");
		SendClientMessage(playerid, COLOR_GRAD5, " + 대단한 체력과 싸움의 기술이 필요하지! 링위에 올라가서 이긴다면 돈과 명예를 한꺼번에 얻을 수 있어!");
		SendClientMessage(playerid, COLOR_GRAD5, " + 월급도 없고, 초기자금도 필요없어! 되고싶으면 /boxjob 을 치고, 상대와 싸우고싶다면 /fight 를 입력하면돼!");
	return 1;
	}

	if(strcmp(cmd, "/buyeat", true) == 0)
	{
	    if(IsPlayerConnected(playerid))
	    {
			if(getCheckpointType(playerid) == CP_BANK || getCheckpointType(playerid) == CP_BANK3 || getCheckpointType(playerid) == CP_BANK4 || getCheckpointType(playerid) == CP_BANK5 ||
			getCheckpointType(playerid) ==  CP_RETIRE || getCheckpointType(playerid) == CP_BANK6)
			{
			tmp = strtok(cmdtext, idx);
			if(!strlen(tmp))
			{
				SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /buyeat [아이템번호]");
				SendClientMessage(playerid, COLOR_GRAD5, " + 1: 통닭재료 : $100     2: 햄버거재료 : $100");
				SendClientMessage(playerid, COLOR_GRAD5, " + 3: 피자재료 : $100");
				return 1;
			}
			new item = strval(tmp);
			if (item == 1 && GetMoney(playerid) > 100)
			{
			    if(Groceries[playerid][pChickens] >= 3)
			    {
			        SendClientMessage(playerid, COLOR_GREY, " + 당신은 이미 통닭 재료를 많이 가지고 있습니다.");
			        return 1;
			    }
				    GiveMoney(playerid, - 100);
					format(string, sizeof(string), "~r~-$%d", 100);
					GameTextForPlayer(playerid, string, 5000, 1);

				new Float:X, Float:Y, Float:Z;
				GetPlayerPos(playerid, X, Y, Z);
				PlayerPlaySound(playerid, 1052, X, Y, Z);
				format(string, sizeof(string), " + 통닭 재료를 샀습니다.");
				SendClientMessage(playerid, COLOR_GRAD4, string);
				new rand = random(15);
				Groceries[playerid][pChickens] += 1;
				Groceries[playerid][pChicken] += rand;
				return 1;
			}
			else if (item == 2 && GetMoney(playerid) > 100)
			{
			    if(Groceries[playerid][pHamburgers] >= 3)
			    {
			        SendClientMessage(playerid, COLOR_GREY, " + 당신은 이미 햄버거를 많이 가지고 있습니다.");
			        return 1;
			    }


			    GiveMoney(playerid, - 100);
			    new Float:X, Float:Y, Float:Z;
				GetPlayerPos(playerid, X, Y, Z);
				PlayerPlaySound(playerid, 1052, X, Y, Z);
				format(string, sizeof(string), " + 햄버거 재료를 샀습니다.");
				SendClientMessage(playerid, COLOR_GRAD4, string);
				format(string, sizeof(string), "~r~-$%d", 100);
				GameTextForPlayer(playerid, string, 5000, 1);
				new rand = random(15);
				Groceries[playerid][pHamburgers] += 1;
				Groceries[playerid][pHamburger] += rand;
				return 1;
			}
			else if (item == 3 && GetMoney(playerid) > 100)
			{
			    if(Groceries[playerid][pPizzas] >= 3)
			    {
			        SendClientMessage(playerid, COLOR_GREY, " + 당신은 이미 피자를 많이 가지고 있습니다.");
			        return 1;
			    }
			    GiveMoney(playerid, - 100);
				format(string, sizeof(string), "~r~-$%d", 100);
				GameTextForPlayer(playerid, string, 5000, 1);
				new Float:X, Float:Y, Float:Z;
				GetPlayerPos(playerid, X, Y, Z);
				PlayerPlaySound(playerid, 1052, X,Y,Z);
				format(string, sizeof(string), " + 피자 재료를 샀습니다.");
				SendClientMessage(playerid, COLOR_GRAD4, string);
				new rand = random(15);
				Groceries[playerid][pPizzas] += 1;
				Groceries[playerid][pPizza] += rand;
				return 1;
			}
			else
			{
				SendClientMessage(playerid, COLOR_WHITE, " + 돈이 없습니다.");
			}
			} else SendClientMessage(playerid, COLOR_WHITE, " + 편의점에서 가능합니다.");
		}
		return 1;
	}

	if(strcmp(cmd, "/cookhelp", true) == 0)
	{
	    if(IsPlayerConnected(playerid))
	    {
			SendClientMessage(playerid, COLOR_GREEN,"_______________________________________");
			SendClientMessage(playerid, COLOR_GRAD3,"*** COOKING *** /cook - 요리하기   /cooked - 요리 한것 보기");
			SendClientMessage(playerid, COLOR_GRAD3,"*** COOKING *** /eat - 음식 먹기");
		}
		return 1;
	}
if(strcmp(cmd, "/selleat", true) == 0)
	{
	    if(IsPlayerConnected(playerid))
	    {
			new x_nr[256];
			x_nr = strtok(cmdtext, idx);
			if(!strlen(x_nr)) {
				SendClientMessage(playerid, COLOR_WHITE, "|____________________ 팔기 ___________________|");
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /selleat [팔것]");
		  		SendClientMessage(playerid, COLOR_GREY, "팔것: Fish, Cook(요리는 다른사람게 줄 수 밖에 없습니다.)");
				SendClientMessage(playerid, COLOR_WHITE, "|_____________________________________________|");
				return 1;
			}
			if(getCheckpointType(playerid) == CP_BANK || getCheckpointType(playerid) == CP_BANK3 || getCheckpointType(playerid) == CP_BANK4 || getCheckpointType(playerid) == CP_BANK5 ||
			getCheckpointType(playerid) ==  CP_RETIRE || getCheckpointType(playerid) == CP_BANK6) {
		    if(strcmp(x_nr,"fish",true) == 0)
			{
			    tmp = strtok(cmdtext, idx);
				if(!strlen(tmp)) {
					SendClientMessage(playerid, COLOR_WHITE, "USAGE: /selleat fish [fish]");
					return 1;
				}
				new price;
				new fishid = strval(tmp);
				if(fishid < 1 || fishid > 5) { SendClientMessage(playerid, COLOR_GREY, " + 물고기는 1~5번 바구니에 있습니다."); return 1; }
				else if(fishid == 1 && Fishes[playerid][pWeight1] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 1번 바구니에 물고기가 없습니다."); return 1; }
				else if(fishid == 2 && Fishes[playerid][pWeight2] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 2번 바구니에 물고기가 없습니다."); return 1; }
				else if(fishid == 3 && Fishes[playerid][pWeight3] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 3번 바구니에 물고기가 없습니다."); return 1; }
				else if(fishid == 4 && Fishes[playerid][pWeight4] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 4번 바구니에 물고기가 없습니다."); return 1; }
				else if(fishid == 5 && Fishes[playerid][pWeight5] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 5번 바구니에 물고기가 없습니다."); return 1; }
				switch (fishid)
				{
				    case 1:
				    {
				        if(Fishes[playerid][pWeight1] < 20)
				        {
				            SendClientMessage(playerid, COLOR_WHITE, " + 1번 바구니의 물고기는 20cm 가 넘지 못하여 팔 수 없습니다.");
				            return 1;
				        }
                        price = FishCost(playerid, Fishes[playerid][pFid1]);
                        price = price * Fishes[playerid][pWeight1];

				            new skill = price / 100;
				            new payout = (skill)*(5);
				            price += payout;

                        GameTextForPlayer(playerid, "~g~Fish~n~~r~Sold", 3000, 1);
                        format(string, sizeof(string), " + 당신은 %s 물고기 %dcm 짜리를 $%d 를 받고 팔았습니다.", Fishes[playerid][pFish1],Fishes[playerid][pWeight1],price);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						GiveMoney(playerid, price);
						ClearFishID(playerid, 1);
				    }
				    case 2:
				    {
				        if(Fishes[playerid][pWeight2] < 20)
				        {
				            SendClientMessage(playerid, COLOR_WHITE, " + 2번 바구니의 물고기는 20cm 가 넘지 못하여 팔 수 없습니다.");
				            return 1;
				        }
            			price = FishCost(playerid, Fishes[playerid][pFid2]);
                        price = price  + Fishes[playerid][pWeight2];

				            new skill = price / 100;
				            new payout = (skill)*(5);
				            price += payout;

                        GameTextForPlayer(playerid, "~g~Fish~n~~r~Sold", 3000, 1);
                        format(string, sizeof(string), " + 당신은 %s 물고기 %dcm 짜리를 $%d 를 받고 팔았습니다.", Fishes[playerid][pFish2],Fishes[playerid][pWeight2],price);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						GiveMoney(playerid, price);
						ClearFishID(playerid, 2);
				    }
				    case 3:
				    {
				        if(Fishes[playerid][pWeight3] < 20)
				        {
				            SendClientMessage(playerid, COLOR_WHITE, " + 3번 바구니의 물고기는 20cm 가 넘지 못하여 팔 수 없습니다.");
				            return 1;
				        }
            			price = FishCost(playerid, Fishes[playerid][pFid3]);
                        price = price * Fishes[playerid][pWeight3];

				            new skill = price / 100;
				            new payout = (skill)*(5);
				            price += payout;

                        GameTextForPlayer(playerid, "~g~Fish~n~~r~Sold", 3000, 1);
                        format(string, sizeof(string), " + 당신은 %s 물고기 %dcm 짜리를 $%d 를 받고 팔았습니다.", Fishes[playerid][pFish3],Fishes[playerid][pWeight3],price);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						GiveMoney(playerid, price);
						ClearFishID(playerid, 3);
				    }
				    case 4:
				    {
				        if(Fishes[playerid][pWeight4] < 20)
				        {
				            SendClientMessage(playerid, COLOR_WHITE, " + 4번 바구니의 물고기는 20cm 가 넘지 못하여 팔 수 없습니다.");
				            return 1;
				        }
            			price = FishCost(playerid, Fishes[playerid][pFid4]);
                        price = price * Fishes[playerid][pWeight4];

				            new skill = price / 100;
				            new payout = (skill)*(5);
				            price += payout;

                        GameTextForPlayer(playerid, "~g~Fish~n~~r~Sold", 3000, 1);
                        format(string, sizeof(string), " + 당신은 %s 물고기 %dcm 짜리를 $%d 를 받고 팔았습니다.", Fishes[playerid][pFish4],Fishes[playerid][pWeight4],price);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						GiveMoney(playerid, price);
						ClearFishID(playerid, 4);
				    }
				    case 5:
				    {
				        if(Fishes[playerid][pWeight5] < 20)
				        {
				            SendClientMessage(playerid, COLOR_WHITE, " + 5번 바구니의 물고기는 20cm 가 넘지 못하여 팔 수 없습니다.");
				            return 1;
				        }
            			price = FishCost(playerid, Fishes[playerid][pFid5]);
                        price = price * Fishes[playerid][pWeight5];

				            new skill = price / 100;
				            new payout = (skill)*(5);
				            price += payout;

                        GameTextForPlayer(playerid, "~g~Fish~n~~r~Sold", 3000, 1);
                        format(string, sizeof(string), " + 당신은 %s 물고기 %dcm 짜리를 $%d 를 받고 팔았습니다.", Fishes[playerid][pFish5],Fishes[playerid][pWeight5],price);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						GiveMoney(playerid, price);
						ClearFishID(playerid, 5);
				    }
				}
				Fishes[playerid][pLastFish] = 0;
				Fishes[playerid][pFishID] = 0;

				udb_UserSetInt(loginid[playerid],"fishWeight5",Fishes[playerid][pWeight5]);
    udb_UserSetInt(loginid[playerid],"fishWeight4",Fishes[playerid][pWeight4]);
    udb_UserSetInt(loginid[playerid],"fishWeight3",Fishes[playerid][pWeight3]);
    udb_UserSetInt(loginid[playerid],"fishWeight2",Fishes[playerid][pWeight2]);
    udb_UserSetInt(loginid[playerid],"fishWeight1",Fishes[playerid][pWeight1]);
	udb_UserSetInt(loginid[playerid],"fishFid5",Fishes[playerid][pFid5]);
    udb_UserSetInt(loginid[playerid],"fishFid4",Fishes[playerid][pFid4]);
    udb_UserSetInt(loginid[playerid],"fishFid3",Fishes[playerid][pFid3]);
    udb_UserSetInt(loginid[playerid],"fishFid2",Fishes[playerid][pFid2]);
    udb_UserSetInt(loginid[playerid],"fishFid1",Fishes[playerid][pFid1]);

				return 1;
				}
				else if(strcmp(x_nr,"cook",true) == 0)
			{
			    tmp = strtok(cmdtext, idx);
			    if(!strlen(tmp)) {
					SendClientMessage(playerid, COLOR_WHITE, "USAGE: /selleat cook [cooknumber]");
					return 1;
				}
				new cookid = strval(tmp);
				if(cookid < 1 || cookid > 5) { SendClientMessage(playerid, COLOR_GREY, " + 음식은 1~5까지의 바구니에 있습니다."); return 1; }
				else if(cookid == 1 && Cooking[playerid][pCWeight1] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 1번 바구니에 음식이 없습니다."); return 1; }
				else if(cookid == 2 && Cooking[playerid][pCWeight2] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 2번 바구니에 음식이 없습니다."); return 1; }
				else if(cookid == 3 && Cooking[playerid][pCWeight3] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 3번 바구니에 음식이 없습니다."); return 1; }
				else if(cookid == 4 && Cooking[playerid][pCWeight4] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 4번 바구니에 음식이 없습니다."); return 1; }
				else if(cookid == 5 && Cooking[playerid][pCWeight5] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 5번 바구니에 음식이 없습니다."); return 1; }
				        switch(cookid)
				        {
		    				case 1:
						    {
						        new price;
		                        price = Cooking[playerid][pCWeight1] * 50 * PlayerInfo[playerid][pCookSkill];

		                        GameTextForPlayer(playerid, "~g~Food~n~~r~Sold", 3000, 1);
		                        format(string, sizeof(string), " + 당신은 %s 음식, 완성도:%d%% 짜리를 $%d 를 받고 팔았습니다.",Cooking[playerid][pCook1],Cooking[playerid][pCWeight1],price);
								SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
								GiveMoney(playerid, price);
						    }
						    case 2:
						    {
						        new price;
		                        price = Cooking[playerid][pCWeight2] * 50 * PlayerInfo[playerid][pCookSkill];


		                        GameTextForPlayer(playerid, "~g~Food~n~~r~Sold", 3000, 1);
		                        format(string, sizeof(string), " + 당신은 %s 음식, 완성도:%d%% 짜리를 $%d 를 받고 팔았습니다.",Cooking[playerid][pCook2],Cooking[playerid][pCWeight2],price);
								SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
								GiveMoney(playerid, price);
						    }
						    case 3:
						    {
						        new price;
								price = Cooking[playerid][pCWeight3] * 50 * PlayerInfo[playerid][pCookSkill];


		                        GameTextForPlayer(playerid, "~g~Food~n~~r~Sold", 3000, 1);
		                        format(string, sizeof(string), " + 당신은 %s 음식, 완성도:%d%% 짜리를 $%d 를 받고 팔았습니다.",Cooking[playerid][pCook3],Cooking[playerid][pCWeight3],price);
								SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
								GiveMoney(playerid, price);
						    }
						    case 4:
						    {
						        new price;
						        price = Cooking[playerid][pCWeight4] * 50 * PlayerInfo[playerid][pCookSkill];


		                        GameTextForPlayer(playerid, "~g~Food~n~~r~Sold", 3000, 1);
		                        format(string, sizeof(string), " + 당신은 %s 음식, 완성도:%d%% 짜리를 $%d 를 받고 팔았습니다.",Cooking[playerid][pCook4],Cooking[playerid][pCWeight4],price);
								SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
								GiveMoney(playerid, price);
						    }
						    case 5:
						    {
						        new price;
						        price = Cooking[playerid][pCWeight5] * 50 * PlayerInfo[playerid][pCookSkill];


		                        GameTextForPlayer(playerid, "~g~Food~n~~r~Sold", 3000, 1);
		                        format(string, sizeof(string), " + 당신은 %s 음식, 완성도:%d%% 짜리를 $%d 를 받고 팔았습니다.",Cooking[playerid][pCook5],Cooking[playerid][pCWeight5],price);
								SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
								GiveMoney(playerid, price);
							}
				        }//switch

				        ClearCookingID(playerid, cookid);
	udb_UserSetInt(loginid[playerid],"pChicken",Groceries[playerid][pChicken]);
    udb_UserSetInt(loginid[playerid],"pChickens",Groceries[playerid][pChickens]);
    udb_UserSetInt(loginid[playerid],"pHamburger",Groceries[playerid][pHamburger]);
    udb_UserSetInt(loginid[playerid],"pHamburgers",Groceries[playerid][pHamburgers]);
    udb_UserSetInt(loginid[playerid],"pPizza",Groceries[playerid][pPizza]);
    udb_UserSetInt(loginid[playerid],"pPizzas",Groceries[playerid][pPizzas]);

	udb_UserSetInt(loginid[playerid],"cookWeight5",Cooking[playerid][pCWeight5]);
    udb_UserSetInt(loginid[playerid],"cookWeight4",Cooking[playerid][pCWeight4]);
    udb_UserSetInt(loginid[playerid],"cookWeight3",Cooking[playerid][pCWeight3]);
    udb_UserSetInt(loginid[playerid],"cookWeight2",Cooking[playerid][pCWeight2]);
    udb_UserSetInt(loginid[playerid],"cookWeight1",Cooking[playerid][pCWeight1]);
	udb_UserSetInt(loginid[playerid],"cookID5",Cooking[playerid][pCookID5]);
    udb_UserSetInt(loginid[playerid],"cookID4",Cooking[playerid][pCookID4]);
    udb_UserSetInt(loginid[playerid],"cookID3",Cooking[playerid][pCookID3]);
    udb_UserSetInt(loginid[playerid],"cookID2",Cooking[playerid][pCookID2]);
    udb_UserSetInt(loginid[playerid],"cookID1",Cooking[playerid][pCookID1]);
	udb_UserSetInt(loginid[playerid],"pCookSkill",PlayerInfo[playerid][pCookSkill]);
	
	udb_UserSet(loginid[playerid],"cook5",Cooking[playerid][pCook5]);
    udb_UserSet(loginid[playerid],"cook4",Cooking[playerid][pCook4]);
    udb_UserSet(loginid[playerid],"cook3",Cooking[playerid][pCook3]);
    udb_UserSet(loginid[playerid],"cook2",Cooking[playerid][pCook2]);
    udb_UserSet(loginid[playerid],"cook1",Cooking[playerid][pCook1]);
	            return 1;
				} else SendClientMessage(playerid, COLOR_GRAD2, " + 편의점에서 팔 수 있습니다.");
			}
	}
}
if(strcmp(cmd,"/eat",true)==0)
    {
        if(IsPlayerConnected(playerid))
	    {
	        tmp = strtok(cmdtext, idx);
			if(!strlen(tmp)) {
				SendClientMessage(playerid, COLOR_WHITE, "USAGE: /eat [cook number]");
				return 1;
			}
			new cookid = strval(tmp);
			if(cookid < 1 || cookid > 5) { SendClientMessage(playerid, COLOR_GREY, " + 음식은 1~5까지의 바구니에 있습니다."); return 1; }
			else if(cookid == 1 && Cooking[playerid][pCWeight1] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 1번 바구니에 음식이 없습니다."); return 1; }
			else if(cookid == 2 && Cooking[playerid][pCWeight2] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 2번 바구니에 음식이 없습니다."); return 1; }
			else if(cookid == 3 && Cooking[playerid][pCWeight3] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 3번 바구니에 음식이 없습니다."); return 1; }
			else if(cookid == 4 && Cooking[playerid][pCWeight4] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 4번 바구니에 음식이 없습니다."); return 1; }
			else if(cookid == 5 && Cooking[playerid][pCWeight5] < 1) { SendClientMessage(playerid, COLOR_GREY, " + 5번 바구니에 음식이 없습니다."); return 1; }
            GetPlayerName(playerid, sendername, sizeof(sendername));
			new Float:health;
			if(GetPlayerHealth(playerid, health) < 40)
			{
			    new hp = 0;
			    switch (cookid)
			    {
					case 1:
					{
					    SetPlayerHealth(playerid, health + (Cooking[playerid][pCWeight1] / 3));
					    hp = Cooking[playerid][pCWeight1];
					    format(string, sizeof(string), " + %s 님이 %s 를 드셨습니다.", sendername, Cooking[playerid][pCook1]);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					    format(string, sizeof(string), " + 당신은 %s 를 먹어 %d체력를 더 회복하셨습니다.",Cooking[playerid][pCook1], hp);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					}
					case 2:
					{
					    SetPlayerHealth(playerid, health + (Cooking[playerid][pCWeight2] / 3));
					    hp = Cooking[playerid][pCWeight2];
					    format(string, sizeof(string), " + %s 님이 %s 를 드셨습니다.", sendername, Cooking[playerid][pCook2]);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					    format(string, sizeof(string), " + 당신은 %s 를 먹어 %d체력를 더 회복하셨습니다.",Cooking[playerid][pCook2], hp);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					}
					case 3:
					{
					    SetPlayerHealth(playerid, health + (Cooking[playerid][pCWeight3] / 3));
					    hp = Cooking[playerid][pCWeight3];
					    format(string, sizeof(string), " + %s 님이 %s 를 드셨습니다.", sendername, Cooking[playerid][pCook3]);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					    format(string, sizeof(string), " + 당신은 %s 를 먹어 %d체력를 더 회복하셨습니다.",Cooking[playerid][pCook3], hp);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					}
					case 4:
					{
					    SetPlayerHealth(playerid, health + (Cooking[playerid][pCWeight4] / 3));
					    hp = Cooking[playerid][pCWeight4];
					    format(string, sizeof(string), " + %s 님이 %s 를 드셨습니다.", sendername, Cooking[playerid][pCook4]);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					    format(string, sizeof(string), " + 당신은 %s 를 먹어 %d체력를 더 회복하셨습니다.",Cooking[playerid][pCook4], hp);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					}
					case 5:
					{
					    SetPlayerHealth(playerid, health + (Cooking[playerid][pCWeight5] / 3));
					    hp = Cooking[playerid][pCWeight5];
					    format(string, sizeof(string), " + %s 님이 %s 를 드셨습니다.", sendername, Cooking[playerid][pCook5]);
						ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					    format(string, sizeof(string), " + 당신은 %s 를 먹어 %d체력를 더 회복하셨습니다.",Cooking[playerid][pCook5], hp);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
					}
			    }
				ClearCookingID(playerid, cookid);
				    udb_UserSetInt(loginid[playerid],"pChicken",Groceries[playerid][pChicken]);
    udb_UserSetInt(loginid[playerid],"pChickens",Groceries[playerid][pChickens]);
    udb_UserSetInt(loginid[playerid],"pHamburger",Groceries[playerid][pHamburger]);
    udb_UserSetInt(loginid[playerid],"pHamburgers",Groceries[playerid][pHamburgers]);
    udb_UserSetInt(loginid[playerid],"pPizza",Groceries[playerid][pPizza]);
    udb_UserSetInt(loginid[playerid],"pPizzas",Groceries[playerid][pPizzas]);

	udb_UserSet(loginid[playerid],"cook5",Cooking[playerid][pCook5]);
    udb_UserSet(loginid[playerid],"cook4",Cooking[playerid][pCook4]);
    udb_UserSet(loginid[playerid],"cook3",Cooking[playerid][pCook3]);
    udb_UserSet(loginid[playerid],"cook2",Cooking[playerid][pCook2]);
    udb_UserSet(loginid[playerid],"cook1",Cooking[playerid][pCook1]);

	udb_UserSetInt(loginid[playerid],"cookWeight5",Cooking[playerid][pCWeight5]);
    udb_UserSetInt(loginid[playerid],"cookWeight4",Cooking[playerid][pCWeight4]);
    udb_UserSetInt(loginid[playerid],"cookWeight3",Cooking[playerid][pCWeight3]);
    udb_UserSetInt(loginid[playerid],"cookWeight2",Cooking[playerid][pCWeight2]);
    udb_UserSetInt(loginid[playerid],"cookWeight1",Cooking[playerid][pCWeight1]);
	udb_UserSetInt(loginid[playerid],"cookID5",Cooking[playerid][pCookID5]);
    udb_UserSetInt(loginid[playerid],"cookID4",Cooking[playerid][pCookID4]);
    udb_UserSetInt(loginid[playerid],"cookID3",Cooking[playerid][pCookID3]);
    udb_UserSetInt(loginid[playerid],"cookID2",Cooking[playerid][pCookID2]);
    udb_UserSetInt(loginid[playerid],"cookID1",Cooking[playerid][pCookID1]);
	udb_UserSetInt(loginid[playerid],"pCookSkill",PlayerInfo[playerid][pCookSkill]);
			}
			else
			{
			    SendClientMessage(playerid, COLOR_GREY, " + 당신체력이 괜찮습니다.");
			    return 1;
			}
	    }
	    return 1;
	}
 	if(strcmp(cmd,"/cook",true)==0)
    {
      if(MyJob[playerid] == JOB_COOK) {
        if(IsPlayerConnected(playerid))
	    {
	        if(IsAtCookPlace(playerid))
	        {
	            new x_nr[256];
	            x_nr = strtok(cmdtext, idx);
				if(!strlen(x_nr)) {
					SendClientMessage(playerid, COLOR_WHITE, " USAGE: /cook [name]");
					SendClientMessage(playerid, COLOR_WHITE, " + 음식이름: lastfish, Fish, chicken, HBG, Pizza.");
                    SendClientMessage(playerid, COLOR_WHITE, " + HBG 는 햄버거입니다.");
					return 1;
				}
				if(strcmp(x_nr,"lastfish",true) == 0)
				{
				    if(Fishes[playerid][pLastFish] > 0)
	        		{
					    new found = 0;
					    if(Cooking[playerid][pCWeight1] == 0)
					    {
						    format(string, sizeof(string), "%s", FishNames[Fishes[playerid][pFishID]]);
						    strmid(Cooking[playerid][pCook1], string, 0, strlen(string), 255);
						    Cooking[playerid][pCWeight1] = Fishes[playerid][pLastWeight];
						    Cooking[playerid][pCookID1] = Fishes[playerid][pFishID];
						    format(string, sizeof(string), " + 당신은 %s(%d%%완성도) 짜리를 요리했습니다.",Cooking[playerid][pCook1],Cooking[playerid][pCWeight1]);
						    SendClientMessage(playerid,COLOR_LIGHTBLUE, string);
						    found = 1;
					    }
					    else if(Cooking[playerid][pCWeight2] == 0)
					    {
						    format(string, sizeof(string), "%s", FishNames[Fishes[playerid][pFishID]]);
						    strmid(Cooking[playerid][pCook2], string, 0, strlen(string), 255);
						    Cooking[playerid][pCWeight2] = Fishes[playerid][pLastWeight];
						    Cooking[playerid][pCookID2] = Fishes[playerid][pFishID];
