	if(strcmp(cmd,"/스탯",true) == 0) {
	format(string, sizeof(string), " + 당신의 전재산: $%d",bank[playerid]+GetMoney(playerid));
	SendClientMessage(playerid, COLOR_GREEN, string);
	format(string, sizeof(string), " + 당신의 레벨 : [%d], 당신의 EXP (경험치) : [%d/%d]Point, 레벨업에 필요한 포인트 : [%d]Point",GetPlayerLevel(playerid),exp[playerid],GetPlayerUPPoint(playerid),GetPlayerUPPoint(playerid)-exp[playerid]);
	SendClientMessage(playerid, COLOR_GREEN, string);
	SendClientMessage(playerid, COLOR_WHITE, "--------------------------------------------");
 	  new level = PlayerInfo[playerid][pFishSkill];
	  if(level >= 0 && level <= 50) { SendClientMessage(playerid, COLOR_YELLOW, " + 낚시스킬 : [1]."); format(string, sizeof(string), " + 낚시 스킬을 올리려면 %d 포인트가 더 필요합니다.", 50 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 51 && level <= 250) { SendClientMessage(playerid, COLOR_YELLOW, " + 낚시스킬 : [2]."); format(string, sizeof(string), " + 낚시 스킬을 올리려면 %d 포인트가 더 필요합니다.", 250 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 251 && level <= 500) { SendClientMessage(playerid, COLOR_YELLOW, " + 낚시스킬 : [3]."); format(string, sizeof(string), " + 낚시 스킬을 올리려면 %d 포인트가 더 필요합니다.", 500 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 501 && level <= 999) { SendClientMessage(playerid, COLOR_YELLOW, " + 낚시스킬 : [4]."); format(string, sizeof(string), " + 낚시 스킬을 올리려면 %d 포인트가 더 필요합니다.", 1000 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 1000) { SendClientMessage(playerid, COLOR_YELLOW, " + 낚시스킬 : [5]."); }
	  if(MyJob[playerid] == JOB_COOK) {
   	  level = PlayerInfo[playerid][pCookSkill];
	  if(level >= 0 && level <= 50) { SendClientMessage(playerid, COLOR_YELLOW, " + 요리스킬 : [1]."); format(string, sizeof(string), " + 요리 스킬을 올리려면 %d 포인트가 더 필요합니다.", 50 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 51 && level <= 250) { SendClientMessage(playerid, COLOR_YELLOW, " + 요리스킬 : [2]."); format(string, sizeof(string), " + 요리 스킬을 올리려면 %d 포인트가 더 필요합니다.", 250 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 251 && level <= 500) { SendClientMessage(playerid, COLOR_YELLOW, " + 요리스킬 : [3]."); format(string, sizeof(string), " + 요리 스킬을 올리려면 %d 포인트가 더 필요합니다.", 500 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 501 && level <= 999) { SendClientMessage(playerid, COLOR_YELLOW, " + 요리스킬 : [4]."); format(string, sizeof(string), " + 요리 스킬을 올리려면 %d 포인트가 더 필요합니다.", 1000 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 1000) { SendClientMessage(playerid, COLOR_YELLOW, " + 요리스킬 : [5]."); }
	  }
	  if(MyJob[playerid] == JOB_BOXER) {
      level = PlayerInfo[playerid][pBoxSkill];
	  if(level >= 0 && level <= 50) { SendClientMessage(playerid, COLOR_YELLOW, " + 복싱스킬 : [1]."); format(string, sizeof(string), " + 복싱 스킬을 올리려면 %d 포인트가 더 필요합니다.", 50 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 51 && level <= 100) { SendClientMessage(playerid, COLOR_YELLOW, " + 복싱스킬 : [2]."); format(string, sizeof(string), " + 복싱 스킬을 올리려면 %d 포인트가 더 필요합니다.", 250 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 101 && level <= 200) { SendClientMessage(playerid, COLOR_YELLOW, " + 복싱스킬 : [3]"); format(string, sizeof(string), " + 복싱 스킬을 올리려면 %d 포인트가 더 필요합니다.", 500 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 201 && level <= 400) { SendClientMessage(playerid, COLOR_YELLOW, " + 복싱스킬 : [4]."); format(string, sizeof(string), " + 복싱 스킬을 올리려면 %d 포인트가 더 필요합니다.", 1000 - level); SendClientMessage(playerid, COLOR_YELLOW, string); }
	  else if(level >= 401) { SendClientMessage(playerid, COLOR_YELLOW, " + 복싱스킬 : [5]"); }
	  format(string, sizeof(string), " + 전적 : %d승 %d패",PlayerInfo[TBoxer][pWins],PlayerInfo[TBoxer][pLoses]);
	  SendClientMessage(playerid, COLOR_YELLOW, string);
	  }
	  SendClientMessage(playerid, COLOR_WHITE, "--------------------------------------------");
	return 1;
	}


	if(strcmp(cmdtext,"/work",true)==0)
	{
	if(worker[playerid]!=0)
	    {
	    worker[playerid]=0;
		SendClientMessage(playerid,COLOR_ORANGERED," + 일을 포기했습니다!");
		return 1;
		}
	switch(getCheckpointType(playerid))
	    {
	    case CP_HUNTER:
	        {
			if(IsPlayerInCheckpoint(playerid))
			    {
				worker[playerid]=2;
				SendClientMessage(playerid,COLOR_YELLOW," + 물건을 받았습니다. 바위산 근처 공사장에 가져다 주십시오.");
			    }
	        }
	    case CP_JUNKYARD:
	    	{
	    	if(IsPlayerInCheckpoint(playerid))
	    	    {
	    	    worker[playerid]=1;
	    	    SendClientMessage(playerid,COLOR_YELLOW," + 물건을 받았습니다. 라스벤츄라 근처 공사장에 가져다 주십시오.");
	    	    }
	    	}
	    }
	return 1;
	}

	GetPlayerName(playerid, name, MAX_PLAYER_NAME);

	if(strcmp(cmd, "/salaries", true) == 0)
 	{
  	new x;

  	SendClientMessage(playerid, COLOR_ORANGE, " + 월급 지급 정보:");
     for(new i=0; i < MAX_PLAYERS; i++) {
   	if(IsPlayerConnected(i) && salary[playerid][i] > 0) {
    GetPlayerName(i, giveplayer, sizeof(giveplayer));
    format(string, sizeof(string), "%s%s(%d): $%d", string,giveplayer,i,salary[playerid][i]);

    x++;
    if(x > 3) {
        SendClientMessage(playerid, COLOR_YELLOW, string);
        x = 0;
     format(string, sizeof(string), " ");
    } else {
     format(string, sizeof(string), "%s, ", string);
    }
   }
  }

  if(x <= 3 && x > 0) {
   string[strlen(string)-2] = '.';
      SendClientMessage(playerid, COLOR_YELLOW, string);
  }

  return 1;
 }
	if(strcmp(cmd, "/salary", true) == 0) {

 	tmp = strtok(cmdtext, idx);
	if(!strlen(tmp)) {
	SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /salary [줄ID] [금액]");
	SendClientMessage(playerid, COLOR_IVORY, " + 월급을 주지 않으려면 금액을 0 으로 하십시오.");
	return 1;
	}
	giveplayerid = strval(tmp);
 	tmp = strtok(cmdtext, idx);
  	if(!strlen(tmp)) {
   	SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /salary [줄ID] [금액]");
    SendClientMessage(playerid, COLOR_IVORY, " + 월급을 주지 않으려면 금액을 0 으로 하십시오.");
   	return 1;
  	}
  	moneys = strval(tmp);
	if(moneys <= -1) {
	SendClientMessage(playerid, COLOR_WHITE, " + 월급은 $0 이상 줄 수 있습니다.");
   	return 1;
  	}
  	if(playerid == giveplayerid) {
  	SendClientMessage(playerid, COLOR_WHITE, " + 월급을 자기한테 줘서 뭐하려구요.");
   	return 1;
  	}
  	if(!IsPlayerConnected(giveplayerid)) {
  	SendClientMessage(playerid, COLOR_WHITE, " + 그 분은 접속하시지 않았습니다.");
   	return 1;
  	}
 	GetPlayerName(giveplayerid, giveplayername, sizeof(giveplayername));
 	GetPlayerName(playerid, playername, sizeof(playername));
  	if(moneys == 0) {
	format(string, sizeof(string), " + %s(%d)님 에게 월급을 지급하지 않기로 했습니다.",giveplayername,giveplayerid);
	SendClientMessage(playerid, COLOR_CYAN, string);
	format(string, sizeof(string), " + %s(%d)님 에게 이제부터 월급을 받지 않습니다.",playername,playerid);
	SendClientMessage(giveplayerid, COLOR_GREEN, string);
	return 1;
	}


	format(string, sizeof(string), " + %s(%d)님 에게 10분씩 드릴 월급은 $%d 입니다.",giveplayername,giveplayerid,moneys);
	SendClientMessage(playerid, COLOR_CYAN, string);
	format(string, sizeof(string), " + %s(%d)님 에게 10분씩 받을 월급은 $%d 입니다.",playername,playerid,moneys);
	SendClientMessage(giveplayerid, COLOR_GREEN, string);


	salary[playerid][giveplayerid] = moneys;
	return 1;
	}
