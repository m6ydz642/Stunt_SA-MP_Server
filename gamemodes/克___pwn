	}
	if (strcmp("/boom", cmd) == 0) {
		if (isoadmin(playerid)) {
		    new banid;
		    new Float:r;
			if(!strlen(stringslice(cmdtext,1)) || !strlen(stringslice(cmdtext,2))) {
			    SendClientMessage(playerid, COLOR_LIGHTBLUE, " + USAGE : /boom [type] [r]");
			    return 1;
			}
			new Float:x,Float:y,Float:z;
            banid = strval(stringslice(cmdtext,1));
            r = strval(stringslice(cmdtext,2));
			GetPlayerPos(playerid,x,y,z);
			CreateExplosion(x,y,z,banid,r);
		} else
		 SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/unjail", cmd) == 0) {
		if (isadmin(playerid)) {
		    new banid;
			if(!strlen(stringslice(cmdtext,1))) {
			    SendClientMessage(playerid, COLOR_LIGHTBLUE, " + USAGE : /unjail [playerid]");
			    return 1;
			}
            banid = strval(stringslice(cmdtext,1));
            if(IsPlayerConnected(banid)) {
                if(isJail[banid] == 1) {
				isJail[banid] = 0;
				JailTime[banid] = 0;
				SetPlayerInterior(banid,0);
				SetPlayerPos(banid,-1619.9363,691.8935,7.1875);
				SendClientMessage(banid, COLOR_LIGHTBLUE, " + 석방되셨습니다.");
				SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 그 분은 석방되셨습니다.");
				} else {
				    SendClientMessage(banid, COLOR_LIGHTBLUE, " + 그 분은 감옥에 수감되어 있지 않습니다.");

				}
}
		}
		else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/pk", cmd) == 0) {
		if (isadmin(playerid)) {
			if(pkon == 1) {
			    pkon = 0;
			    SendClientMessageToAll(COLOR_LIGHTBLUE, " + PK 가능");
			} else {
				pkon = 1;
				SendClientMessageToAll(COLOR_LIGHTBLUE, " + PK 불가");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/gopro", cmd) == 0) {
		if (isoadmin(playerid)) {
			PropertyScoreUpdate();
			SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 지급했어 ㅡㅡ");
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
 	}

	if (strcmp("/gassystem", cmd) == 0) {
		if (ismadmin(playerid)) {
			if(gassystem == 1) {
			    gassystem = 0;
			    SendClientMessageToAll(COLOR_LIGHTBLUE, " + 기름 시스템 꺼짐");
			} else {
				gassystem = 1;
				SendClientMessageToAll(COLOR_LIGHTBLUE, " + 기름 시스템 가동");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/usesky", cmd) == 0) {
		if (ismadmin(playerid)) {
			if(use_skyship == 1) {
			    use_skyship = 0;
			    SendClientMessageToAll(COLOR_LIGHTBLUE, " + 스카이쉽 꺼짐");
			} else {
				use_skyship = 1;
				SendClientMessageToAll(COLOR_LIGHTBLUE, " + 스카이쉽 가동");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}

	if (strcmp("/carreset", cmd) == 0) {
		if (isoadmin(playerid)) {
		    new resetkey;
			if(strlen(stringslice(cmdtext,1))) resetkey=strval(stringslice(cmdtext,1)); else resetkey = 50;
			new c;
			for (c=1;c<MAX_VEHICLES;c++) {
				lock[c] = -1;
				gastank[c] = resetkey;
				tgastank[c] = resetkey;
				LinkVehicleToInterior(c,0);
			}
			SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 성공!!");
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
/*
	if (strcmp("/hackscan", cmd) == 0 && isoadmin(playerid)) {
		new msg[255];
		SendClientMessageToAll(COLOR_RED," + 핵 스캔 시스템 version 0.1 beta");
		format(msg,sizeof(msg)," + 게임시각 : %d:%d",word_h,word_m);
		SendClientMessageToAll(COLOR_RED,msg);
		for (new n = 0; n < MAX_PLAYERS; n++) {
			if (IsPlayerConnected(n)){
				new hour,mmin;
				GetPlayerTime(n,hour,mmin);
//				if( word_h == hour) {
//				} else if( (word_h-hour) == 1 ) {
//				} else {
				    format(msg,sizeof(msg)," + 핵감지됨 : %d번 사용자 , %d:%d",n,hour,mmin);
					SendClientMessageToAll(COLOR_RED,msg);
//				}
			}
		}
		SendClientMessageToAll(COLOR_RED," + 스캔끝");
	}
	*/
	if (strcmp("/carreset2", cmd) == 0) {
		if (isoadmin(playerid)) {
		    new resetkey;
			if(strlen(stringslice(cmdtext,1))) resetkey=strval(stringslice(cmdtext,1)); else resetkey = 50;
			new c;
			for (c=1;c<MAX_VEHICLES;c++) {
				lock[c] = -1;
				if(tgastank[c] < resetkey) {
					gastank[c] = tgastank[c];
				} else {
					gastank[c] = resetkey;
				}
				LinkVehicleToInterior(c,0);
			}
			SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 성공!!");
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/ahideadmin", cmd) == 0) {
		if(isoadmin(playerid)) {
		    if(ishadmin(playerid)) {
		        udb_UserSetInt(loginid[playerid],"hideadmin",0);
		        SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 해제됨");
		    } else {
			    udb_UserSetInt(loginid[playerid],"hideadmin",1);
			    SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 성공!!");
			}
		}
	}
	if (strcmp("/ahideexit", cmd) == 0 && ishadmin(playerid)) {
	    new gexit[255];
		new aplayername[255];
		GetPlayerName(playerid,aplayername,sizeof(aplayername));
		format(gexit, sizeof(gexit), " + %s(%d) 님이 퇴장 하셨습니다.(이유:오랜 잠수 ㅡㅅㅡ)", aplayername ,playerid);
		SendClientMessageToAll(COLOR_MEDIUMAQUA, gexit);
	}
	if (strcmp("/ahidejoin", cmd) == 0 && ishadmin(playerid)) {
	    new gexit[255];
		new aplayername[255];
		if(strlen(stringslice(cmdtext,1))) {
			GetPlayerName(playerid,aplayername,sizeof(aplayername));
			format(gexit, sizeof(gexit), " + %s(%d)님이 입장 하셨습니다.", aplayername ,playerid);
			SendClientMessageToAll(COLOR_SPRINGGREEN, gexit);
		}
	}
	if (strcmp("/gexit", cmd) == 0) {
		if (isoadmin(playerid)) {
		    new resetkey;
			if(strlen(stringslice(cmdtext,1))) {
				resetkey=strval(stringslice(cmdtext,1));
				if(IsPlayerConnected(resetkey)) {
					SetPlayerPos(resetkey,2382983479234782934,23482934789234792374892348234,23847293847298347);
					SetPlayerInterior(resetkey,2348923472983479999999);
        	    	SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 성공!!");
				} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 활동하고있는 유저가 아니");
			} else {
				SendClientMessage(playerid, COLOR_LIGHTBLUE, " + USAGE:/gexit [playerid]");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/playsound", cmd) == 0) {
		if (ismadmin(playerid)) {
		    new resetkey;
			if(strlen(stringslice(cmdtext,1))) {
                PlaySoundForPlayer(playerid,resetkey);
                SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Good");
			} else {
				SendClientMessage(playerid, COLOR_LIGHTBLUE, " + USAGE:/playsound [playerid]");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}

	if (strcmp("/cadmin", cmd) == 0) {
		if (isoadmin(playerid)) {
			if(cadmin == 1) {
			    cadmin = 0;
			    SendClientMessageToAll(COLOR_LIGHTBLUE, " + 시스템 코드 1 중단");
			} else {
				cadmin = 1;
				SendClientMessageToAll(COLOR_LIGHTBLUE, " + 시스템 코드 1 가동");
			}
		} else SendClientMessage(playerid, COLOR_LIGHTBLUE, " + Admin 만 사용 가능합니다.");
		return 1;
	}
	if (strcmp("/vkick", cmd) == 0)
	{
		tmp = strtok(cmdtext, idx);
		giveplayerid = strval(tmp);

  		player_num=0;

			for(new i=0; i<MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i) == 1) {player_num++;
				}
			}
			if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /vkick [대상ID]");
			return 1;
			}
			if(player_num < 6)
			{
				SendClientMessage(playerid, 0xFF0000AA, " + 킥투표 명령어는 6명 이상일 경우에만 사용 가능합니다.");
				return 1;
			}
			if(vote_flag == 1)
			{
				SendClientMessage(playerid, 0xFF0000AA, " + 현재 투표가 진행중입니다. 현재 진행중인 투표가 끝난 후에 사용하세요. ");
				return 1;
			}
				if(IsPlayerConnected(giveplayerid) == 1)
				{
				    if(ismadmin(giveplayerid) && !ismadmin(playerid)) { Kick(playerid); return 1; }
					GetPlayerName(giveplayerid, giveplayer , MAX_PLAYER_NAME);
					GetPlayerName(playerid, playername , MAX_PLAYER_NAME);
					format(string, sizeof(string), " + %d 번 %s 님에 대한 강퇴 찬반투표가 1분동안 진행됩니다. (신청자: %d번 %s님)",giveplayerid , giveplayer, playerid,playername);
					SendClientMessageToAll(COLOR_YELLOW, string);
					for(new i=0 ; i<MAX_PLAYERS ; i++) {
					vote[i] = 0;
					}
					vote_time = 60;
					vote_flag = 1;
					vote_ID = giveplayerid;
					return 1;
				}
				else
				{
					format(string, sizeof(string), " + 현재 접속되어 있지 않은 사용자입니다.");
					SendClientMessage(playerid, 0xFF0000AA, string);
				}
		return 1;
	}
	if(strcmp(cmd, "/yes", true) == 0)
	{
		if(vote_flag == 0)
		{
			SendClientMessage(playerid, 0xFF0000AA, " + 현재 투표중이 아닙니다. ");
			return 1;
		}
		else if(playerid == vote_ID)
		{
			SendClientMessage(playerid, 0xFF0000AA, " + 투표 대상자는 투표권이 없습니다. ");
			return 1;
		}
		else if(vote[playerid] && !ismadmin(playerid))
		{
			SendClientMessage(playerid, 0xFF0000AA, " + 이미 투표를 하셨습니다. ");
			return 1;
		}
		else
		{
			vote[playerid] = 1;
			SendClientMessage(playerid, 0xFF0000AA, " + 투표 성공.");
		}
		return 1;
	}
	// PROCESS OTHER COMMANDS
	if(ErrorCounter < 15) {
		ErrorCounter++;
	} else {
		ErrorCounter = 0;
	}
   	SendClientMessage(playerid,COLOR_THISTLE,ErrorMessages[ErrorCounter]);
	return 1;
}

//------------------------------------------------------------------------------------------------------

public OnPlayerSpawn(playerid)
{
	TogglePlayerClock(playerid,1);
	SetPlayerTime(playerid,word_h,word_m); //접속이나 스폰시 시간 보이게 하기
	for(new i=0;i<55;i++){// 모든 무기에 대해 적용
		weapons[i][WEAPON_BOUGHT][playerid]=false;// 초기화 합니다.
		weapons[i][WEAPON_AMMO][playerid]=0;
	}
    IS_PLAYER_SUICIDED[playerid]=false; //v1.1 - 수정.
	OLDHEALTH[playerid]=100.0;// 기존체력 초기화
	ospawn[playerid] = 1;
	ClearAnimations(playerid);
	if(MyJob[playerid] == JOB_BOXER) {
	SetPlayerSkin(playerid, 81);
	}
	if(loginflag[playerid]==0) {
	/*
		SetPlayerPos(playerid,2324.4199,-1147.1016,1050.7101);
		SetPlayerInterior(playerid,12);
	  	SendClientMessage(playerid, COLOR_GREEN, " + 로그인이 필요합니다.");
	  	SendClientMessage(playerid, COLOR_GREEN, " + 로그인을 하시려면 /logincom 을 참고해주세요");
	  	SendClientMessage(playerid, COLOR_GREEN, " + 3분 동안 로그인하지 않으시면 강퇴 됩니다.");
		loginse[playerid] = 0;
		TogglePlayerControllable(playerid,0);
		*/
		Kick(playerid);
	} else if(login_spawn_check[playerid] != 3) {
	    new money;
	    money = udb_UserInt(loginid[playerid],"money");
		moneyhack[playerid] = money;
		SetPlayerMoney(playerid,money);
		if(udb_UserFloat(loginid[playerid],"Health")) SetPlayerHealth(playerid,udb_UserFloat(loginid[playerid],"Health"));
		if(login_spawn_check[playerid] == 1) {
			if(udb_UserInt(loginid[playerid],"Int") == 12 || !udb_UserFloat(loginid[playerid],"X") || !udb_UserFloat(loginid[playerid],"Y") || !udb_UserFloat(loginid[playerid],"Z")) {
				SetPlayerInterior(playerid,0);
				SetPlayerRandomSpawn(playerid);
			} else {
				SetPlayerInterior(playerid,udb_UserInt(loginid[playerid],"Int"));
				SetPlayerPos(playerid,udb_UserFloat(loginid[playerid],"X"),udb_UserFloat(loginid[playerid],"Y"),udb_UserFloat(loginid[playerid],"Z"));
			}
			login_spawn_check[playerid] = 3;
			loginsystem(playerid,1);
		} else if(login_spawn_check[playerid] == 2) {
			SetPlayerInterior(playerid,0);
			SetPlayerRandomSpawn(playerid);
			login_spawn_check[playerid] = 3;
			loginsystem(playerid,1);
		}
		TextDrawShowForPlayer(playerid,crazyText);
		SetPlayerVirtualWorld(playerid,0);
	}
	TogglePlayerControllable(playerid,1);
	TogglePlayerControllable(playerid,1);
	if(god[playerid] == 1) SetPlayerHealth(playerid, 99999.0);
	if(ismadmin(playerid))c_GivePlayerWeapon(playerid,38,5000);

	c_GivePlayerWeapon(playerid,udb_UserInt(loginid[playerid],"weapon1"),1);
	c_GivePlayerWeapon(playerid,udb_UserInt(loginid[playerid],"weapon2"),udb_UserInt(loginid[playerid],"ammo2"));
	c_GivePlayerWeapon(playerid,udb_UserInt(loginid[playerid],"weapon3"),udb_UserInt(loginid[playerid],"ammo3"));
    c_GivePlayerWeapon(playerid,udb_UserInt(loginid[playerid],"weapon4"),udb_UserInt(loginid[playerid],"ammo4"));
    c_GivePlayerWeapon(playerid,udb_UserInt(loginid[playerid],"weapon5"),udb_UserInt(loginid[playerid],"ammo5"));
    TogglePlayerClock(playerid, 1);
	SetPlayerTime(playerid,word_h,word_m); //싱글시간
	if(login_spawn_check[playerid] == 3) {
		SetPlayerInterior(playerid,0);
		SetPlayerRandomSpawn(playerid);
	}
	classing[playerid] = 0;


	if (PKzoneplay[playerid] == 1) {
	SetPlayerInterior(playerid,0);
	  new pkzone = random(8);
   		switch(pkzone){
       	case 0:{SetPlayerPos(playerid,-989.0357,-647.0072,32.0078);}
       	case 1:{SetPlayerPos(playerid,-1041.1925,-696.2205,32.0078);}
		case 2:{SetPlayerPos(playerid,-1063.6085,-699.9950,32.3516);}
        case 3:{SetPlayerPos(playerid,-1076.5234,-731.9996,32.0078);}
        case 4:{SetPlayerPos(playerid,-994.7500,-726.1249,32.0078);}
        case 5:{SetPlayerPos(playerid,-995.7006,-720.0246,35.9375);}
        case 6:{SetPlayerPos(playerid,-976.6788,-618.6646,32.0078);}
        case 7:{SetPlayerPos(playerid,-1083.0332,-592.9813,32.0078);}
        case 8:{SetPlayerPos(playerid,-1060.2990,-610.4663,34.0896);}
        }
  	SendClientMessage(playerid, COLOR_GREEN, " + 일반 PK존으로 왔습니다. PK존으로 스폰 되기 싫으시면 /pkreturn 하십시오.");
	}


	weapons[0][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[0][WEAPON_AMMO][playerid]=65535;// 주먹 총알수(없어도 무방합니다.)
	weapons[24][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[24][WEAPON_AMMO][playerid]=300;// 주먹 총알수(없어도 무방합니다.)
	weapons[49][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[49][WEAPON_AMMO][playerid]=65535;// 주먹 총알수(없어도 무방합니다.)
	weapons[51][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[51][WEAPON_AMMO][playerid]=65535;// 주먹 총알수(없어도 무방합니다.)
	weapons[50][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[50][WEAPON_AMMO][playerid]=65535;// 주먹 총알수(없어도 무방합니다.)

	weapons[46][WEAPON_BOUGHT][playerid]=true;// 주먹
	weapons[46][WEAPON_AMMO][playerid]=65535;// 주먹 총알수(없어도 무방합니다.)

	// 이외에 기본으로 지급하는 무기가 있을경우 아래에 계속 추가하세요.
	SKIP_WEAPONCHECK[playerid]=false;// v1.2 - 타이머에서 다시 해당 플레이어에 대해 무기핵 체크를 하게 합니다.



	return 1;
}

public SetPlayerRandomSpawn(playerid)
{
		new rand = random(sizeof(gRandomPlayerSpawns));
		if(use_skyship == 0) rand = 4;
		if(SpawnSet[playerid] == 0) {
		SetPlayerPos(playerid, gRandomPlayerSpawns[rand][0], gRandomPlayerSpawns[rand][1], gRandomPlayerSpawns[rand][2]); // Warp the player
		} else if(SpawnSet[playerid] == 1) {
		SetPlayerPos(playerid,2000.7450,1561.8743,15.3672); //해적선
		} else if(SpawnSet[playerid] == 2) {
		SetPlayerPos(playerid,2495.3630,-1686.8370,13.5149);
		} else if(SpawnSet[playerid] == 3) {
		SetPlayerPos(playerid,-377.5717,2242.5410,42.6185);
		}
	return 1;
}

//------------------------------------------------------------------------------------------------------

public OnPlayerDeath(playerid, killerid, reason)
{

	new str[256];
	new killername[MAX_PLAYER_NAME];
 	new playername[MAX_PLAYER_NAME];
 	new Float:X, Float:Y, Float:Z;
	new weap[200];
 	new ammo; // not used
 	new temp;
 	new s[256];
 	new PX;
 	new PY;
 	new string[256];
 	new killer[MAX_PLAYER_NAME];
	new player[MAX_PLAYER_NAME];

	
	SKIP_WEAPONCHECK[playerid]=true;// v1.2 - 죽은경우 스폰될때까지 무기핵 검사를 중단합니다.
 	SendDeathMessage(killerid,playerid,reason);
	GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
	GetPlayerName(killerid, killername, MAX_PLAYER_NAME);
//	format( szSend, 512, "DEATH %i %i %i", playerid, killerid, reason );
//	GUIRCON_SendToAll( szSend );
	FishTime[playerid] = 0;
 	taxi[playerid][custom]=-1;
	taxi[playerid][driver]=-1;
	if(mobile_state[playerid] == 4) {
		if( call_data[mobile_call[playerid]][cstate] == 1 ) {
			for(new i=0; i < MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i)) {
					if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
						mobile_call[i] = -1;
						mobile_state[i] = 3;
					}
				}
			}
			mobile_call[playerid] = -1;
			mobile_state[playerid] = 3;
			call_data[mobile_call[playerid]][use] = false;
			call_data[mobile_call[playerid]][check] = false;
			call_data[mobile_call[playerid]][cid] = -1;
			call_data[mobile_call[playerid]][cstate] = 0;
			call_data[mobile_call[playerid]][time] = 0;
			call_data[mobile_call[playerid]][time2] = 0;
			SendClientMessage(playerid, COLOR_GRAD2, " + 당신은 죽어서 전화가 끊겼습니다.");
			SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
		} else if(call_data[mobile_call[playerid]][cstate] == 2) {
			for(new i=0; i < MAX_PLAYERS; i++) {
				if(IsPlayerConnected(i)) {
					if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
						SendClientMessage(i,COLOR_RED," + 상대방이 죽어서 통화가 종료됩니다.");
						mobile_call[i] = -1;
						mobile_state[i] = 3;
						SetPlayerSpecialAction(i,SPECIAL_ACTION_STOPUSECELLPHONE);
					}
				}
			}
			SendClientMessage(playerid,COLOR_RED," + 당신은 죽어서 통화가 종료되었습니다.");
			SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
			new money;
			money = 15*call_data[mobile_call[playerid]][time];
			GiveMoney(call_data[mobile_call[playerid]][cid],-money);
			new vstr[255];
			format(vstr,sizeof(str)," + 통화 요금으로 $%d 달러 를 지불 하셨 습니다. - LG TeleCom -",money);
			SendClientMessage(call_data[mobile_call[playerid]][cid],COLOR_RED,vstr);
			mobile_state[playerid] = 3;
			mobile_call[playerid] = -1;
			call_data[mobile_call[playerid]][use] = false;
			call_data[mobile_call[playerid]][check] = false;
			call_data[mobile_call[playerid]][cstate] = 0;
			call_data[mobile_call[playerid]][time] = 0;
			call_data[mobile_call[playerid]][time2] = 0;
			call_data[mobile_call[playerid]][cid] = -1;
		}
	}

	TogglePlayerControllable(playerid,1);
	GetPlayerPos(playerid, X, Y, Z);
	if(worker[playerid]!=0) {
		worker[playerid]=0;
		SendClientMessage(playerid,COLOR_ORANGE," + 주인장 : 물건을 잃어버렸구만? 이거 못써먹겟내;;;");
		SendClientMessage(playerid,COLOR_ORANGERED," + 물건배달에 실패했습니다!");
	}
	
	if(deathmatcher1[playerid] == 1){
	    deathmatcher1[playerid] = 0;
	    ResetPlayerWeapons(playerid);
	    SetPlayerWorldBounds(playerid,5000.0000,-5000.0000,5000.0000,-5000.0000);
		playerseaglematch=playerseaglematch-1;
	    return 1;
	}

	if(deathmatcher4[playerid] == 1){
	    deathmatcher4[playerid] = 0;
	    ResetPlayerWeapons(playerid);
	    SetPlayerWorldBounds(playerid,5000.0000,-5000.0000,5000.0000,-5000.0000);
		playerschainmatch=playerschainmatch-1;
	    return 1;
	}

	if(deathmatcher5[playerid] == 1){
	    deathmatcher5[playerid] = 0;
	    ResetPlayerWeapons(playerid);
	    SetPlayerWorldBounds(playerid,5000.0000,-5000.0000,5000.0000,-5000.0000);
		playersminimatch=playersminimatch-1;
	    return 1;
	}

	if(deathmatcher6[playerid] == 1){
	    deathmatcher6[playerid] = 0;
	    ResetPlayerWeapons(playerid);
	    SetPlayerWorldBounds(playerid,5000.0000,-5000.0000,5000.0000,-5000.0000);
		playersuzimatch=playersuzimatch-1;
	    return 1;
	}
	if(bounty[playerid] > 0) {
	   	if(IsPlayerConnected(killerid)) {
		format(string, sizeof(string), " + %s(%d)님이 현상범[%s(%d)]님을 잡아서 $%d의 현상금을 벌었습니다.", killername, killerid, playername,playerid, bounty[playerid]);
		SendClientMessageToAll(COLOR_BLUE, string);
		GiveMoney(killerid, bounty[playerid]);
		bounty[playerid] = 0;
		udb_UserSetInt(loginid[playerid],"bounty",bounty[playerid]);
		SetPlayerWantedLevel(playerid,0);
		}
	}
	if(killerid == INVALID_PLAYER_ID || IS_PLAYER_SUICIDED[playerid]) {
		if(isJail[playerid]==1){
     		SendClientMessage(playerid,COLOR_RED," + 감옥안에서 자살했으므로 강퇴됩니다.");
     		Kick(playerid);
     	} else {
			GiveMoney(playerid, -500);
			for(new i=0;i<15;i++){
				// Getting random pos near to player
				format(s, 256 ,"%.0f", X);
				temp = strval(s);
				PX = random((temp+2)-(temp-2))+(temp-2);
				format(s, 256 ,"%.0f", Y);
				temp = strval(s);
				PY = random((temp+2)-(temp-2))+(temp-2);
				// end of pos
				// Placing Pickups
				if(i<12){
					GetPlayerWeaponData(playerid, i, weap[i], ammo);
					if(weapmod[weap[i]] == 1212){
						new pickid = CreatePickup(1212, 3,PX, PY, Z);
						PickUpMoney[pickid]=true;
						DropPick[pickid]=true;
					} else {
					    if(weapmod[weap[i]] != 38) {
						DropPick[CreatePickup(weapmod[weap[i]], 3,PX, PY, Z)]=true;
						}
					}
                }
				if(i==13)DropPick[CreatePickup(1240, 3,PX, PY, Z)]=true;
				if(i==14)if(GetPlayerArmourEx(playerid)>0.0)DropPick[CreatePickup(1242, 3,PX, PY, Z)]=true; else {
					new pickid = CreatePickup(1212, 3,PX, PY, Z);
					PickUpMoney[pickid]=true;
					DropPick[pickid]=true;
				}
				if(i==15){ // 돈핵뜨는거 막기위해..
				//	new pickid = CreatePickup(1212, 3,PX, PY, Z);
				//	PickUpMoney[pickid]=true;
				//	DropPick[pickid]=true;
				}
			}
		}
	} else {
		if( (weapons[reason][WEAPON_DISABLED] || GetDistanceBetweenPlayers(playerid,killerid) > weapons[reason][WEAPON_RANGE] || (IsPlayerInAnyVehicle(killerid) && (reason != 32 && reason != 49 && reason !=28 && reason !=29)) || (OLDHEALTH[playerid] > CONSIDER_PLAYER_FAKEKILL)) && reason != 49 && reason != 46 && reason != 51 && reason != 50){
			format(str, sizeof(str), " + %s(%d) 패이크핵 경고!",playername, playerid);
			SendClientMessageToAll(COLOR_RED, str);
			return 1;// 종료
		} else {
		/*
			if(!weapons[reason][WEAPON_BOUGHT][killerid]){// 무기를 구입한 적이 없는 경우
*				format(str, sizeof(str), " + %s(%d) 무기핵으로 %d번 무기를 사용하였습니다.",killername, killerid ,reason);
				SendClientMessageToAll(COLOR_RED, str);
//				Kick(killerid);
				return 1;// 종료
				*
			}*/

			if(minimatchplayer[killerid] == 1 || chainmatchplayer[killerid] == 1 || eaglematchplayers[killerid] == 1 || uzimatchplayer[killerid] == 1) {
			}
			
 				if(event_flag == 1 && event_value == 0 && playerid == runaway_ID)
				{
				GetPlayerName(playerid, player, sizeof(player));
				GetPlayerName(killerid, killer, sizeof(killer));
				format(string, sizeof(string), " +  %s 님이 %s 님을 잡았습니다! %s 님은 도망에 실패하였습니다. 이벤트 종료", killer, player, player);
				SendClientMessageToAll(COLOR_ORANGE, string);
				format(string, sizeof(string), " + %s 님께 상금 %d 을 드립니다. 다시 PK 금지!", killer, RUNAWAY_MONEY_KILLER);
				if(IsPlayerConnected(killerid)==1)
				{
				GiveMoney(killerid, RUNAWAY_MONEY_KILLER);
				}
				SendClientMessageToAll(COLOR_ORANGE, string);
				event_flag = 0;
				event_value = random(3);
				murder_flag = 0;
				run_time = RUNAWAY_TIME;
				return 1;
				}
				else if( IsPlayerInArea(playerid, -1126.6427,-758.9443, -965.6680,-573.7631)&& IsPlayerInArea(killerid, -1126.6427,-758.9443, -965.6680,-573.7631)) {
				new killmoney = random(4000)+1000;
				GiveMoney(killerid,killmoney);
				GiveMoney(playerid,-killmoney);
				format(strings, sizeof(strings)," + 당신은 PK존에서 사람을 기절시켜 $%d 를 얻었습니다.", killmoney);
				SendClientMessage(killerid,COLOR_VIOLET, strings);
				format(strings, sizeof(strings)," + 당신은 PK존에서 $%d 를 잃었습니다.", killmoney);
				SendClientMessage(playerid,COLOR_MEDIUMAQUA, strings);
				return 1;
				} else if(pkon == 0 || murder_flag == 1 || (GetPlayerVirtualWorld(killerid) == 1 && GetPlayerVirtualWorld(playerid) == 1)) {
				new killmoney = random(4000)+1000;
				GiveMoney(killerid,killmoney);
				GiveMoney(playerid,-killmoney);
				format(strings, sizeof(strings)," + 사람을 기절시켜 $%d 를 얻었습니다.", killmoney);
				SendClientMessage(killerid,COLOR_VIOLET, strings);
				format(strings, sizeof(strings)," + 죽어서 $%d 를 잃었습니다.", killmoney);
				SendClientMessage(playerid,COLOR_MEDIUMAQUA, strings);
				return 1;
				} else if(killcount[killerid] == 1) {
				format(str, sizeof(str), " + %s(%d)님은 %s(%d)님을 살해해서 킥 되셨습니다.",killername, killerid,playername,playerid);
				SendClientMessageToAll(COLOR_RED, str);
				Kick(killerid);
				} else if(isJail[killerid] == 1) {
				isJail[playerid] = 0;
				JailTime[playerid] = 0;
				SetPlayerInterior(playerid,0);
				SetPlayerRandomSpawn(playerid);
				format(str, sizeof(str), " + %s(%d) 님이 %s(%d) 님을 감옥안에서 살해 해서 킥 되었습니다.",killername, killerid ,playername,playerid);
				SendClientMessageToAll(COLOR_RED, str);
				Kick(killerid);
			} else if(loginflag[killerid] == 0) {
		//		format(str, sizeof(str), " + %s(%d) 님이 %s(%d) 님을 로그인 대기 맵에서 살해 해서 킥 되었습니다.",killername, killerid ,playername,playerid);
		//		SendClientMessageToAll(COLOR_RED, str);
		//		Kick(killerid);
		}
 else {
				SetPlayerInterior(killerid,6);
				SetPlayerPos(killerid,263.51,77.76,1001.05);
				GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
				GetPlayerName(killerid, killername, MAX_PLAYER_NAME);
				killcount[killerid] --;
				ResetPlayerWeapons(killerid);
				format(str, sizeof(str), " + %d번 %s 님이 %d번 %s 님을 살해 해서 2분간 감옥에 수감됩니다. %d번 더 살해시 킥 됩니다.", killerid,killername ,playerid,playername,killcount[killerid]);
				SendClientMessageToAll(COLOR_RED, str);
				isJail[killerid] = 1;
				JailTime[killerid] = 120;
			}
		}
 	}
// - End of Defines
