public KillText()
{
	new i;

	for(i = 0; i < MAX_PLAYERS; i++)
	{

		if(GetPlayerWeapon(i) == 44 || GetPlayerWeapon(i) == 45) {
		ResetPlayerWeapons(i);
		}
	    if(gUseVText[i] == 1)
	    {
			gVTextNum[i]++;
			if(gVTextNum[i] >= 5) {
				//TextDrawHideForAll(gVText[i]);
				TextDrawDestroy(gVText[i]);
				gVTextNum[i] = 0;
				gUseVText[i] = 0;
			}
		}
	}
}

public OnPlayerSelectedMenuRow(playerid, row)
{
    new Menu:Current = GetPlayerMenu(playerid);
    if(Current == Upgrades) {
    switch(row){
        case 0:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Paintjobs, playerid);
			}
        case 1:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Colors, playerid);
			}
        case 2:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Exhausts, playerid);
			}
        case 3:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Frontbumper, playerid);
			}
        case 4:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Rearbumper, playerid);
			}
        case 5:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Roof, playerid);
			}
        case 6:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Spoilers, playerid);
			}
        case 7:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Sideskirts, playerid);
			}
        case 8:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Wheels, playerid);
			}
        case 9:
            if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Carstereo, playerid);
			}
        case 10:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Hydraulics, playerid);
			}
        case 11:
			if(IsPlayerConnected(playerid))
			{
				ShowMenuForPlayer(Nitro, playerid);
			}
	}
	}
	if(Current == Paintjobs) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 500)
	        {
				new car = GetPlayerVehicleID(playerid);
				ChangeVehiclePaintjob(car,0);
				GiveMoney(playerid,-500);
				GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$500",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:
		    if(GetMoney(playerid) >= 500)
		    {
                new car = GetPlayerVehicleID(playerid);
				ChangeVehiclePaintjob(car,1);
				GiveMoney(playerid,-500);
				GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$500",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 2:
		    if(GetMoney(playerid) >= 500)
		    {
                new car = GetPlayerVehicleID(playerid);
				ChangeVehiclePaintjob(car,2);
				GiveMoney(playerid,-500);
				GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$500",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 3:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
	if(Current == Colors) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 150)
	        {
	            new car = GetPlayerVehicleID(playerid);
	            ChangeVehicleColor(car,0,0);
	            GiveMoney(playerid,-150);
	            GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$150",3000,5);
	            ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:
		    if(GetMoney(playerid) >= 150)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        ChangeVehicleColor(car,1,1);
		        GiveMoney(playerid,-150);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$150",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 2:
		    if(GetMoney(playerid) >= 150)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        ChangeVehicleColor(car,3,3);
		        GiveMoney(playerid,-150);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$150",3000,5);
