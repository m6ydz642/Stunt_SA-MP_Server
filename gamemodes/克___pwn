	if (strcmp( stringslice(cmdtext,0), "/fillup", true)==0) {
		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER){
			new StationLoc;
			StationLoc=IsVehicleAtStation(playerid);
			if(StationLoc == 999) {
				SendClientMessage(playerid, COLOR_RED , " + 주유소에서 기름 주입이 가능합니다.");
				//SendClientMessage(playerid, COLOR_RED , " + 주유소로 견인하기 (준비중)");
			} else {
				if(GetMoney(playerid) < 50000) {
					SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 우선 $50000 의 돈이 필요합니다.");
					return 1;
            	}
 				if(GetVehicleType(GetPlayerVehicleID(playerid)) == VT_AIR || GetVehicleType(GetPlayerVehicleID(playerid)) == VT_SEA || GetVehicleType(GetPlayerVehicleID(playerid)) == VT_TRAIN){
				SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 비행기, 보트는 기름을 충전할 수 없습니다.");
				return 1;
				}

				if(GetVehicleType(GetPlayerVehicleID(playerid)) == VT_BIKE){
				SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 자전거가 기름으로 가던가요??");
				return 1;
				}
				SendClientMessage(playerid,COLOR_RED," + 주유를 시작 합니다.");
				SendClientMessage(playerid,COLOR_RED," + 주유 멈추기: /fillstop");
				//SendClientMessage(playerid,COLOR_RED," + 기름을 꽉 채우시려면 /fillstop 을 안 입력해도 됩니다");
				TogglePlayerControllable(playerid,0);
				tgasup[playerid] = 0;
				isgasup[playerid] = 1;
				lock[GetPlayerVehicleID(playerid)] = playerid;
			}
		} else {
			SendClientMessage(playerid,COLOR_RED," + 운전자 만 가능합니다.");
		}
		return 1;
	}
	if (strcmp( stringslice(cmdtext,0), "/fillcall", true)==0)
	{
