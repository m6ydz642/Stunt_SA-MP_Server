}
	if(strcmp(cmd, "/mypro", true) == 0) {
		new propertycounter = 0;
		new TotalValue;
		new TotalEarning;
		new Totalstrings[256];
		new keydata[255];
		for(new i=0; i<MAX_POINTS; i++) {
			if(!strlen(propertyOwner[i])) {
			} else if(!udb_Exists(propertyOwner[i])) {
				propertyOwner[i] = "";
				format(keydata,sizeof(keydata),"%d",i);
				dini_Set("p.cfg",keydata,"");
			} else if (strcmp(loginid[playerid], propertyOwner[i], true) == 0) {
				if(propertyValues[i] >= 1) {
					propertycounter++;
					format(Totalstrings, sizeof(Totalstrings), " + %d. %s 건물의 가격 - $%d / 예상 수익금 - $%d ", i,propertyNames[i],propertyValues[i],propertyValues[i]/40);
					TotalValue+=propertyValues[i];
					TotalEarning+=propertyValues[i]/40;
					SendClientMessage(playerid, COLOR_MEDIUMAQUA , Totalstrings);
				}
			}
		}
		if (propertycounter == 0 ) {
		   	SendClientMessage(playerid, COLOR_SEAGREEN2 , " + 당신은 건물을 소유하고 있지 않습니다.");
		} else {
    	 	format(string, sizeof(string), " + 소유 건물 갯수: %d 개 *** 전체 건물 가격: $%d 전체 건물 예상 수익금: $%d ", propertycounter,TotalValue,TotalEarning);
			SendClientMessage(playerid, COLOR_SEAGREEN2 , string);
		}
		return 1;
	}
	if(strcmp(cmd, "/getip", true) == 0) {
		tmp = strtok(cmdtext, idx);
		giveplayerid = strval(tmp);
		new ip[256];

		if (!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /getip [플레이어ID]");
			return 1;
		}
		GetPlayerIp(giveplayerid,ip,256);
		GetPlayerName(giveplayerid, giveplayer, MAX_PLAYER_NAME);
		format(string, sizeof(string), " + [%d] %s 의 아이피: %s",giveplayerid,giveplayer,ip);
		SendClientMessage(playerid, COLOR_GREEN, string);
	return 1;
	}
	if(strcmp(cmd, "/bwmelee", true) == 0) {

		tmp = strtok(cmdtext, idx);

		if (!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /bwmelee [weaponid]");
			return 1;
		}
		weaponid = strval(tmp);

		tmp = strtok(cmdtext, idx);

		if (GetMoney(playerid) < weaponCostMelee[weaponid]) {
			SendClientMessage(playerid, COLOR_RED, " + 충분한 돈이 있질 않습니다.");
			return 1;
		}
		if (weaponid < 0 || weaponid > 3){
			SendClientMessage(playerid, COLOR_RED, " + 옳지않은 무기구입 방법입니다.");
			return 1;
		}
        if (playerCheckpoint[playerid] == CP_AMMU || playerCheckpoint[playerid] == CP_AMMU2) {
		GetPlayerName(playerid, giveplayer, sizeof(giveplayer));
		format(string, sizeof(string), "--- %s (id: %d) 님은 %s 를 $%d 에 구입하였습니다.", giveplayer,playerid,weaponNamesMelee[weaponid],weaponCostMelee[weaponid]);
		printf(string);
		format (string, sizeof(string), " + %s 를 $%d 에 구입하였습니다.", weaponNamesMelee[weaponid],weaponCostMelee[weaponid]);
		SendClientMessage(playerid, COLOR_GREEN, string);

		c_GivePlayerWeapon(playerid, weaponIDsMelee[weaponid], 1);

		udb_UserSetInt(loginid[playerid],"weapon1",weaponIDsMelee[weaponid]);
		GiveMoney(playerid, -weaponCostMelee[weaponid]);
		}else {
		SendClientMessage(playerid, COLOR_YELLOW, " + 무기점 안에서만 무기를 구입 할수있습니다.");
		}
		return 1;
	}

	if(strcmp(cmd, "/bwpistol", true) == 0) {
	    new ammo;

		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /bwpistol [weaponid] [ammo]");
			return 1;
		}
		weaponid = strval(tmp);

		tmp = strtok(cmdtext, idx);

		if(!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE, " + USAGE: /bwpistol [weaponid] [ammo]");
			return 1;
		}
		ammo = strval(tmp);

		if(GetMoney(playerid) < weaponCostPistols[weaponid]*ammo) {
			SendClientMessage(playerid, COLOR_RED, " + 충분한 돈이 있질 않습니다.");
			return 1;
		}
		if(weaponid < 0 || weaponid > 2){
			SendClientMessage(playerid, COLOR_RED, " + 옳지않은 무기구입 방법입니다.");
			return 1;
		}
		if(ammo < 1) {
			SendClientMessage(playerid, COLOR_YELLOW, " + 명령어를 똑바로 쳐주세요.");
			return 1;
		}
  		if (playerCheckpoint[playerid] == CP_AMMU || playerCheckpoint[playerid] == CP_AMMU2) {
		GetPlayerName(playerid, giveplayer, sizeof(giveplayer));
		format(string, sizeof(string), "--- %s (id: %d) 님은 %s (탄약: %d) 를 $%d 에 구입하였습니다.", giveplayer,playerid,weaponNamesPistols[weaponid],ammo,weaponCostPistols[weaponid]*ammo);
		printf(string);
		format (string, sizeof(string), " + %s (탄약: %d) 를 $%d 에 구입하였습니다.", weaponNamesPistols[weaponid],ammo,weaponCostPistols[weaponid]*ammo);
		SendClientMessage(playerid, COLOR_GREEN, string);

		c_GivePlayerWeapon(playerid, weaponIDsPistols[weaponid], ammo);

        udb_UserSetInt(loginid[playerid],"weapon2",weaponIDsPistols[weaponid]);

        udb_UserSetInt(loginid[playerid],"ammo2",ammo+udb_UserInt(loginid[playerid],"ammo2"));
		GiveMoney(playerid, -weaponCostPistols[weaponid]*ammo);
		}else {
		SendClientMessage(playerid, COLOR_YELLOW, " + 무기점 안에서만 무기를 구입 할수있습니다.");
		}
		return 1;
	}

