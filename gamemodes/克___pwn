		SendClientMessageToAll(COLOR_RED,"실행");

		return 1;
	}


 	if (strcmp( stringslice(cmdtext,0), "/r", true)==0)
        {
        	strmid(nnn[playerid], cmdtext, 2, strlen(cmdtext));
