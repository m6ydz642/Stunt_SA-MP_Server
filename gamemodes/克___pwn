public OnGameModeExit ()
{
    DestroyMenu(Menu:Upgrades);
	DestroyMenu(Menu:Paintjobs);
	DestroyMenu(Menu:Colors);
	DestroyMenu(Menu:Exhausts);
	DestroyMenu(Menu:Frontbumper);
	DestroyMenu(Menu:Rearbumper);
	DestroyMenu(Menu:Roof);
	DestroyMenu(Menu:Spoilers);
	DestroyMenu(Menu:Sideskirts);
	DestroyMenu(Menu:Wheels);
	DestroyMenu(Menu:Carstereo);
	DestroyMenu(Menu:Hydraulics);
	DestroyMenu(Menu:Nitro);
	DestroyMenu(Menu:testtext);
	//DestroyMenu(Menu:gastank); //gastank
//	GUIRCON_Deactivate();
	del_gang();
  printf(" + 모드 리셋 또는 서버 다운......"); //서버종료 메시지
  DestroyObject(objects[0]);
  DestroyObject(objects[1]);
  cdelobject();

  lottotest = 400000;
  
  for (new i=0; i<MAX_PLAYERS; i++)
  {
  	if (rampid[i] != -1) DestroyPlayerObject(i,rampid[i]);
  }
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
		new Float:x, Float:y, Float:z, Float:angle;
		new Float:px,Float:py,Float:pz;
		new Float:cx,Float:cy,Float:cz;

		GetPlayerPos(playerid, x, y, z);
  		GetPlayerPos(playerid,px,py,pz);

if  ((PlayerIsDancing[playerid]==1)&&(allow[playerid]>1))
	    {
		if 	(newkeys == currentKey)
		    {
		    GameTextForPlayer(playerid,getGoodSpeech(),500,3);
		    allow[playerid]=0;
		    score[playerid]++;
		    }
		else
		    {
			if 	(newkeys == lateKey)
			    {
			    GameTextForPlayer(playerid,getLateSpeech(),1000,3);
			    allow[playerid]=0;
			    }
			else
			    {
				if 	(newkeys == nextKey)
				    {
				    GameTextForPlayer(playerid,getEarlySpeech(),1000,3);
				    allow[playerid]=0;
				    }
				else
				    {
					if  (newkeys != 0)
					    {
				    	GameTextForPlayer(playerid,getBadSpeech(),1000,3);
				    	allow[playerid]=0;
				    	}
				    }
				}
			}
		}
	if ((newkeys==KEY_ACTION)&&(IsPlayerInAnyVehicle(playerid))&&(GetPlayerState(playerid)==PLAYER_STATE_DRIVER)) {
		if (GetVehicleModel(GetPlayerVehicleID(playerid)) == 525) {

			new Float:pX,Float:pY,Float:pZ;
			GetPlayerPos(playerid,pX,pY,pZ);
			new Float:vX,Float:vY,Float:vZ;
			new Found=0;
			new vid=0;
			
			/*if (IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid))) {
					SendClientMessage(playerid,0xFFFF00AA," + 차 견인 해제");
					DetachTrailerFromVehicle(GetVehicleTrailer(GetPlayerVehicleID(playerid)));
					return;
					} */
			while((vid<MAX_VEHICLES)&&(!Found)) {
				vid++;
				GetVehiclePos(vid,vX,vY,vZ);

				if  ((floatabs(pX-vX)<7.0)&&(floatabs(pY-vY)<7.0)&&(floatabs(pZ-vZ)<7.0)&&(vid!=GetPlayerVehicleID(playerid))) {
					if (IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid))) {
					SendClientMessage(playerid,0xFFFF00AA," + 차 견인 해제");
					DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));
					return;
					} else {
					Found=1;
					SendClientMessage(playerid,0xFFFF00AA," + 차를 견인합니다.");
					AttachTrailerToVehicle(vid,GetPlayerVehicleID(playerid));
					}
       			}
			}
			if  (!Found) {
				SendClientMessage(playerid,0xFFFF00AA," + 주위에 차가 없습니다.");
			}
			return;
		}
	}
/*	for(new j = 0; j < MAX_VEHICLES; j++) {
				if(GetVehicleModel(j) == 519) {
					GetVehiclePos(j,cx,cy,cz);
     						if( floatsqroot(  floatpower(px-cx,2.0) + floatpower(py-cy,2.0) + floatpower(pz-cz,2.0)) < 3.0) {
							if(newkeys == KEY_ANALOG_UP) {
							SetPlayerInterior(playerid,1);
							SetPlayerFacingAngle(playerid,0.0);
							SetPlayerPos(playerid,1.7540,24.4852,1199.5938);
							SetCameraBehindPlayer(playerid);
							InShamal[playerid]=j;
						}
					}
				}
			}
	for(new j = 0; j < MAX_VEHICLES; j++) {
				if(GetVehicleModel(j) == 592) {
					GetVehiclePos(j,cx,cy,cz);
							if( floatsqroot(  floatpower(px-cx,2.0) + floatpower(py-cy,2.0) + floatpower(pz-cz,2.0)) < 3.0) {
							if(newkeys == KEY_ANALOG_UP) {
   							SetPlayerInterior(playerid,9);
   							SetPlayerFacingAngle(playerid,0.0);
   							SetPlayerPos(playerid, 315.856170,1024.496459,1949.797363);
   							SetCameraBehindPlayer(playerid);
   							InAndrom[playerid]=j;
						}
					}
				}
			} */
//		new gstr[256];
//		format(gstr,sizeof(gstr)," + Key check it %d , %d",playerid,newkeys);
//		SendClientMessageToAll(COLOR_RED,gstr);
/*
		if(newkeys & 1024 || newkeys & 4) {
			if(mobile_state[playerid] == 4) {
				if( call_data[mobile_call[playerid]][cstate] == 1 ) {
					for(new i=0; i < MAX_PLAYERS; i++) {
						if(IsPlayerConnected(i)) {
							if(i != playerid && mobile_call[playerid] == mobile_call[i]) {
								mobile_call[i] = -1;
