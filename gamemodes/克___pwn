				    GiveMoney(playerid,-790);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$790",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:
		    if(GetMoney(playerid) >= 690)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 562)
		        {
		            AddVehicleComponent(car,1037);
		            GiveMoney(playerid,-690);
		            GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
		            ShowMenuForPlayer(Upgrades, playerid);
				}
				else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 565)
				{
				    AddVehicleComponent(car,1045);
				    GiveMoney(playerid,-690);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
				else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 559)
				{
				    AddVehicleComponent(car,1066);
				    GiveMoney(playerid,-690);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
				else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 561)
				{
				    AddVehicleComponent(car,1059);
				    GiveMoney(playerid,-690);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
				else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 560)
				{
				    AddVehicleComponent(car,1029);
				    GiveMoney(playerid,-690);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
				else if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 558)
				{
				    AddVehicleComponent(car,1092);
				    GiveMoney(playerid,-690);
				    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$690",3000,5);
				    ShowMenuForPlayer(Upgrades, playerid);
				}
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 2:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
    if(Current == Frontbumper) {
    switch(row){
        case 0:
            if(GetMoney(playerid) >= 990)
            {
                new car = GetPlayerVehicleID(playerid);
				if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 562)
                {
                    AddVehicleComponent(car,1171);
                    GiveMoney(playerid,-990);
                    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$990",3000,5);
                    ShowMenuForPlayer(Upgrades, playerid);
				}
