 	if(strcmp(cmd,"/nitro",true) == 0)
        {
            if(GetMoney(playerid) <= 30000) {
            SendClientMessage(playerid, COLOR_RED, " + 돈이 부족합니다 니트로 가격은 3만원 입니다");
			return 1;
			}
			if(quest[playerid] == 8) {
			SendClientMessage(playerid,COLOR_YELLOW," ");
			SendClientMessage(playerid,COLOR_YELLOW," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
			SendClientMessage(playerid,COLOR_YELLOW," + 보상: $5000, EXP : 20");
			quest[playerid] = 9;
			exp[playerid] += 20;
			GetPlayerLevel(playerid);
			GiveMoney(playerid, 5000);
			}
                if(!IsPlayerInAnyVehicle(playerid))
                    return SendClientMessage(playerid, 0xFF4040FF, " + 사람한테 니트로를 달려구요??");
            	new vid = GetPlayerVehicleID(playerid);
        		if(!IsVehicleSkinCompatible_TF(GetVehicleModel(vid)))
                return SendClientMessage(playerid, 0xFF4040FF, " + 그 차는 니트로를 달 수 없는 차량입니다.");
                AddVehicleComponent(vid, 1010); // 10x Nitro
                GiveMoney(playerid, -40000);
                Timers[playerid] = SetTimerEx("Update", 20000, true, "i", playerid); //시간초
                return SendClientMessage(playerid, 0x40FF40FF, " + 니트로를 달았습니다.");
        }


	if(strcmp(cmd, "/hitman", true) == 0) {
  tmp = strtok(cmdtext, idx);

  if(!strlen(tmp)) {
   SendClientMessage(playerid, COLOR_RED, " + /hitman [ID] [돈] : 현상금 걸기");
   SendClientMessage(playerid, COLOR_RED, " + 수수료는 $50000 입니다.");
   return 1;
  }
  giveplayerid = strval(tmp);

  tmp = strtok(cmdtext, idx);
  if(!strlen(tmp)) {
   SendClientMessage(playerid, COLOR_RED, " + /hitman [ID] [돈] : 현상금 걸기");
   SendClientMessage(playerid, COLOR_RED, " + 수수료는 $50000 입니다.");

   return 1;
  }
   moneys = strval(tmp);

     if(moneys + 50000 > GetMoney(playerid)) {
   SendClientMessage(playerid, COLOR_RED, " + 돈이 부족하군요.");
   return 1;
     }
     if(moneys < 1) {
   SendClientMessage(playerid, COLOR_RED, " + /hitman [ID] [돈] : 현상금 걸기");
   return 1;
  }
  if(IsPlayerConnected(giveplayerid)==0) {
   SendClientMessage(playerid, COLOR_RED, " + 그런 유저는 존재하지 않습니다.");
   return 1;
  }

  bounty[giveplayerid]+=moneys;
  GiveMoney(playerid, 0-moneys);

  GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
  GetPlayerName(playerid, sendername, sizeof(sendername));

  format(string, sizeof(string), " + %s 님의 목에 $%d 만큼의 현상금이 %s 님에게부터 걸렸습니다. (현재 총 걸린 금액: $%d).", giveplayer, moneys, sendername, bounty[giveplayerid]);
  SendClientMessageToAll(COLOR_RED, string);

  format(string, sizeof(string), " + 당신의 목에 $%d 만큼의 현상금이 %s(%d) 님으로부터 걸렸습니다.", moneys, sendername, playerid);
  SendClientMessage(giveplayerid, COLOR_BLUE, string);
  udb_UserSetInt(loginid[giveplayerid],"bounty",bounty[giveplayerid]);
  SendClientMessage(playerid, COLOR_CORAL, " + 경찰에게 현상금 수수료 $50000 을 냈습니다.");
  GiveMoney(playerid, -50000);
  SetPlayerWantedLevel(giveplayerid, 2);
  return 1;
 }

 //------------------- /bounty

 if(strcmp(cmd, "/bounty", true) == 0) {
  tmp = strtok(cmdtext, idx);

  if(!strlen(tmp)) {
   SendClientMessage(playerid, COLOR_RED, " + USAGE:/bounty [ID] : 현상금 보기");
   return 1;
  }
  giveplayerid = strval(tmp);

  if(IsPlayerConnected(giveplayerid)) {
   GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
   format(string, sizeof(string), " + %s (id: %d) 님의 현상금은 총 $%d 입니다.", giveplayer,giveplayerid,bounty[giveplayerid]);
   SendClientMessage(playerid, COLOR_RED, string);
  } else {
   SendClientMessage(playerid, COLOR_RED, " + 존재하지 않는 유저입니다.");
  }

  return 1;
 }
if(strcmp(cmd, "/claners", true) == 0)
 {
  new x;

     for(new i=0; i < MAX_PLAYERS; i++) {
   if(IsPlayerConnected(i) && udb_UserInt(loginid[i],"claner") == 1) {
    GetPlayerName(i, giveplayer, sizeof(giveplayer));
    format(string, sizeof(string), "%s%s(%d)", string,giveplayer,i);

    x++;
    if(x > 3) {
        SendClientMessage(playerid, COLOR_YELLOW, string);
        x = 0;
     format(string, sizeof(string), " ");
    } else {
     format(string, sizeof(string), "%s, ", string);
    }
   }
  }

  if(x <= 3 && x > 0) {
   string[strlen(string)-2] = '.';
      SendClientMessage(playerid, COLOR_YELLOW, string);
  }

  return 1;
 }
 //------------------- /bounties
 if(strcmp(cmd, "/bounties", true) == 0)
 {
  new x;

  SendClientMessage(playerid, COLOR_ORANGE, " + 현재 현상범:");
     for(new i=0; i < MAX_PLAYERS; i++) {
   if(IsPlayerConnected(i) && bounty[i] > 0) {
    GetPlayerName(i, giveplayer, sizeof(giveplayer));
    format(string, sizeof(string), "%s%s(%d): $%d", string,giveplayer,i,bounty[i]);

    x++;
    if(x > 3) {
        SendClientMessage(playerid, COLOR_YELLOW, string);
        x = 0;
     format(string, sizeof(string), " ");
    } else {
     format(string, sizeof(string), "%s, ", string);
    }
   }
  }

  if(x <= 3 && x > 0) {
   string[strlen(string)-2] = '.';
      SendClientMessage(playerid, COLOR_YELLOW, string);
  }

  return 1;
 }

	if(strcmp(cmd, "/flog", true) == 0) {
		new sellername[256];
		new propertyid;
		tmp = strtok(cmdtext,idx);
		propertyid = strval(tmp);
	    if (!strlen(tmp)) {
			SendClientMessage(playerid, COLOR_WHITE , " + USAGE: /flog [propertyid].");
			SendClientMessage(playerid, COLOR_WHITE , " + 내 소유 건물 보기: /mypro");
			SendClientMessage(playerid, COLOR_WHITE , " + 20%% 의 세금을 내고 원격으로 팝니다. 건물의 체크포인트에서 /sell 로 팔면 세금을 안냅니다.");
			return 1;
		}

		if(propertyid >= MAX_POINTS || propertyid <= 0) {
			SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 그 번호의 건물은 없습니다.");
			return 1;
		}

        if(!strlen(propertyOwner[propertyid])) {
			SendClientMessage(playerid, COLOR_SYSTEM_PW , " + 그 건물은 당신 것이 아닙니다.");
			return 1;
		}

		if (strcmp(loginid[playerid], propertyOwner[propertyid], true)) {
            format(strings, sizeof(strings), " + 그 건물은 로그인아이디 %s 님 것입니다.", propertyOwner[propertyid]);
			SendClientMessage(playerid, COLOR_SYSTEM_PW , strings);
			return 1;
		}
		if(strcmp(loginid[playerid], propertyOwner[propertyid], true) == 0) {
			GetPlayerName(playerid, playername, sizeof(playername));
			GetPlayerPos(playerid,X250[playerid],Y250[playerid],Z250[playerid]);
			PlayerPlaySound(playerid,1085,X250[playerid],Y250[playerid],Z250[playerid]);
			// TO-DO money scanner update
			GiveMoney(playerid, propertyValues[propertyid]/10*8);
			format(strings, sizeof(strings), " + 당신은 %s 를 팔아 $%d 를 얻었습니다.", propertyNames[propertyid], propertyValues[propertyid]/10*8);
			SendClientMessage(playerid, COLOR_YELLOW , strings);

			//New function when selling a property: message to all.
			GetPlayerName(playerid, sellername, sizeof(sellername));
			format(strings, sizeof(strings), " + %s 님은 %s 를 팔아서 $%d 를 얻었습니다.", sellername,propertyNames[propertyid],propertyValues[propertyid]/10*8);
			SendClientMessageToAll(COLOR_SYSTEM_GM , strings);
			propertyOwner[propertyid] = "";
			new keydata[255];
			ownedtime[propertyid] = 0;
			format(keydata,sizeof(keydata),"%d",propertyid);
			dini_Unset("p.cfg",keydata);
		}
		return 1;
	}

	if (strcmp(cmd, "/getout", true) == 0)
		{
        new str[256];
		tmp = strtok(cmdtext,idx);
		if(!strlen(tmp)){
			SendClientMessage(playerid, WHITE," + USAGE:/getout [승객번호]");
			return 1;
		}
		new id = strval(tmp);
		if(IsPlayerConnected(id)){
		    if(GetPlayerState(playerid)== PLAYER_STATE_DRIVER){
				if(GetPlayerVehicleID(playerid)==GetPlayerVehicleID(id)){
					RemovePlayerFromVehicle(id);
					GetPlayerName(id,name,24);
					format(str,sizeof(str)," + %s님께서 강제로 하차되셨습니다.",name);
					SendClientMessage(playerid,WHITE,str);
					format(str,sizeof(str)," + %s님께서 강제로 하차되셨습니다.",name);
					SendClientMessage(id,WHITE,str);
				}
				else SendClientMessage(playerid,RED," + 운전사와 같은차에 타고있지 않습니다.");
			}
			else SendClientMessage(playerid,RED," + 당신이 운전석에 타고있지 않습니다.");
		}
		return 1;
	}

	if (strcmp(cmd, "/taxi", true) == 0)
	{
	new str[256];
		tmp = strtok(cmdtext,idx);
		if(!strlen(tmp)){
			SendClientMessage(playerid, WHITE," + USAGE:/taxi [손님번호]");
			return 1;
		}
		if(MyJob[playerid] == JOB_TAXI) {
		new id = strval(tmp);
		if(IsPlayerConnected(id)){
		    if(GetPlayerState(playerid)== PLAYER_STATE_DRIVER && GetPlayerState(id)==PLAYER_STATE_PASSENGER){
				if(GetPlayerVehicleID(playerid)==GetPlayerVehicleID(id)){
					GetPlayerName(id,name,24);
					GetPlayerName(playerid,playername,24);
					SendClientMessage(playerid,SALMON," + 택시를 시작합니다.");
					format(str,sizeof(str)," + 택시기사 %s(%d)님의 요청으로  요금을 냅니다.",playername,playerid);
					SendClientMessage(id,SALMON,str);
        			taxi[id][driver]=playerid;
					taxi[playerid][custom]=id;
				}
				else SendClientMessage(playerid,RED," + 택시기사와 같은차에 타고있지 않습니다.");
			}
			else SendClientMessage(playerid,RED," + 당신이 운전자가 아니거나 그 사람은은 당신에 차에 탑승하지 않았습니다.");
		  }
		  else SendClientMessage(playerid,RED," + 접속하지 않은 사람입니다.");
		}
		else SendClientMessage(playerid,RED," + 당신은 택시기사를 직업으로 가지지 않았습니다.");

	    return 1;
	}

