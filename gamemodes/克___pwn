		{
			for(new i=0 ; i<MAX_PLAYERS ; i++)
			{
				if(IsPlayerConnected(i))
				{
					GetPlayerName(i, giveplayer, MAX_PLAYER_NAME);
					if(strcmp(tmp, giveplayer) == 0)
					{
						format(string, sizeof(string), " + %s 님의 ID : %d",tmp, i);
						SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
						return 1;
					}
				}
			}
			SendClientMessage(playerid, 0xFF0000AA," + 확인 되지 않은 사용자입니다.");
