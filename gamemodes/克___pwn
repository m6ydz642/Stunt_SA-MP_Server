				if(!strlen(stringslice(cmdtext,1))) {
			   	 SendClientMessage(playerid, COLOR_LIGHTBLUE, " + USAGE : /aboom [playerid]");
   					return 1;
				}
            	banid = strval(stringslice(cmdtext,1));
            if(IsPlayerConnected(banid)) {
				if(boomcheck[banid] == 0) {
				    booms[playerid] = 1;
					boomcheck[banid] = 1;
			        SendPlayerFormattedText(playerid," + 폭탄 장착, 불사신",0);
   			        SendPlayerFormattedText(banid," + 폭탄 장착, 불사신",0);
				} else {
				    booms[playerid] = 0;
					SetPlayerHealth(banid, 100);
					boomcheck[banid] = 0;
    				SendPlayerFormattedText(playerid," + 폭탄 장착해제, 불사신 풀림",0);
    				SendPlayerFormattedText(banid," + 폭탄 장착해제, 불사신 풀림",0);
