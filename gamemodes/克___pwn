	if (strcmp( stringslice(cmdtext,0), "/vcall", true)==0 && ismadmin(playerid)) {
		new id;
		new data;
	    if(!strlen(stringslice(cmdtext,1))) {
		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/vcall [playerid] [seatid]");
		    return 1;
	    }
	    if(!strlen(stringslice(cmdtext,2))) {
		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/vcall [playerid] [seatid]");
			return 1;
	    }
	    id = strval(stringslice(cmdtext,1));
	    data = strval(stringslice(cmdtext,2));
	    if(data > 13) {
		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/vcall [playerid] [seatid]");
			return 1;
		}
		if(!IsPlayerInAnyVehicle(playerid)) {
		   SendClientMessage(playerid, COLOR_GREEN, " + 차에 타!");
			return 1;
		}
		if(IsPlayerConnected(id)) {
			PutPlayerInVehicle(id,GetPlayerVehicleID(playerid),data);
		   SendClientMessage(playerid, COLOR_GREEN, " + GOOD!");
		} else {
			SendClientMessage(playerid, COLOR_GREEN, " + 비접속중");
		}
		return 1;
	}
	if (strcmp( stringslice(cmdtext,0), "/jail2", true)==0 && ismadmin(playerid)) {
