		        //SendClientMessage(playerid,COLOR_GREY," ");
		        //SendClientMessage(playerid,COLOR_GREY," ");
		        SendClientMessage(playerid,COLOR_RED, " + 당신의 퀘스트: /help 입력해서 도움말 보기");
				SendClientMessage(playerid,COLOR_ORANGE, " + 당신의 퀘스트: /help 입력해서 도움말 보기");
				SendClientMessage(playerid,COLOR_YELLOW, " + 아이디를 /log 1 1 이런식으로 만들거나 단순하면 삭제 됩니다");
				GiveMoney(playerid, 1000000); //기본자금
			}
			return 1;
		} else {
			loginsystem(playerid,1);
			SendClientMessage(playerid,COLOR_GREEN," + 이미 로그인 상태입니다."); //이미 로그인을 한 상태다.
			return 1;
