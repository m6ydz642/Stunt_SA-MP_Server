				ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 5:
		    if(GetMoney(playerid) >= 1100)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1073);
		        GiveMoney(playerid,-1100);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1100",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 6:
		    if(GetMoney(playerid) >= 1620)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1077);
		        GiveMoney(playerid,-1620);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1620",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 7:
		    if(GetMoney(playerid) >= 1560)
		    {
			    new car = GetPlayerVehicleID(playerid);
			    AddVehicleComponent(car,1083);
			    GiveMoney(playerid,-1560);
			    GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1560",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 8:
		    if(GetMoney(playerid) >= 1200)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1078);
		        GiveMoney(playerid,-1200);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1200",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 9:
		    if(GetMoney(playerid) >= 1560)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1076);
		        GiveMoney(playerid,-1560);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1560",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 10:
		    if(GetMoney(playerid) >= 1350)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1084);
		        GiveMoney(playerid,-1350);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1350",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 11:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
	if(Current == Carstereo) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 100)
	        {
	            new car = GetPlayerVehicleID(playerid);
	            AddVehicleComponent(car,1086);
	            GiveMoney(playerid,-100);
	            GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$100",3000,5);
	            ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
	if(Current == Hydraulics) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 1500)
	        {
				new car = GetPlayerVehicleID(playerid);
				AddVehicleComponent(car,1087);
				GiveMoney(playerid,-1500);
				GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1500",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
	if(Current == Nitro) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 200)
	        {
	            new car = GetPlayerVehicleID(playerid);
	            AddVehicleComponent(car,1009);
	            GiveMoney(playerid,-200);
	            GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$200",3000,5);
	            ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 1:
		    if(GetMoney(playerid) >= 500)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1008);
		        GiveMoney(playerid,-500);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$500",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
				GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
				ShowMenuForPlayer(Upgrades, playerid);
			}
		case 2:
		    if(GetMoney(playerid) >= 1000)
		    {
		        new car = GetPlayerVehicleID(playerid);
		        AddVehicleComponent(car,1010);
		        GiveMoney(playerid,-1000);
		        GameTextForPlayer(playerid,"~w~Component installed succesfully~n~~r~$1000",3000,5);
		        ShowMenuForPlayer(Upgrades, playerid);
			}
			else
			{
			    GameTextForPlayer(playerid,"~r~Not enough money!",3000,5);
			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 3:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
}

public OnPlayerExitedMenu(playerid)
{
	TogglePlayerControllable(playerid, true);
}

public GiveMoney(playerid,money) {
 	if((moneyhack[playerid] + money) >= 899000000) {
 	SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 돈이 최대치에 머물렀습니다. 돈이 날라가도 돈을 복구해드릴 수 없습니다.");
 	SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 은행에 입금 하던가 거지에게 자비를 베푸십시오.");
 	return 1;
 	}
	moneyhack[playerid] = moneyhack[playerid] + money;
	SetPlayerMoney(playerid,moneyhack[playerid]);
	if(loginflag[playerid] == 1) udb_UserSetInt(loginid[playerid],"money",moneyhack[playerid]);
	return 1;
}
public GetMoney(playerid) {
	return moneyhack[playerid];
}
public SetMoney(playerid,money) {
	if(money >= 899000000) {
 	SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 돈이 최대치에 머물렀습니다. 돈이 날라가도 돈을 복구해드릴 수 없습니다.");
 	SendClientMessage(playerid, COLOR_LIGHTBLUE, " + 은행에 입금 하던가 거지에게 자비를 베푸십시오.");
 	return 1;
 	}
	moneyhack[playerid] = money;
	SetPlayerMoney(playerid,money);
	if(loginflag[playerid] == 1) udb_UserSetInt(loginid[playerid],"money",moneyhack[playerid]);
	return 1;
}

public CasinoUpdate()
{

	for(new i=0; i<MAX_PLAYERS; i++) {
		if(IsPlayerConnected(i)) {
			spawn_de[i] = 0;
          for(new j=0; j < MAX_3DLOCS; j++) {
				GetPlayerPos(i, X250[i], Y250[i], Z250[i]);
				if(X250[i] >= casCoords[j][0] && X250[i] <= casCoords[j][3] && Y250[i] >= casCoords[j][1] && Y250[i] <= casCoords[j][4] && Z250[i] >= casCoords[j][2] && Z250[i] <= casCoords[j][5]) {
							SendClientMessage(i, COLOR_CYAN," + 카지노를 허용하지 않습니다.");
				   			SetPlayerInterior(i, 0);
							SetPlayerPos(i, casSpawn[j][0], casSpawn[j][1], casSpawn[j][2]);
				return 1;
			    }
			}
		}
	}
 	return 1;
}

