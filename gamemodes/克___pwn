	if (strcmp("/setramp ", cmdtext, true, 9) == 0)
	{
		new ramp = strval(cmdtext[9]);
		if (ramp < 0 || ramp >= sizeof(ramptypes)) {
		    SendClientMessage(playerid, 0xFFFFFFAA, " + 그 번호의 스턴트대는 없습니다 (1에서 5까지만 있습니다)");
		} else {
			playerramptypes[playerid] = ramp;
			SendClientMessage(playerid, 0xFFFFFFAA, " + 소환될 스턴트대가 바뀌었습니다.");
	 	}
	    return 1;
	}

	if(strcmp(cmd, "/setramp", true) == 0)
	{
