			    ShowMenuForPlayer(Upgrades, playerid);
			}
		case 8:ShowMenuForPlayer(Upgrades, playerid);
	}
	}
	if(Current == Exhausts) {
	switch(row){
	    case 0:
	        if(GetMoney(playerid) >= 790)
	        {
	            new car = GetPlayerVehicleID(playerid);
	            if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 562)
	            {
	            	AddVehicleComponent(car,1034);
	            	GiveMoney(playerid,-790);
