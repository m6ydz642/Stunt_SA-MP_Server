	if(strcmp(cmd, "/call", true) == 0) {
	if (isadmin(playerid)) {
			tmp = strtok(cmdtext, idx);
			if(!strlen(tmp)) {
				SendPlayerFormattedTextr(playerid," + 사용: /call ",0);
			} else {
				new Float:X;
				new Float:Y;
				new Float:Z;
			    if(strval(tmp) == 999 && isadmin(playerid)) {
					for(new i=0; i<MAX_PLAYERS; i++) {
						if (IsPlayerConnected(i) && PlayerBoxing[i] == 0) {
							SetPlayerInterior(i,GetPlayerInterior(playerid));
							GetPlayerPos(playerid,X,Y,Z);
							if(IsPlayerInAnyVehicle(i)) {
								SetVehiclePos(GetPlayerVehicleID(i), X + 0.01, Y + 0.01, Z);
								SetVehicleVirtualWorld(GetPlayerVehicleID(i),GetPlayerVirtualWorld(playerid));
							} else {
								SetPlayerPos(i, X + 0.01, Y + 0.01, Z);
							}
							SetPlayerVirtualWorld(i,GetPlayerVirtualWorld(playerid));
						}
					}
                    SendPlayerFormattedTextr(playerid," + 전체 소환완료",0);
                    return 1;
			    }
				if(!IsPlayerConnected(strval(tmp))) {
					SendPlayerFormattedTextr(playerid," + 유저가 존재하지 않습니다.",0);
				} else {
                    if(minimatchplayer[playerid] == 1 || chainmatchplayer[playerid] == 1 || eaglematchplayers[playerid] == 1 || uzimatchplayer[playerid] == 1){
                   	SendPlayerFormattedTextr(playerid," + 그 분은 일을 하고 계십니다.",0);
					return 1;
					}
					if(PlayerBoxing[strval(tmp)] < 0) {
					SendPlayerFormattedTextr(playerid," + 그 분은 일을 하고 계십니다.",0);
					return 1;
					}
					if(IsPlayerInAnyVehicle(strval(tmp))) {
					    //They're in a vehicle. Transport it, too.
					    SetPlayerInterior(strval(tmp),GetPlayerInterior(playerid));
					    VehicleID = GetPlayerVehicleID(strval(tmp));
						GetPlayerPos(playerid,X,Y,Z);
						SetVehiclePos(VehicleID, X + 0.01, Y + 0.01, Z);
						SetVehicleVirtualWorld(GetPlayerVehicleID(VehicleID),GetPlayerVirtualWorld(playerid));
					} else {
						SetPlayerInterior(strval(tmp),GetPlayerInterior(playerid));
						GetPlayerPos(playerid,X,Y,Z);
						SetPlayerPos(strval(tmp), X + 0.01, Y + 0.01, Z);
						SetPlayerPos(strval(tmp), X + 0.01, Y + 0.01, Z);
						SetPlayerPos(strval(tmp), X + 0.01, Y + 0.01, Z);
						SetPlayerVirtualWorld(strval(tmp),GetPlayerVirtualWorld(playerid));
					}
				    new tempstr[256];
				    GetPlayerName(strval(tmp),giveplayername,sizeof(giveplayername));
				    format(tempstr, sizeof(tempstr), " + 당신은 %s님을 소환하셨습니다.",giveplayername);
					SendPlayerFormattedTextg(playerid,tempstr,0);
				}
			}
			}
			return 1;
		}

	if(strcmp(cmd, "/dash", true) == 0 && isJail[playerid] == 0) {
		if(!cspeed[playerid]) {
          cspeed[playerid] = 1;
          SendPlayerFormattedText(playerid," + 대쉬보드 작동됨 :)",0);
		} else {
          cspeed[playerid] = 0;
          SendPlayerFormattedText(playerid," + 대쉬보드 작동해제됨 :)",0);
        }
	    return 1;
	}
