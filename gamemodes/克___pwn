	if (strcmp( stringslice(cmdtext,0), "/admindoor", true)==0 && ismadmin(playerid)) {
		new adoors;
	    if(!strlen(stringslice(cmdtext,1))) {
		    SendClientMessage(playerid, COLOR_GREEN, " + USAGE:/admindoor [open key(1~6)]");
	    }
	    adoors = strval(stringslice(cmdtext,1));
	    if(adoors == 1) {
			MoveObject(objects[0],doors[0][0],doors[0][1],doors[0][2],MOVE_SPEED);
			MoveObject(objects[1],doors[1][0],doors[1][1],doors[1][2],MOVE_SPEED);
			doors_open[0] = -2;
			doors_open[1] = -2;
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 닫힙니다.");
		} else if(adoors == 2) {
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 열립니다.()");
			MoveObject(objects[0],doors[0][0]+5.0,doors[0][1],doors[0][2],MOVE_SPEED);
			MoveObject(objects[1],doors[1][0]-5.0,doors[1][1],doors[1][2],MOVE_SPEED);
			doors_open[0] = -1;
			doors_open[1] = -1;
		} else if(adoors == 3) {
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 닫힙니다.");
			MoveObject(objects[0],doors[0][0],doors[0][1],doors[0][2],MOVE_SPEED);
		} else if(adoors == 4) {
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 닫힙니다.");
			MoveObject(objects[1],doors[1][0],doors[1][1],doors[1][2],MOVE_SPEED);
		} else if(adoors == 5) {
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 열립니다.()");
			MoveObject(objects[0],doors[0][0]+5.0,doors[0][1],doors[0][2],MOVE_SPEED);
		} else if(adoors == 6) {
			SendClientMessage(playerid, COLOR_YELLOW," + 문이 열립니다.()");
			MoveObject(objects[1],doors[1][0]-5.0,doors[1][1],doors[1][2],MOVE_SPEED);
		} else {
			SendClientMessage(playerid, COLOR_YELLOW," + 에러");
		}
		return 1;
	}
if (strcmp( stringslice(cmdtext,0), "/reg", true)==0) {
//   Nameerrorck(PName[playerid]); 이 함수는 기본 아이디를 저장할 때 특수문자를 사용하는 사람때문에 발생하는 에러를 위해 만들었으나 지금 이 강좌에선 사용 안 함.
		if(loginflag[playerid] == 0) {
			if(!strlen(stringslice(cmdtext,1)) || !strlen(stringslice(cmdtext,2))) {
				SendClientMessage(playerid,COLOR_GREEN," + Usage: /reg [login id] [password]"); //다시 입력하라는 메시지 출력
			} else {
			    new aloginid[255];
			    new aloginpass[255];
			    aloginid = stringslice(cmdtext,1);
			    InvertCase(aloginid);
				aloginpass = stringslice(cmdtext,2);
				if(check_name(aloginid)) {
					SendClientMessage(playerid,COLOR_GREEN," + 올바른 아이디 형식이 아닙니다.");
					return 1;
				}
				if(udb_Exists(aloginid)) {
					SendClientMessage(playerid,COLOR_GREEN," + 이미 있는 아이디입니다."); // 이미 로그인을 한 상태다.
					return 1;
				}
				udb_Create(aloginid,aloginpass);
				loginflag[playerid] = 1;
				loginid[playerid] = aloginid;
				loginpw[playerid] = aloginpass;

				mobile_state[playerid] = 3;
				loginsystem(playerid,1);
				SendClientMessage(playerid,COLOR_GREEN," + 아이디를 만들었습니다 "); // 이미 로그인을 한 상태다.
				new str[255];
				GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
				format(str, sizeof(str), " + %s(%d)님이 %s로 로그인하셨습니다.", playername ,playerid,loginid[playerid]);
                printf(" + %s(%d)님이 로그인하셨습니다)", playername ,playerid); //도스창에 결과출력
				SendClientMessageToAll(COLOR_GREEN, str);
				//cyan 색깔 저장
	  			//SendClientMessage(playerid,COLOR_GREY," ");
		        //SendClientMessage(playerid,COLOR_GREY," ");
		        //SendClientMessage(playerid,COLOR_GREY," ");
		        //SendClientMessage(playerid,COLOR_GREY," ");
