	    if(!strcmp(cmdtext,"/draw",true,5))
    {
        if(!strlen(cmdtext[6])) SendClientMessage(playerid, COLOR_GREEN, "USAGE: /draw [x] [y] [font] [text]");
        else {
            new Float:pos[2], font;
            pos[0] = floatstr(strtok(cmdtext,idx));
            pos[1] = floatstr(strtok(cmdtext,idx));
            font = strval(strtok(cmdtext,idx));

            if(!strlen(cmdtext[idx])) SendClientMessage(playerid, COLOR_GREEN, "USAGE: /draw [x] [y] [font] [text]");
            else {
                TextDrawDestroy(testtext);
                testtext = TextDrawCreate(pos[0], pos[1], cmdtext[idx]);
                TextDrawUseBox(testtext, 0);
                TextDrawFont(testtext, font);
                TextDrawSetShadow(testtext,0);
                TextDrawSetOutline(testtext,0);
                TextDrawBackgroundColor(testtext,0x000000FF);
                TextDrawColor(testtext,0xFFFFFFFF);
                TextDrawShowForAll(testtext);
				SendClientMessage(playerid, COLOR_GREEN, "Nice!");
            }
        }
        return 1;
    }


