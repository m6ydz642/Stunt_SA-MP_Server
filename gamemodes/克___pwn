	switch (getCheckpointType(playerid)) {

	case CP_JOBHAVE:
	    {
     	SendClientMessage(playerid,COLOR_ORANGE," + 직업을 얻고싶나? 그러면 /joblist를 쳐봐.");
  		SendClientMessage(playerid,COLOR_ORANGE," + 지금은 직업이 별로 없지만 많이 생길꺼야.");
	    }
	case CP_HUNTER:
	    {
	    if(worker[playerid]==0)
	        {
   	 		SendClientMessage(playerid,COLOR_WHITE," + 돈을 벌고 싶나요?");
			format(string,sizeof(string)," + 바산근처 공사장까지 물건가져다주면 $%d 를 드릴께요~ 그대신 열심히 하세요!!",AB_MONEY_1);
			SendClientMessage(playerid,COLOR_WHITE,string);
			SendClientMessage(playerid,COLOR_WHITE," + 운송 알바 하기: /work, 포기하기 : /work");
			}
		if(worker[playerid] == 1) {
		if(quest[playerid] == 2) {
			SendClientMessage(playerid,COLOR_CRIMSON," ");
			SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
			SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $10000, EXP: 20");
			GiveMoney(playerid, 10000);
			quest[playerid] ++;
			exp[playerid] += 20;
			GetPlayerLevel(playerid);
			}
  		SendClientMessage(playerid,COLOR_YELLOW," + 수고했습니다, 여기 일당받아요.");
		format(string,sizeof(string)," + $%d를 받았습니다.",AB_MONEY_1);
		SendClientMessage(playerid,COLOR_GREEN,string);
		GiveMoney(playerid,AB_MONEY_1);
		worker[playerid]=0;
		exp[playerid]++;
		GetPlayerLevel(playerid);
	    }
	}
	case CP_COLLEGE:
	    {
