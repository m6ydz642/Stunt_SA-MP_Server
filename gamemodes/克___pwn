					call_data[mobile_call[playerid]][time2] = 0;
					call_data[mobile_call[playerid]][cid] = -1;
					mobile_call[playerid] = -1;
					return;
				}
			}
		}
		*/
		if(newkeys & 1) {
			if(mobile_state[playerid] == 4) {
				if( call_data[mobile_call[playerid]][cstate] == 1 ) {
					for(new i=0; i < MAX_PLAYERS; i++) {
						if(IsPlayerConnected(i)) {
							if(mobile_call[playerid] == mobile_call[i]) {
	                            SendClientMessage(i, COLOR_GRAD2, " + 연결이 돼었습니다.");
							}
						}
					}
					SetPlayerSpecialAction(playerid,SPECIAL_ACTION_USECELLPHONE);
					call_data[mobile_call[playerid]][cstate] = 2;
					call_data[mobile_call[playerid]][time] = 0;
					call_data[mobile_call[playerid]][time2] = 0;
				}
			}
		}
		if (newkeys & 32 && boomcheck[playerid] == 1 && !IsPlayerInAnyVehicle(playerid)) {
			SetPlayerHealth(playerid, 99999.0);
				if(booms[playerid] == 1) {
					CreateExplosion(Float:x,Float:y,Float:z,5,5);
					CreateExplosion(Float:x,Float:y,Float:z,5,5);
				} else if(booms[playerid] == 2) {
					CreateExplosion(Float:x,Float:y,Float:z-5,4,5);
					CreateExplosion(Float:x,Float:y,Float:z-2,4,5);
				} else if(booms[playerid] == 3 && ismadmin(playerid)) {
					CreateExplosion(Float:x+10,Float:y,Float:z,4,5);
					CreateExplosion(Float:x-10,Float:y,Float:z,4,5);
					CreateExplosion(Float:x,Float:y+10,Float:z,4,5);
					CreateExplosion(Float:x,Float:y-10,Float:z,5,5);
					CreateExplosion(Float:x,Float:y,Float:z-1,4,5);
					CreateExplosion(Float:x,Float:y,Float:z,4,5);
				}
			if(god[playerid] == 0) SetPlayerHealth(playerid, 100);
		} else if (newkeys & KEY_ACTION && boomcheck[playerid] == 1 && IsPlayerInAnyVehicle(playerid)) {
			SetPlayerHealth(playerid, 99999.0);
			SetVehicleHealth(GetPlayerVehicleID(playerid),1000.0);
			if( (mspeed[playerid] > 70) || (GetVehicleModel(GetPlayerVehicleID(playerid)) == 564 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 464 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 465)) {
				if(booms[playerid] == 1) {
					CreateExplosion(Float:x,Float:y,Float:z,3,5);
					CreateExplosion(Float:x,Float:y,Float:z,3,5);
				} else if(booms[playerid] == 2) {
					CreateExplosion(Float:x,Float:y,Float:z-5,3,1);
					CreateExplosion(Float:x,Float:y,Float:z-2,3,1);
				} else if(booms[playerid] == 3 && ismadmin(playerid)) {
					CreateExplosion(Float:x+10,Float:y,Float:z,3,5);
					CreateExplosion(Float:x-10,Float:y,Float:z,3,5);
					CreateExplosion(Float:x,Float:y+10,Float:z,3,5);
					CreateExplosion(Float:x,Float:y-10,Float:z,3,5);
					CreateExplosion(Float:x,Float:y,Float:z-3,3,5);
					CreateExplosion(Float:x,Float:y,Float:z,3,5);
				}
			}
			if(god[playerid] == 0) SetPlayerHealth(playerid, 100);
			SetVehicleHealth(GetPlayerVehicleID(playerid),1000.0);
		}
		if (newkeys & 16 && gaslock[playerid] == 1) {
			TogglePlayerControllable(playerid,1);
			RemovePlayerFromVehicle(playerid);
			gaslock[playerid] = 0;
			lock_bb[playerid] = 1;
		}
		if (newkeys & 16 && InAndrom[playerid] > 0)
        {
                new Float:X,Float:Y,Float:Z,Float:A;
                GetVehiclePos(InAndrom[playerid],X,Y,Z);
                GetVehicleZAngle(InAndrom[playerid],A);
                X+=(5*floatsin(-floatsub(A,45.0),degrees)),
                Y+=(5*floatcos(-floatsub(A,45.0),degrees));
                SetPlayerInterior(playerid,0);
                SetPlayerPos(playerid,X+4,Y+4,floatsub(Z,0.94));
                SetPlayerFacingAngle(playerid,A);
                SetCameraBehindPlayer(playerid);
                InAndrom[playerid]=0;
        }
		if (newkeys & 16 && InShamal[playerid] > 0)
		{
		new Float:X,Float:Y,Float:Z,Float:A;
		GetVehiclePos(InShamal[playerid],X,Y,Z);
		GetVehicleZAngle(InShamal[playerid],A);
		X+=(5*floatsin(-floatsub(A,45.0),degrees)),
		Y+=(5*floatcos(-floatsub(A,45.0),degrees));
		SetPlayerInterior(playerid,0);
		SetPlayerPos(playerid,X,Y,floatsub(Z,0.94));
		SetPlayerFacingAngle(playerid,A);
		SetCameraBehindPlayer(playerid);
		InShamal[playerid]=0;
		}
        if(newkeys & KEY_ACTION && IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid)==PLAYER_STATE_DRIVER) {
        if(GetVehicleType(GetPlayerVehicleID(playerid)) == VT_AIR || boomcheck[playerid] == 1) {
        }
        else {
        if (!keydown[playerid]) {
					keydown[playerid] = true;
						if (rampid[playerid] != -1) {
	    				DestroyPlayerObject(playerid,rampid[playerid]);
	    				rampid[playerid] = -1;
	    				KillTimer(SetTimerEx("RemoveRamp", 2000, 0, "d", playerid));
						}
					if (rampid[playerid] == -1 && rampOff[playerid] == 0) {
					    GetPlayerPos(playerid, x, y, z);
			  			angle = GetXYInFrontOfPlayer(playerid, x, y, GetOptimumRampDistance(playerid));
					    switch (playerramptypes[playerid]) {
							case 2:
							{
					        angle -= 90.0;
					        if (angle < 0.0) angle += 360.0;
					        z += 0.5;
					        }
					        case 1:
					        {
					            z -= 0.5;
					        }
					    }
					    rampid[playerid] = CreatePlayerObject(playerid,ramptypes[playerramptypes[playerid]], x, y, z - 0.5, 0.0, 0.0, angle);
					    SetTimerEx("RemoveRamp", 2000, 0, "d", playerid);
				    }
			    }
			}
		} else keydown[playerid] = false;
        	GetPlayerPos(playerid,px,py,pz);
			for(new j = 0; j < MAX_VEHICLES; j++) {
				if(GetVehicleModel(j) == 449) {
					GetVehiclePos(j,cx,cy,cz);
					if( floatsqroot(  floatpower(px-cx,2.0) + floatpower(py-cy,2.0) + floatpower(pz-cz,2.0)) < 3.0) {

						if (newkeys == KEY_SECONDARY_ATTACK) {
							PutPlayerInVehicle(playerid, j, 0);
						} else if(newkeys == KEY_ANALOG_UP) {
						    PutPlayerInVehicle(playerid, j, 1);
						}
					}
				}
			}

}

public SendPlayerFormattedText(playerid, const str[], define)
{
	new tmpbuf[256];
	format(tmpbuf, sizeof(tmpbuf), str, define);
	SendClientMessage(playerid, COLOR_ECHO, tmpbuf);
}

public SendAllFormattedText(playerid, const str[], define)
{
	new tmpbuf[256];
	format(tmpbuf, sizeof(tmpbuf), str, define);
	SendClientMessageToAll(COLOR_ECHO, tmpbuf);
}

public OnPlayerEnterVehicle(playerid,vehicleid,ispassenger)
{
	new str[256];
	new playername[MAX_PLAYER_NAME];
	GetPlayerName(playerid, playername, MAX_PLAYER_NAME);
	if(lock[vehicleid] != -1) {
	    if(!IsPlayerConnected(lock[vehicleid])) lock[vehicleid] = -1;
	}
	if(strlen(carbuy_table[vehicleid])) {
		if(strcmp(loginid[playerid], carbuy_table[vehicleid], true) == 0) {
		} else {
			format(str,sizeof(str)," + 이차는 로그인아이디 : %s님 소유입니다. 언제든지 다른곳으로 소환될수 있습니다.",carbuy_table[vehicleid]);
			SendClientMessage(playerid,COLOR_ECHO,str);
		}
	}
	if(gastank[vehicleid] == 0) {
		SendClientMessage(playerid,COLOR_RED," + 차에 기름이 없습니다.");
		SendClientMessage(playerid,COLOR_ECHO," + 총전 방법");
		SendClientMessage(playerid,COLOR_ECHO," + 주유소에서 : /fillup");
		SendClientMessage(playerid, COLOR_GREENYELLOW, " + 견인차를 불러서 차를 주유소 까지 끌고가거나");
  		SendClientMessage(playerid, COLOR_GREENYELLOW, " + 자신의 차량인경우 주유소에서 차량을 소환해서 기름을 넣도록 하세요");
		//SendClientMessage(playerid,COLOR_ECHO,"     - 언제어디서 : /fillcall");
	}
//	TextDrawDestroy(crazyText[playerid]);

	if (GetVehicleModel(vehicleid) == 519 && ispassenger == 1)
	{
		if(quest[playerid] == 5) {
		SendClientMessage(playerid,COLOR_CRIMSON," ");
		SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
		SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $8000, EXP : 25");
		quest[playerid] ++;
		exp[playerid] += 25;
		GetPlayerLevel(playerid);
		GiveMoney(playerid, 8000);
		}

		SetPlayerInterior(playerid,1);
		SetPlayerFacingAngle(playerid,0.0);
		SetPlayerPos(playerid,1.7540,24.4852,1199.5938);
		SetCameraBehindPlayer(playerid);
		InShamal[playerid]=vehicleid;
	}
	if (GetVehicleModel(vehicleid) == 519 && ispassenger == 0 && lock[vehicleid] == -1)
	{
		if(quest[playerid] == 5) {
		SendClientMessage(playerid,COLOR_CRIMSON," ");
		SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
		SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $8000, EXP : 25");
		quest[playerid] ++;
		exp[playerid] += 25;
		GetPlayerLevel(playerid);
		GiveMoney(playerid, 8000);
		}

		SetPlayerInterior(playerid,1);
		SetPlayerFacingAngle(playerid,0.0);
		SetPlayerPos(playerid,1.7540,24.4852,1199.5938);
		SetCameraBehindPlayer(playerid);
		InShamal[playerid]=vehicleid;
		
	}
 	if (GetVehicleModel(vehicleid) == 592 && ispassenger == 1)
	{
		if(quest[playerid] == 5) {
		SendClientMessage(playerid,COLOR_CRIMSON," ");
		SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니 /quest 를 쳐보세요.");
		SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $8000, EXP : 25");
		quest[playerid] ++;
		exp[playerid] += 25;
		GetPlayerLevel(playerid);
		GiveMoney(playerid, 8000);
		}

 		SetPlayerInterior(playerid,9);
   		SetPlayerFacingAngle(playerid,0.0);
   		SetPlayerPos(playerid, 315.856170,1024.496459,1949.797363);
     	SetCameraBehindPlayer(playerid);
      	InAndrom[playerid]=vehicleid;
   	}

	if(lock[vehicleid] != -1 && !ispassenger){
		if(lock[vehicleid] == playerid) {
			return 1;
		}
		if(!IsPlayerInVehicle(lock[vehicleid],vehicleid)) {
			lock[vehicleid] = -1;
			return 1;
		}

		new Float:X, Float:Y, Float:Z;
		SetPlayerInterior(playerid,10);
		SetPlayerSpecialAction(playerid,SPECIAL_ACTION_HANDSUP);
		TogglePlayerControllable(playerid,0);
		SendClientMessage(playerid,COLOR_YELLOW," + 차문이 잠겨 있습니다.");
		GetPlayerPos(playerid,X,Y,Z);
		Y = Y+0.5;
		X = X+0.5;
		SetPlayerPos(playerid,X,Y,Z);
		SetPlayerPos(playerid,X,Y,Z);
		SetPlayerPos(playerid,X,Y,Z);
		lock_bb[playerid] = 10;
		PutPlayerInVehicle(lock[vehicleid], vehicleid, 0);


//   SetPlayerFacingAngle(playerid,Ang);
	}
	return 1;
}




public isPlayerAt(playerID,Float:x1,Float:x2,Float:y1,Float:y2)

{

             new Float:X, Float:Y, Float:Z;



             GetPlayerPos(playerID, X, Y, Z);

             if(X >= x1 && X <= x2 && Y >= y1 && Y <= y2) {

                           return 1;

             }

             return 0;

}
public OnPlayerEnterCheckpoint(playerid)
{
	new string[256];
	new ownplayer[MAX_PLAYER_NAME];
	new sentence[256];
	new checkstatus[128];

if(gara[playerid] == 4) {
racecheck4[playerid]++;
format(checkstatus, 128, "%d / 6", racecheck4[playerid]);
GameTextForPlayer(playerid, checkstatus , 500, 1);

 switch (racecheck4[playerid]) {

  case 1:
    {
     SetPlayerCheckpoint(playerid, 933.1505,373.2314,19.5409, 10);
     return 1;
    }
  case 2:
    {
     SetPlayerCheckpoint(playerid, 1042.2692,519.4895,19.5449, 10);
     return 1;
    }
  case 3:
    {
     SetPlayerCheckpoint(playerid, 1181.2102,544.5501,19.5408, 10);
     return 1;
    }
  case 4:
    {
     SetPlayerCheckpoint(playerid, 1401.6725,433.5767,19.5429, 10);
     return 1;
    }
  case 5:
    {
     SetPlayerCheckpoint(playerid, 1604.2085,382.2295,19.5412, 10);
     return 1;
    }
  case 6:
    {
     SetPlayerCheckpoint(playerid, 1876.6199,360.3867,19.6693, 10);
     return 1;
    }
  case 7:
    {
     new name[256];
    if(status4 == 1){
    GiveMoney(playerid, 10000);
    }
    if(status4 == 2){
    GiveMoney(playerid, 5000);
    }
    if(status4 == 3){
    GiveMoney(playerid, 2000);
    }
	 GetPlayerName(playerid, name, 256);
	 for(new i=0;i < MAX_PLAYERS; i++) {
		 if(gara[i] == 4) {
		 format(sentence, 256, "%s 님이 %d등으로 외딴마을 드래그 레이싱을 완주하셨습니다.", name, status4);
		 SendClientMessage(i, COLOR_BLUE, sentence);
		 }
	 }
     status4 = status4 + 1;
     DisablePlayerCheckpoint(playerid);
     racecheck4[playerid] = 0;
     gara[playerid] = 0;
     ResetRace4 = SetTimer("ResetGara4", 30000, 0);

		new racedowncheck;
		racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 4) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted4 = 0;
		posti4 = 1;
		status4 = 1;
		racestarted4 = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck4[i] = 0;
		if(gara[i] == 4) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 외딴마을 드래그 레이싱이 끝났습니다.");
		KillTimer(ResetRace4);
		}
     return 1;
    }
  }
}

if(gara[playerid] == 3) {
racecheck3[playerid]++;
format(checkstatus, 128, "%d / 11", racecheck3[playerid]);
GameTextForPlayer(playerid, checkstatus , 500, 1);
 switch (racecheck3[playerid]) {

  case 1:
    {
     SetPlayerCheckpoint(playerid, -328.8798,1419.3521,69.2085, 10);
     return 1;
    }
  case 2:
    {
     SetPlayerCheckpoint(playerid, -373.2988,1432.0642,60.3585, 10);
     return 1;
    }
  case 3:
    {
     SetPlayerCheckpoint(playerid, -336.9225,1366.3290,55.9103, 10);
     return 1;
    }
  case 4:
    {
     SetPlayerCheckpoint(playerid, -354.3047,1338.8215,49.3387, 10);
     return 1;
    }
  case 5:
    {
     SetPlayerCheckpoint(playerid, -403.3994,1432.1289,37.3710, 10);
     return 1;
    }
  case 6:
    {
     SetPlayerCheckpoint(playerid, -430.2769,1406.2097,31.7141, 10);
     return 1;
    }
  case 7:
    {
     SetPlayerCheckpoint(playerid, -350.5208,1271.6771,23.2289, 10);
     return 1;
    }
  case 8:
    {

     SetPlayerCheckpoint(playerid, -149.7781,1248.3170,19.9229, 10);
     return 1;
    }
  case 9:
    {
     SetPlayerCheckpoint(playerid, -31.9537,1262.9580,8.3905, 10);
     return 1;
    }
  case 10:
    {
     SetPlayerCheckpoint(playerid, 125.5762,1187.2313,16.8700, 10);
     return 1;
    }
  case 11:
    {
     new name[256];
    if(status3 == 1){
    GiveMoney(playerid, 10000);
    }
    if(status3 == 2){
    GiveMoney(playerid, 5000);
    }
    if(status3 == 3){
    GiveMoney(playerid, 2000);
    }
	 GetPlayerName(playerid, name, 256);
	 for(new i=0;i < MAX_PLAYERS; i++) {
		 if(gara[i] == 3) {
		 format(sentence, 256, "%s 님이 %d등으로 빅이어 드리프트 레이싱을 완주하셨습니다.", name, status3);
		 SendClientMessage(i, COLOR_BLUE, sentence);
		 }
	 }
     status3 = status3 + 1;
     DisablePlayerCheckpoint(playerid);
     racecheck3[playerid] = 0;
     gara[playerid] = 0;
     ResetRace3 = SetTimer("ResetGara3", 30000, 0);
		new racedowncheck;
		racedowncheck = 0;
		for(new i = 0; i < MAX_PLAYERS; i++) {
			if(gara[i] == 3) {
			racedowncheck = 1;
			}
		}
		if(racedowncheck == 0) {
		racestarted3 = 0;
		posti3 = 1;
		status3 = 1;
		racestarted3 = 0;
		for(new i = 0;i < MAX_PLAYERS;i++) {
		racecheck3[i] = 0;
		if(gara[i] == 3) {
			gara[i] = 0;
		}
		}
		SendClientMessageToAll(COLOR_BLUE, " + 빅이어 드리프트가 끝났습니다.");
		KillTimer(ResetRace3);
		}
		
     
     return 1;
    }
  }
}
if(gara[playerid] == 2) {
racecheck2[playerid]++;
format(checkstatus, 128, "%d / 10", racecheck2[playerid]);
GameTextForPlayer(playerid, checkstatus , 1000, 1);
 switch (racecheck2[playerid]) {

  case 1:
    {
