		if(strcmp(cmd, "/sboom", true) == 0) {
			booms[playerid]++;
			if(booms[playerid] == 3 && !ismadmin(playerid))booms[playerid] = 1;
			if(booms[playerid] == 4)booms[playerid] = 1;
			if(booms[playerid] == 1) {
				SendPlayerFormattedText(playerid," + 일반폭탄",0);
			} else if(booms[playerid] == 2) {
				SendPlayerFormattedText(playerid," + 아래폭탄",0);
			} else if(booms[playerid] == 3) {
				SendPlayerFormattedText(playerid," + 스페셜 폭탄",0);
			}
			return 1;
		}
		if(strcmp(cmd, "/bboom", true) == 0 && isadmin(playerid)) {
			if(boomcheck[playerid] == 0) {
			    booms[playerid] = 1;
		        boomcheck[playerid] = 1;
		        SendPlayerFormattedText(playerid," + 폭탄 장착, 불사신",0);
			} else {
			    booms[playerid] = 0;
				SetPlayerHealth(playerid, 100);
			    boomcheck[playerid] = 0;
                SendPlayerFormattedText(playerid," + 폭탄 장착해제, 불사신 풀림",0);
			}
			return 1;
		}
		if (strcmp("/aboom", cmd) == 0) {
