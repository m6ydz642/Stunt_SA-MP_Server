PayPlayerInArea(playerID, Float:x1, Float:y1, Float:x2, Float:y2, cash)
{

	if(IsPlayerConnected(playerID))
	{
		new Float:X, Float:Y, Float:Z;

		GetPlayerPos(playerID, X, Y, Z);
		if(X >= x1 && X <= x2 && Y >= y1 && Y <= y2)
		{
			GiveMoney(playerID, cash);
			expup[playerID] ++;
	  			if(expup[playerID] == 15) {
	  			exp[playerID] ++;
	  			expup[playerID] = 0;
	  			GetPlayerLevel(playerID);
	  			}

			return 1;
		}
	}
	return 0;
}

isPlayerInArea(playerid, Float:data[4])
{
	new Float:x, Float:y, Float:z;

	GetPlayerPos(playerid, x, y, z);
	if (x >= data[0] && x <= data[2] && y >= data[1] && y <= data[3]) return 1;

	return 0;
}

public PirateShipScoreUpdate()
{
	new Float:x, Float:y, Float:z;

	for (new i=0; i<MAX_PLAYERS; i++) {
	    if (IsPlayerConnected(i)) {
			GetPlayerPos(i, x, y, z);
			PayPlayerInArea(i, 1995.5, 1518.0, 2006.0, 1569.0, 200);
			PayPlayerInArea(i, -1776.3850,1320.5878, -1695.2391,1350.1283, 100);
			PayPlayerInArea(i, -2067.5, -878.0, -2035.0, -842.0, 150);
		}
	}
	return 1;
}
public PropertyScoreUpdate()
{
	new keydata[255];
	new TotalEarning;
	for(new i=0; i < MAX_PLAYERS; i++) {
	    if(loginflag[i] == 1) {
	        TotalEarning =0;
	        for(new j=0; j < MAX_POINTS; j++) {
	            if(!strlen(propertyOwner[j])) {
	            } else if(!udb_Exists(propertyOwner[j])) {
					propertyOwner[j] = "";
					format(keydata,sizeof(keydata),"%d",j);
					dini_Unset("p.cfg",keydata);
	            } else if(strcmp(loginid[i], propertyOwner[j], true) == 0) {
	                TotalEarning+=propertyValues[j]/40;
	            }
			}
			if(TotalEarning != 0) {
				new string[256];
				GiveMoney(i, TotalEarning);
				format(string, sizeof(string), " + 당신의 건물에서 $%d 의 수익금을 얻었습니다.", TotalEarning);
				SendClientMessage(i, COLOR_GREEN, string);
			}
		}
	}
	return 1;
}
/*
public PropertyScoreUpdate()
{
	new owners[MAX_POINTS][256];
	new payments[MAX_POINTS];

	for(new i=0; i < MAX_POINTS; i++)
	    owners[i]="";

	for(new i=0; i < MAX_POINTS; i++)
	{
		if(strlen(propertyOwner[i])) {
			for(new j=0; j < MAX_POINTS; j++) {
			    if(!strlen(owners[j])) {
			    	propertyEarnings[i] = propertyValues[i]/40;
					owners[j]=propertyOwner[i];
					payments[j]+=propertyEarnings[i];
			        j = MAX_POINTS;
				} else if(strcmp(owners[j], propertyOwner[i], true) == 0) {
			    	propertyEarnings[i] = propertyValues[i]/40;
			        payments[j]+=propertyEarnings[i];
			        j = MAX_POINTS;
				}
			}
		}
	}
new realowner;
	for(new i=0; i < MAX_POINTS; i++) {
		if( strlen(propertyOwner[i])) {
		    realowner = -1;
			for(new j=0; j < MAX_PLAYERS; j++) {
				if(loginflag[j] == 1) {
					if(strcmp(owners[i], loginid[j], true) == 0) {
					    realowner = i;
					    break;
					}
				}
			}
			if(realowner != -1) {
			    if(payments[i] >=1){
					new string[256];
					GiveMoney(realowner, payments[i]);
					format(string, sizeof(string), " + 당신의 건물에서 $%d 의 수익금을 얻었습니다.", payments[i]);
					SendClientMessage(realowner, COLOR_GREEN, string);
				}
			}
		}
	}
}
*/
SendPlayerFormattedTextg(playerid, const str[], define)
{
	new tmpbuf[256];
	format(tmpbuf, sizeof(tmpbuf), str, define);
	SendClientMessage(playerid, 0x00DD00FF, tmpbuf);
}
SendPlayerFormattedTextb(playerid, const str[], define)
{
	new tmpbuf[256];
	format(tmpbuf, sizeof(tmpbuf), str, define);
	SendClientMessage(playerid, 0x6CAFF9FF, tmpbuf);
}
SendPlayerFormattedTextr(playerid, const str[], define)
{
	new tmpbuf[256];
	format(tmpbuf, sizeof(tmpbuf), str, define);
	SendClientMessage(playerid, 0xDD0000FF, tmpbuf);
}

public checkpointUpdate()
{
	new size=15;
	new micon;
	new pcheck;
	new sdata;
	new nice;
	new Float:px,Float:py,Float:pz;
	for(new i=0; i<MAX_PLAYERS; i++) {
	    if(IsPlayerConnected(i)) {
		    GetPlayerPos(i,px,py,pz);
			pcheck = playerCheckpoint[i];
			if(pcheck != 999) {
				if( pcheck == CP_STUNT1S || pcheck == CP_STUNT2S) size = 3;
				else if( pcheck == CP_STUNT1E ) size = 5;
				else if( pcheck == CP_JAILFINES ) size = 2;
				if( px >= checkpoints[pcheck][0]-size && px <= checkpoints[pcheck][0]+size && py >= checkpoints[pcheck][1]-size && py <= checkpoints[pcheck][1]+size && pz >= checkpoints[pcheck][2]-10 && pz <= checkpoints[pcheck][2]+10) {

				} else {
				    if(gara[i] == 0) {
					DisablePlayerCheckpoint(i);
					DisablePlayerRaceCheckpoint(i);
					}
					RemovePlayerMapIcon(i,1);
					RemovePlayerMapIcon(i,2);
					RemovePlayerMapIcon(i,3);
					playerCheckpoint[i] = 999;
				}
			}
			nice = 0;
			if(event_flag == 1 && event_value == 1) {
			if(player_cp[i] == 17) {
			SetPlayerRaceCheckpoint(i,4, rock_cp[player_cp[i]][0], rock_cp[player_cp[i]][1], rock_cp[player_cp[i]][2],rock_cp[player_cp[i]][0], rock_cp[player_cp[i]][1], rock_cp[player_cp[i]][2], 10);
			} else {
			SetPlayerRaceCheckpoint(i,0, rock_cp[player_cp[i]][0], rock_cp[player_cp[i]][1], rock_cp[player_cp[i]][2],rock_cp[player_cp[i]+1][0], rock_cp[player_cp[i]+1][1], rock_cp[player_cp[i]+1][2], 10);
			}
			} else if(event_flag == 1 && event_value == 2) {
			if(race_lap[i] == RACE_LAP && player_cp[i] == 17) {
			SetPlayerRaceCheckpoint(i,4, race_cp[player_cp[i]][0], race_cp[player_cp[i]][1], race_cp[player_cp[i]][2],race_cp[player_cp[i]][0], race_cp[player_cp[i]][1], race_cp[player_cp[i]][2], 10);
			} else {
			SetPlayerRaceCheckpoint(i,0, race_cp[player_cp[i]][0], race_cp[player_cp[i]][1], race_cp[player_cp[i]][2],race_cp[player_cp[i]+1][0], race_cp[player_cp[i]+1][1], race_cp[player_cp[i]+1][2], 10);
			}
			}
		    for(new j=0; j < MAX_POINTS; j++) {
		        if( j == CP_ADMINDOOR1 || j == CP_ADMINDOOR2) {
					sdata = 8;
				} else {
					sdata = 20;
				}
				if( px >= checkpoints[j][0]-sdata && px <= checkpoints[j][0]+sdata && py >= checkpoints[j][1]-sdata && py <= checkpoints[j][1]+sdata && pz >= checkpoints[j][2]-10 && pz <= checkpoints[j][2]+10) {
						if( j == CP_STUNT1S || j == CP_STUNT2S) size = 3;
						else if( j == CP_STUNT1E ) size = 5;
						else if( j == CP_JAILFINES ) size = 2;
						else size = 2;
					 	if(playerCheckpoint[i] == j) {
						    nice = 1;
						} else {

//							if(px >= checkpoints[j][0]-size && px <= checkpoints[j][0]+size && py >= checkpoints[j][1]-size && py <= checkpoints[j][1]+size && pz >= checkpoints[j][2]-10 && pz <= checkpoints[j][2]+10) {
								playerCheckpoint[i] = j;
//							}
							if(gara[i] == 0) {
							    if(nice == 0) {
								    nice = 1;
									SetPlayerCheckpoint(i, checkpoints[j][0],checkpoints[j][1],checkpoints[j][2], size);
								} else {
									SetPlayerRaceCheckpoint(i, 0, checkpoints[j][0],checkpoints[j][1],checkpoints[j][2], checkpoints[j][0],checkpoints[j][1],checkpoints[j][2], size);
								}
							}
							micon = 37;
							if(PropertyCheckpointSelection(checkpointType[j]) == P_BANK || PropertyCheckpointSelection(checkpointType[j]) == P_BANK2 || PropertyCheckpointSelection(checkpointType[j]) == P_BANK3 || PropertyCheckpointSelection(checkpointType[j]) == P_BANK4 || checkpointType[j] == CP_BANK5 || checkpointType[j] == CP_BANK6) micon = 52;
							if(checkpointType[j] == CP_DILLGAS) micon = 2;
							if(checkpointType[j] == CP_MONTGAS) micon = 2;
							if(checkpointType[j] == CP_WHETGAS) micon = 2;
							if(checkpointType[j] == CP_ANGELGAS) micon = 2;
							if(checkpointType[j] == CP_SFGAS) micon = 2;
							if(checkpointType[j] == CP_ELQUEGAS) micon = 2;
							if(checkpointType[j] == CP_BONEGAS) micon = 2;
							if(checkpointType[j] == CP_LVGAS) micon = 2;
							if(checkpointType[j] == CP_GASEMER) micon = 2;
							if(checkpointType[j] == CP_GASBEHCAM) micon = 2;
							if(checkpointType[j] == CP_GASBESCAM) micon = 2;
							if(checkpointType[j] == CP_GASSPINY) micon = 2;
							if(checkpointType[j] == CP_GASTIERRA) micon = 2;
							if(checkpointType[j] == CP_GASJUNIP) micon = 2;
							if(checkpointType[j] == CP_GASIDLE) micon = 2;
							if(checkpointType[j] == CP_GASFLINT) micon = 2;
							if(checkpointType[j] == CP_AIRMUS) micon = 5;
							if(checkpointType[j] == CP_AIRCONTR) micon = 5;
							if(checkpointType[j] == CP_MONTGAS) micon = 5;
							if(checkpointType[j] == CP_REVIN) micon = 34;
							if(!strlen(propertyOwner[j]) && micon == 37) {
							    micon = 31;
							} else if(strlen(propertyOwner[j]) && micon == 37) {
								micon = 32;
							}
							if(propertyValues[j] == 0) {
							} else {
								if(gara[i] == 0) {
								SetPlayerMapIcon(i,1,checkpoints[j][0],checkpoints[j][1],checkpoints[j][2],micon,0xAAFFAAFF);
								}
							}
						}
				}
			}
/*			if(nice == 0) {
				if(gara[i] == 0) {
				DisablePlayerCheckpoint(i);
				DisablePlayerRaceCheckpoint(i);
				}
				RemovePlayerMapIcon(i,1);
				RemovePlayerMapIcon(i,2);
				RemovePlayerMapIcon(i,3);
				playerCheckpoint[i] = 999;
			} */
			new Float:x;
			new Float:y;
			new Float:z;
			GetVehiclePos(279,x,y,z);
			SetPlayerMapIcon(i,2,x,y,z,29,0xAAFFAAFF);
			if(cadmin == 1) {
				for(new a=0; a<MAX_PLAYERS; a++) {
					if(IsPlayerConnected(a)) {
					    if(ismadmin(a)) {
						    GetPlayerPos(a,x,y,z);
						    if(gara[i] == 0) {
						    SetPlayerMapIcon(i,1,x,y,z,61,0xAAFFAAFF);
						    }
					    }
					}
				}
			}
		}
	}
}
