							MoveObject(objects[0],doors[0][0],doors[0][1],doors[0][2],MOVE_SPEED);
							MoveObject(objects[1],doors[1][0],doors[1][1],doors[1][2],MOVE_SPEED);
							doors_open[0] = -2;
							doors_open[1] = -2;
						} else { // Closed, open them
							SendClientMessage(playerid, COLOR_YELLOW," + 문이 열립니다.()");
							MoveObject(objects[0],doors[0][0]+5.0,doors[0][1],doors[0][2],MOVE_SPEED);
							MoveObject(objects[1],doors[1][0]-5.0,doors[1][1],doors[1][2],MOVE_SPEED);
							doors_open[0] = -1;
							doors_open[1] = -1;
						}
					} else {
						SendClientMessage(playerid, COLOR_YELLOW," + 문이 고장난거 같아여..");
					}
		        } else {
					PlayerPlaySound(playerid,1056,X,Y,Z);
					SendClientMessage(playerid, COLOR_YELLOW," + 문이 열리지 않습니다.");
		        }
