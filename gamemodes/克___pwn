if(strcmp(cmd,"/fish",true)==0)
    {
        if(IsPlayerConnected(playerid))
	    {
   			if(GetPlayerLevel(playerid) == 2) {
	        SendClientMessage(playerid, COLOR_RED, " + 레벨 2 이상만 할 수 있습니다 당신은 현재 낚시를 할수없으므로 알바로 레벨 2를 만드세요");
			return 1;
			}
	        if(Fishes[playerid][pWeight1] > 0 && Fishes[playerid][pWeight2] > 0 && Fishes[playerid][pWeight3] > 0 && Fishes[playerid][pWeight4] > 0 && Fishes[playerid][pWeight5] > 0)
	        {
	            SendClientMessage(playerid, COLOR_GREY, " + 바구니가 꽉찼습니다. 물고기는 요리를 하거나, 편의점에서 팔거나, 물에다가 놓아줄 수 있습니다.");
	            return 1;
	        }
        	if(!IsAtFishPlace(playerid))
	        {
         		SendClientMessage(playerid, COLOR_GREY, " + 낚시가 허용 된 구역에서만 할 수 있습니다.");
	            return 1;
	        }
	        if(FishTime[playerid] >= 2) {
       			SendClientMessage(playerid, COLOR_GREY, " + 당신은 이미 낚시를 하고 계십니다.");
	            return 1;
	        }
   		    if(quest[playerid] == 3) {
			SendClientMessage(playerid,COLOR_CRIMSON," ");
			SendClientMessage(playerid,COLOR_CRIMSON," + 퀘스트를 완료했습니다. 다음 퀘스트가 기다리고 있으니   를 쳐보세요.");
			SendClientMessage(playerid,COLOR_CRIMSON," + 보상: $5000 EXP: 200");
			GiveMoney(playerid, 5000);
			exp[playerid] += 200;
			quest[playerid] ++;
			}
			FishTime[playerid] = random(15)+10;
			SendClientMessage(playerid, COLOR_GREY, " + 낚시대를 던졌습니다. 잠시만 기다리시면 물고기가 올라옵니다.");
	    }
	    return 1;
