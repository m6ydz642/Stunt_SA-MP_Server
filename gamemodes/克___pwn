					format(str,sizeof(str)," + %s(%d) - 모드개발자",playernamea,i);
	            	SendClientMessage(playerid,COLOR_YELLOW,str);
                } else if(ismadmin(i)) {
                	okay = 1;
					new str[255];
					new playernamea[MAX_PLAYER_NAME];
					GetPlayerName(i,playernamea, MAX_PLAYER_NAME);
					format(str,sizeof(str)," + %s(%d) - 운영진",playernamea,i);
	            	SendClientMessage(playerid,COLOR_YELLOW,str);
            	} else if(issubadmin(i)) {
                	okay = 1;
					new str[255];
					new playernamea[MAX_PLAYER_NAME];
					GetPlayerName(i, playernamea, MAX_PLAYER_NAME);
					format(str,sizeof(str)," + %s(%d) - 부운영진",playernamea,i);
	            	SendClientMessage(playerid,COLOR_YELLOW,str);
            	}
            }
		}
		if(okay == 0) SendClientMessage(playerid,COLOR_YELLOW," + 현재 접속중인 어드민이 없습니다.");
		SendClientMessage(playerid,COLOR_YELLOW," + ---------------------------------------------- ");
		return 1;
