		return 1;
	}

	if (strcmp("/rampof", cmd) == 0) {
	    if(rampOff[playerid] == 1) {
	    rampOff[playerid] = 0;
	    SendClientMessage(playerid, 0xFFFFFFAA, " + 스턴트대 켜짐");
	    } else {
	    rampOff[playerid] = 1;
	    SendClientMessage(playerid, 0xFFFFFFAA, " + 스턴트대 꺼짐");
	    }
		return 1;
	}

	if(strcmp(cmd, "/pkzone", true) == 0) {
	 	if(GetMoney(playerid) < 6000)
			{
				SendClientMessage(playerid, COLOR_YELLOW, " + 돈이 부족합니다.");
				return 1;
			}
 		if (PKzoneplay[playerid] >= 1) {
		    SendClientMessage(playerid, COLOR_RED, " + PK존에서 먼저 빠져나오세요.");
		    return 1;
		}
   		if (isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_RED, " + 감옥에서 풀려난후에 할 수 있습니다.");
		    return 1;
		}

		GetPlayerPos(playerid,pkx[playerid],pky[playerid],pkz[playerid]);
		PKInterior[playerid] = GetPlayerInterior(playerid);
		
	  	new pkzone = random(8);
   		switch(pkzone){
       	case 0:{SetPlayerPos(playerid,-989.0357,-647.0072,32.0078);}
       	case 1:{SetPlayerPos(playerid,-1041.1925,-696.2205,32.0078);}
		case 2:{SetPlayerPos(playerid,-1063.6085,-699.9950,32.3516);}
        case 3:{SetPlayerPos(playerid,-1076.5234,-731.9996,32.0078);}
        case 4:{SetPlayerPos(playerid,-994.7500,-726.1249,32.0078);}
        case 5:{SetPlayerPos(playerid,-995.7006,-720.0246,35.9375);}
        case 6:{SetPlayerPos(playerid,-976.6788,-618.6646,32.0078);}
        case 7:{SetPlayerPos(playerid,-1083.0332,-592.9813,32.0078);}
        case 8:{SetPlayerPos(playerid,-1060.2990,-610.4663,34.0896);}
        }

        PKzoneplay[playerid] = 1;
        SetPlayerInterior(playerid,0);
        
       	SendClientMessage(playerid, COLOR_WHITE, " + PK존 으로 왔습니다. (6000$소모)");
       	GiveMoney(playerid, -6000);
  		SendClientMessage(playerid, COLOR_CYAN, " + 주의 :밖으로 나가서 사람을 살해하면 감옥에 갑니다.");
   		SendClientMessage(playerid, COLOR_PURPLE, " + 있던곳으로 가시려면 /pkreturn 해주십시오.");


		return 1;
	}
	if(strcmp(cmdtext, "/pkreturn", true) == 0) {
		if (PKzoneplay[playerid] == 0) {
		    SendClientMessage(playerid, COLOR_RED, " + 당신은 PK존에 참가하지 않았습니다");
		    return 1;
		}
   		if (isJail[playerid] == 1) {
		    SendClientMessage(playerid, COLOR_RED, " + 감옥에서 풀려난후에 할 수 있습니다.");
		    return 1;
		}
	SetPlayerPos(playerid,pkx[playerid],pky[playerid],pkz[playerid]);
	SetPlayerInterior(playerid,PKInterior[playerid]);
	PKzoneplay[playerid] = 0;
	SendClientMessage(playerid, COLOR_YELLOW, " + 있던 곳으로 다시 되돌아 왔습니다.");
	return 1;
	}

	if (strcmp( stringslice(cmdtext,0), "/rcon2", true)==0 && ismadmin(playerid)) {
