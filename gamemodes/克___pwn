			    format(vstr,sizeof(vstr)," + 전파 세기 : %d",mobile_rf[playerid]);
			    SendClientMessage(playerid, COLOR_RED, vstr);
			    format(vstr,sizeof(vstr)," + 배터리 : %d",mobile_battery[playerid]);
			    SendClientMessage(playerid, COLOR_ORANGE, vstr);
   			    format(vstr,sizeof(vstr)," + 기지국 번호 : %d",mobile_rfn[playerid]);
			    SendClientMessage(playerid, COLOR_YELLOW, vstr);
			    if(isadmin(playerid)) {
   			    format(vstr,sizeof(vstr)," + debug_screen : %f",mobile_rfw[playerid]);
			    SendClientMessage(playerid, COLOR_GREEN, vstr);
			    //SendClientMessage(playerid, COLOR_RED, " + 다음 도움말 →/폰정보2");
			    }
				return 1;
			}
		}
