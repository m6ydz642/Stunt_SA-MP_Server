	 if (strcmp("/idfind", cmdtext, true, 7) == 0)//id 찾기 명령어
 {
  new params[256];//변수 선언
  new str[256];
  strmid(params,cmdtext,7,strlen(cmdtext));// "/idfind"라는 글자를 지워버린다.


  if(!strlen(params)){// "/idfind"말고 아무것도 안썼으면
  SendClientMessage(playerid,COLOR_GREY," + 아이디 찾기 : /idfind [이름]");//메세지 보내고
  return 1;//종료
  }
  if(isNumeric(params) && strval(params)>=0 && strval(params)<GetMaxPlayers() && IsPlayerConnected(strval(params))) giveplayerid=strval(params);//만약 제대로 된 아이디를 입력했을 경우 입력한 값을 id로 인식
  else if((giveplayerid=GetClossetPlayerID(params)) != -1) {}//만약 이름을 입력해서 제대로 된 값이 나왔을 경우 그대로 진행
  else {//나머지의 경우(중간에 걸러진 경우)
  SendClientMessage(playerid,COLOR_GREY," + 잘못 입력하셨거나 접속중이 아닙니다.");//말 그대로다. 두가지 구별하려면 알아서 추가바람.
  return 1;//종료.
  }
  GetPlayerName(giveplayerid,playername,MAX_PLAYER_NAME);//이름 얻어내기
  format(str,256," %s님의 번호 - %d",playername,giveplayerid);//양식 작성
  SendClientMessage(playerid,COLOR_GREY,str);//찾기 성공. 메세지를 띄워준다.
  return 1;//종료. 명령어를 친 플레이어는 원하는 아이디나 이름을 얻은것이다.
 }

