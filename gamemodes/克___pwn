//------------------------------------------------------------------------------------------------------

logingo(floginid[256],playerid,floginpw[256],wit) {
	SetMoney(playerid,0);
	new mname[255];
	for(new y=0; y<MAX_PLAYERS; y++) {
		if(IsPlayerConnected(y) && playerid != y && loginflag[y] == 1) {
			if(strcmp(loginid[y], floginid, true) == 0) {
				format(mname,sizeof(mname)," + %d님은 중복접속으로 인해 킥당하셨습니다.",y);
			    Kick(y);
			}
		}
	}
	loginid[playerid] = floginid;
	loginpw[playerid] = floginpw;
	new tmpd;
	new mmpd;

	tmpd = udb_UserInt(loginid[playerid],"pd");
	if(tmpd != 0) {
	    new keydata[255];
	    for(new y=1; y<tmpd; y++) {
	        format(keydata,sizeof(keydata),"pd_%d",y);
	        mmpd = udb_UserInt(loginid[playerid],keydata);
	        if(mmpd != 0) {
				format(strings, sizeof(strings)," + 당신의 건물 %s (이)가 팔렸습니다. (팔린 가격: %d)", propertyNames[mmpd],propertyValues[mmpd]);
				SendClientMessage(playerid,IVORY,strings);
				GiveMoney(playerid,propertyValues[mmpd]);
			}
	        udb_UserSetInt(loginid[playerid],keydata,0);
	    }
	    udb_UserSetInt(loginid[playerid],"pd",0);
	}
/*
	for(new i=1; i<9; i++){
		new mnum;
		format(strings, sizeof(strings), "property%d", i);
		mnum = udb_UserInt(loginid[playerid],strings);
		if (propertyOwner[mnum] == 999) {
			propertyOwner[mnum] = playerid;
		} else {
			if(mnum != 0){
				format(strings, sizeof(strings)," + 당신의 건물 %s (이)가 팔렸습니다. (팔린 가격: %d)", propertyNames[mnum],propertyValues[mnum]);
				SendClientMessage(playerid,IVORY,strings);
				GiveMoney(playerid, propertyValues[mnum]);
				format(strings, sizeof(strings), "property%d", i);
				udb_UserSetInt(loginid[playerid],strings,0);
				proget[playerid] -= 1;
				udb_UserSetInt(loginid[playerid],"proget",proget[playerid]);
			}
		}
	}
	*/
	
	BankPass[playerid] = udb_UserInt(loginid[playerid],"BankPass");
	SpawnSet[playerid] = udb_UserInt(loginid[playerid],"SpawnSet");
	
	PlayerInfo[playerid][pBoxSkill] = udb_UserInt(loginid[playerid],"pBoxSkill");
	PlayerInfo[playerid][pWins] = udb_UserInt(loginid[playerid],"pWins");
	PlayerInfo[playerid][pLoses] = udb_UserInt(loginid[playerid],"pLoses");
	
	format(Fishes[playerid][pFish1],20,"%s",udb_User(loginid[playerid],"fish1"));
    format(Fishes[playerid][pFish2],20,"%s",udb_User(loginid[playerid],"fish2"));
    format(Fishes[playerid][pFish3],20,"%s",udb_User(loginid[playerid],"fish3"));
    format(Fishes[playerid][pFish4],20,"%s",udb_User(loginid[playerid],"fish4"));
    format(Fishes[playerid][pFish5],20,"%s",udb_User(loginid[playerid],"fish5"));


	exp[playerid] = udb_UserInt(loginid[playerid],"exp");
	quest[playerid] = udb_UserInt(loginid[playerid],"quest");
	Groceries[playerid][pChickens] = udb_UserInt(loginid[playerid],"pChickens");
 	Groceries[playerid][pChicken] = udb_UserInt(loginid[playerid],"pChicken");
	Groceries[playerid][pHamburgers] = udb_UserInt(loginid[playerid],"pHamburgers");
	Groceries[playerid][pHamburger] = udb_UserInt(loginid[playerid],"pHamburger");
	Groceries[playerid][pPizzas] = udb_UserInt(loginid[playerid],"pPizzas");
	Groceries[playerid][pPizza] = udb_UserInt(loginid[playerid],"pPizza");


	if(udb_UserInt(loginid[playerid],"fishWeight5") != 0) {Fishes[playerid][pWeight5] = udb_UserInt(loginid[playerid],"fishWeight5");}
	if(udb_UserInt(loginid[playerid],"fishWeight4") != 0) {Fishes[playerid][pWeight4] = udb_UserInt(loginid[playerid],"fishWeight4");}
	if(udb_UserInt(loginid[playerid],"fishWeight3") != 0) {Fishes[playerid][pWeight3] = udb_UserInt(loginid[playerid],"fishWeight3");}
	if(udb_UserInt(loginid[playerid],"fishWeight2") != 0) {Fishes[playerid][pWeight2] = udb_UserInt(loginid[playerid],"fishWeight2");}
	if(udb_UserInt(loginid[playerid],"fishWeight1") != 0) {Fishes[playerid][pWeight1] = udb_UserInt(loginid[playerid],"fishWeight1");}

	if(udb_UserInt(loginid[playerid],"fishFid5") != 0) {Fishes[playerid][pFid5] = udb_UserInt(loginid[playerid],"fishFid5");}
	if(udb_UserInt(loginid[playerid],"fishFid4") != 0) {Fishes[playerid][pFid4] = udb_UserInt(loginid[playerid],"fishFid4");}
	if(udb_UserInt(loginid[playerid],"fishFid3") != 0) {Fishes[playerid][pFid3] = udb_UserInt(loginid[playerid],"fishFid3");}
	if(udb_UserInt(loginid[playerid],"fishFid2") != 0) {Fishes[playerid][pFid2] = udb_UserInt(loginid[playerid],"fishFid2");}
	if(udb_UserInt(loginid[playerid],"fishFid1") != 0) {Fishes[playerid][pFid1] = udb_UserInt(loginid[playerid],"fishFid1");}

    format(Cooking[playerid][pCook5],20,"%s",udb_User(loginid[playerid],"cook5"));
    format(Cooking[playerid][pCook4],20,"%s",udb_User(loginid[playerid],"cook4"));
    format(Cooking[playerid][pCook3],20,"%s",udb_User(loginid[playerid],"cook3"));
    format(Cooking[playerid][pCook2],20,"%s",udb_User(loginid[playerid],"cook2"));
    format(Cooking[playerid][pCook1],20,"%s",udb_User(loginid[playerid],"cook1"));


	Cooking[playerid][pCWeight5] = udb_UserInt(loginid[playerid],"cookWeight5");
	Cooking[playerid][pCWeight4] = udb_UserInt(loginid[playerid],"cookWeight4");
	Cooking[playerid][pCWeight3] = udb_UserInt(loginid[playerid],"cookWeight3");
	Cooking[playerid][pCWeight2] = udb_UserInt(loginid[playerid],"cookWeight2");
    Cooking[playerid][pCWeight1] = udb_UserInt(loginid[playerid],"cookWeight1");

	Cooking[playerid][pCookID5] = udb_UserInt(loginid[playerid],"cookID5");
	Cooking[playerid][pCookID4] = udb_UserInt(loginid[playerid],"cookID4");
	Cooking[playerid][pCookID3] = udb_UserInt(loginid[playerid],"cookID3");
	Cooking[playerid][pCookID2] = udb_UserInt(loginid[playerid],"cookID2");
    Cooking[playerid][pCookID1] = udb_UserInt(loginid[playerid],"cookID1");
	MyJob[playerid] = udb_UserInt(loginid[playerid],"myjob");
	PlayerInfo[playerid][pCookSkill] = udb_UserInt(loginid[playerid],"pCookSkill");
    PlayerInfo[playerid][pBiggestFish] = udb_UserInt(loginid[playerid],"pBiggestFish");
    PlayerInfo[playerid][pFishSkill] = udb_UserInt(loginid[playerid],"pFishSkill");
	new colorchanging=udb_UserInt(loginid[playerid],"color");
	if(colorchanging == 1)SetPlayerColor(playerid, 0xA587DE0BA354);
	if(colorchanging == 2)SetPlayerColor(playerid, 0x9CDE7180);
	if(colorchanging == 3)SetPlayerColor(playerid, 0xFF99AADD);
	if(colorchanging == 4)SetPlayerColor(playerid, 0xA7105DEF);
