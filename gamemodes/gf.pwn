				format(string, sizeof(string), " 경찰 본부: %s에 대한 정보가없습니다, 정말로 저게 옳은 이름 입니까? 모른다면 '아니오'로 대답해 주십시오.",tmp);
				SendClientMessage(playerid, COLOR_DBLUE, string);
				return 0;
			}
		}
		if(Mobile[playerid] == 912)
		{
			if(!strlen(tmp))
			{
				SendClientMessage(playerid, COLOR_ALLDEPT, "911센터: 실례지만 다시 한번 말씀해 주시겠습니까?");
				return 0;
			}
			strmid(PlayerCrime[playerid][pAccusing], text, 0, strlen(text), 255);
			SendClientMessage(playerid, COLOR_DBLUE, "경찰 본부: 만약 가해자의 이름을 알거나 부분적으로 안다면 지금 말하시거나 모른다면 '아니오' 라고 말하세요.");
			Mobile[playerid] = 913;
			return 0;
		}
		if(Mobile[playerid] == 911)
		{
			if(!strlen(tmp))
			{
				SendClientMessage(playerid, COLOR_ALLDEPT, "911센터: 죄송합니다만 이해가 안되는군요, '경찰'이나 '응급의료원'이 필요하십니까? 둘중 하나를 말씀해주세요.");
				return 0;
			}
			else if (c_strcmp("경찰", tmp))
			{
				SendClientMessage(playerid, COLOR_ALLDEPT, "911센터: 경찰 본부로 연결해 드리겠습니다, 기다리세요.");
				Mobile[playerid] = 912;
				SendClientMessage(playerid, COLOR_DBLUE, "경찰 본부: 사건의 상황을 짧게 이야기 해주십시오.");
				return 0;
			}
			else if (c_strcmp("응급의료원", tmp))
			{
				SendClientMessage(playerid, COLOR_ALLDEPT, "911센터: 응급의료센터 본부로 연결해 드리겠습니다, 기다리세요.");
				Mobile[playerid] = 914;
				SendClientMessage(playerid, TEAM_CYAN_COLOR, "응급실: 사건의 상황을 짧게 이야기 해주십시오.");
				return 0;
			}
			else
			{
				SendClientMessage(playerid, COLOR_ALLDEPT, "911센터: 죄송합니다만 이해가 안되는군요, 경찰이나 응급의료원이 필요하십니까? 둘중 하나를 말씀해주세요.");
				return 0;
			}
		}
		if(IsPlayerConnected(Mobile[playerid]))
		{
		    if(Mobile[Mobile[playerid]] == playerid)
		    {
				SendClientMessage(Mobile[playerid], COLOR_YELLOW,string);
			}
		}
		else
		{
			SendClientMessage(playerid, COLOR_YELLOW,"거기에는 아무도 없다.");
		}
		return 0;
	}
	if (realchat)
	{
	    if(gPlayerLogged[playerid] == 0)
	    {
	        return 0;
      	}
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "%s 말함: %s", sendername, text);
		ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
		return 0;
	}
	return 1;
}

